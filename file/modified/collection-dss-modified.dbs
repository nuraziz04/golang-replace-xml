      <query id="listExpiredSttu" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>select companyCode, branchCode, sttuNo, hiePositionVersion from (&#xD;&#x9;select &#xD;&#x9;&#x9;a.company_code companyCode,&#xD;&#x9;&#x9;a.branch_code branchCode,&#xD;&#x9;&#x9;a.sttu_no sttuNo,&#xD;&#x9;&#x9;c.hie_position_version hiePositionVersion&#xD;&#x9;from trx_sttu a &#xD;&#x9;join&#xD;&#x9;&#x9;( &#xD;&#x9;&#x9;&#x9;select distinct&#xD;&#x9;&#x9;&#x9;&#x9;company_code companyCode,&#xD;&#x9;&#x9;&#x9;&#x9;trx_code trxCode,&#xD;&#x9;&#x9;&#x9;&#x9;max(trx_seq) over(partition by company_code,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;trx_code) trxMax&#xD;&#x9;&#x9;&#x9;from trx_approval&#xD;&#x9;&#x9;&#x9;where menu_code = &#39;CLT115&#39;&#xD;&#x9;&#x9;) b&#xD;&#x9;&#x9;on a.company_code&#x9;= b.companyCode &#xD;&#x9;&#x9;and a.sttu_no&#x9;&#x9;= b.trxCode&#xD;&#x9;join trx_approval c&#xD;&#x9;on c.company_code = b.companyCode and c.trx_code = b.trxCode and c.trx_seq = b.trxMax and c.decision_sts = &#39;APPR&#39;&#xD;&#x9;and a.company_code = c.company_code and a.sttu_no = c.trx_code&#xD;&#x9;where a.expired_date &lt;= DATEADD(day, 1, :eodDate) and a.sttu_sts in (&#39;APPR&#39;)&#xD;) a where companyCode = :companyCode and branchCode = :branchCode order by branchCode</sql>
