      <query id="getCountMonitorPersediaan" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>select COUNT(0) total from (&#xD;&#x9;select&#xD;&#x9;&#x9;dep.company_code companyCode,&#xD;&#x9;&#x9;dep.branch_code branchCode,&#xD;&#x9;&#x9;br.branch_name branchName,&#xD;&#x9;&#x9;dep.loc_code locCode,&#xD;&#x9;&#x9;loc.branch_name locName,&#xD;&#x9;&#x9;dep.aggr_no aggrNo,&#xD;&#x9;&#x9;agg.customer_code customerCode,&#xD;&#x9;&#x9;agg.customer_fullname customerFullname,&#xD;&#x9;&#x9;dep.execution_date tanggalSerahTerima,&#xD;&#x9;&#x9;dep.tu_deposit_date tanggalValidasiTerima,&#xD;&#x9;&#x9;dep.expired_date as tanggalPersediaan,&#xD;&#x9;&#x9;dep.deposit_sts depositSts,&#xD;&#x9;&#x9;--ds.param_descrip statusPersediaan,&#xD;(SELECT ds.param_descrip from mst_global_param ds (nolock) where dep.company_code = ds.company_code and dep.deposit_sts = ds.param_code and ds.condition = &#39;DEPOSIT_STS&#39; and ds.flag_active = &#39;1&#39;) statusPersediaan,&#xD;&#x9;&#x9;ar.notes statusKontrak,&#xD;&#x9;&#x9;ar.trx_name subStatusKontrak,&#xD;&#x9;&#x9;(dep.tu_cost + dep.penalty_tu_value) as nilaiBiayaPenyelesaian,&#xD;&#x9;&#x9;dep.bal_principal_value nilaiAngsuranPokok,&#xD;&#x9;&#x9;(dep.bal_principal_value + dep.bal_int_value + dep.tu_cost + dep.penalty_tu_value) as nilaiSisaAr,&#xD;&#x9;&#x9;--ct.param_descrip categoryName,&#xD;(select ct.param_descrip from mst_global_param ct (NOLOCK) where dep.company_code = ct.company_code and brd.category_code = ct.param_code and ct.condition = &#39;CATEGORY_CODE&#39; and ct.flag_active = &#39;1&#39;) categoryName,&#xD;&#x9;&#x9;ass.brand_code brandCode,&#xD;&#x9;&#x9;brd.brand_name brandName,&#xD;&#x9;&#x9;ass.type_code typeCode,&#xD;&#x9;&#x9;tp.type_name typeName,&#xD;&#x9;&#x9;ass.model_code modelCode,&#xD;&#x9;&#x9;md.model_name modelName,&#xD;&#x9;&#x9;tp.kind_code kindCode,&#xD;&#x9;&#x9;kn.kind_name kindName,&#xD;&#x9;&#x9;cl.color_name colorName,&#xD;&#x9;&#x9;ass.year_mfg yearMfg,&#xD;&#x9;&#x9;ass.nopol nopol,&#xD;&#x9;&#x9;ass.chassis_no chassisNo,&#xD;&#x9;&#x9;ass.machine_no machineNo,&#xD;&#x9;&#x9;ass.bpkb_name stnkName,&#xD;&#x9;&#x9;dep.km km,&#xD;&#x9;&#x9;dep.stnk_expired_date stnkExpiredDate,&#xD;&#x9;&#x9;bpk.bpkb_location bpkbLocation,&#xD;&#x9;&#x9;bpk.bpkb_location_name bpkbLocationCode,&#xD;&#x9;&#x9;bpkl.branch_name bpkbLocationName,&#xD;&#x9;&#x9;pt1.fullname petugasTarik1,&#xD;&#x9;&#x9;pt2.fullname petugasTarik2,&#xD;&#x9;&#x9;dep.notes_pic_receive2 keterangan,&#xD;&#x9;&#x9;agg.wo_date woDate&#xD;&#x9;from trx_tu_deposit dep (NOLOCK)&#xD;&#x9;left join mst_branch br (NOLOCK)&#xD;&#x9;on dep.company_code = br.company_code and dep.branch_code = br.branch_code&#xD;&#x9;left join mst_branch loc (NOLOCK)&#xD;&#x9;on dep.company_code = loc.company_code and dep.loc_code = loc.branch_code&#xD;&#x9;left join trx_aggr agg (NOLOCK)&#xD;&#x9;on dep.company_code = agg.company_code and dep.aggr_no = agg.aggr_no&#xD;&#x9;--left join mst_global_param ds&#xD;&#x9;--on dep.company_code = ds.company_code and dep.deposit_sts = ds.param_code and ds.condition = &#39;DEPOSIT_STS&#39; and ds.flag_active = &#39;1&#39;&#xD;&#x9;left join mst_ar_sts ar (NOLOCK)&#xD;&#x9;on agg.ar_sts = ar.ar_sts and isnull(agg.sub_sts, &#39;&#39;) = isnull(ar.sub_sts, &#39;&#39;)&#xD;&#x9;left join trx_aggr_asset ass (NOLOCK)&#xD;&#x9;on dep.company_code = ass.company_code and agg.aggr_no = ass.aggr_no&#xD;&#x9;left join mst_brand brd (NOLOCK)&#xD;&#x9;on dep.company_code = brd.company_code and ass.brand_code = brd.brand_code&#xD;&#x9;--left join mst_global_param ct&#xD;&#x9;--on dep.company_code = ct.company_code and brd.category_code = ct.param_code and ct.condition = &#39;CATEGORY_CODE&#39; and ct.flag_active = &#39;1&#39;&#xD;&#x9;left join mst_type tp (NOLOCK)&#xD;&#x9;on dep.company_code = tp.company_code and ass.type_code = tp.type_code&#xD;&#x9;left join mst_kind kn (NOLOCK)&#xD;&#x9;on dep.company_code = kn.company_code and tp.kind_code = kn.kind_code&#xD;&#x9;left join mst_model md (NOLOCK)&#xD;&#x9;on dep.company_code = md.company_code and ass.model_code = md.model_code&#xD;&#x9;left join mst_color cl (NOLOCK)&#xD;&#x9;on dep.company_code = cl.company_code and ass.color_code = cl.color_code&#xD;&#x9;left join mst_bpkb bpk (NOLOCK)&#xD;&#x9;on dep.aggr_no = bpk.aggr_no&#xD;&#x9;left join mst_branch bpkl (NOLOCK)&#xD;&#x9;on bpk.bpkb_location_name = bpkl.branch_code&#xD;&#x9;left join trx_sttu stt (NOLOCK)&#xD;&#x9;on stt.sttu_no = dep.sttu_no&#xD;&#x9;left join mst_employee pt1 (NOLOCK)&#xD;&#x9;on stt.pic1_code = pt1.nik&#xD;&#x9;left join mst_employee pt2 (NOLOCK)&#xD;&#x9;on stt.pic2_code = pt2.nik&#xD;&#x9;where dep.deposit_sts in (&#39;1&#39;,&#39;3&#39;,&#39;4&#39;) and agg.ar_sts != &#39;LN&#39;&#xD;&#x9;&#xD;&#xD;&#x9;union all&#xD;&#xD;&#x9;select&#xD;&#x9;&#x9;inv.company_code companyCode,&#xD;&#x9;&#x9;inv.branch_code branchCode,&#xD;&#x9;&#x9;br.branch_name branchName,&#xD;&#x9;&#x9;inv.loc_code loceCode,&#xD;&#x9;&#x9;loc.branch_name locName,&#xD;&#x9;&#x9;inv.aggr_no aggrNo,&#xD;&#x9;&#x9;agg.customer_code customerCode,&#xD;&#x9;&#x9;agg.customer_fullname customerFullname,&#xD;&#x9;&#x9;dep.execution_date tanggalSerahTerima,&#xD;&#x9;&#x9;dep.tu_deposit_date tanggalValidasiTerima,&#xD;&#x9;&#x9;inv.enter_date tanggalPersediaan,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when inv.inventory_sts = &#39;1&#39; then &#39;2&#39;&#xD;&#x9;&#x9;end depositSts,&#xD;&#x9;&#x9;--ds.param_descrip statusPersediaan,&#xD;(select ds.param_descrip from mst_global_param ds (nolock) where inv.company_code = ds.company_code and inv.inventory_sts = ds.param_code and ds.condition = &#39;INVENTORY_STS&#39; and ds.flag_active = &#39;1&#39;) statusPersediaan,&#xD;&#x9;&#x9;ar.notes statusKontrak,&#xD;&#x9;&#x9;ar.trx_name subStatusKontrak,&#xD;&#x9;&#x9;(inv.tu_cost + inv.penalty_tu_value) as nilaiBiayaPenyelesaian,&#xD;&#x9;&#x9;inv.bal_principal_value nilaiAngsuranPokok,&#xD;&#x9;&#x9;(inv.bal_principal_value + inv.bal_int_value + inv.tu_cost + inv.penalty_tu_value) as nilaiSisaAr,&#xD;&#x9;&#x9;--ct.param_descrip categoryName,&#xD;(select ct.param_descrip from mst_global_param ct (nolock) where inv.company_code = ct.company_code and brd.category_code = ct.param_code and ct.condition = &#39;CATEGORY_CODE&#39; and ct.flag_active = &#39;1&#39;) categoryName,&#xD;&#x9;&#x9;ass.brand_code brandCode,&#xD;&#x9;&#x9;brd.brand_name brandName,&#xD;&#x9;&#x9;ass.type_code typeCode,&#xD;&#x9;&#x9;tp.type_name typeName,&#xD;&#x9;&#x9;ass.model_code modelCode,&#xD;&#x9;&#x9;md.model_name modelName,&#xD;&#x9;&#x9;tp.kind_code kindCode,&#xD;&#x9;&#x9;kn.kind_name kindName,&#xD;&#x9;&#x9;cl.color_name colorName,&#xD;&#x9;&#x9;ass.year_mfg yearMfg,&#xD;&#x9;&#x9;ass.nopol nopol,&#xD;&#x9;&#x9;ass.chassis_no chassisNo,&#xD;&#x9;&#x9;ass.machine_no machineNo,&#xD;&#x9;&#x9;ass.bpkb_name stnkName,&#xD;&#x9;&#x9;dep.km km,&#xD;&#x9;&#x9;dep.stnk_expired_date stnkExpiredDate,&#xD;&#x9;&#x9;bpk.bpkb_location bpkbLocation,&#xD;&#x9;&#x9;bpk.bpkb_location_name bpkbLocationCode,&#xD;&#x9;&#x9;bpkl.branch_name bpkbLocationName,&#xD;&#x9;&#x9;pt1.fullname petugasTarik1,&#xD;&#x9;&#x9;pt2.fullname petugasTarik2,&#xD;&#x9;&#x9;dep.notes_pic_receive2 keterangan,&#xD;&#x9;&#x9;agg.wo_date woDate&#xD;&#x9;from trx_inventory_tu inv (NOLOCK)&#xD;&#x9;left join mst_branch br (NOLOCK)&#xD;&#x9;on inv.company_code = br.company_code and inv.branch_code = br.branch_code&#xD;&#x9;left join mst_branch loc (NOLOCK)&#xD;&#x9;on inv.company_code = br.company_code and inv.loc_code = loc.branch_code&#xD;&#x9;left join trx_aggr agg (NOLOCK)&#xD;&#x9;on inv.company_code = agg.company_code and inv.aggr_no = agg.aggr_no&#xD;&#x9;left join trx_tu_deposit dep (NOLOCK)&#xD;&#x9;on inv.company_code = dep.company_code and inv.tu_deposit_no = dep.tu_deposit_no and inv.aggr_no = dep.aggr_no&#xD;&#x9;--left join mst_global_param ds&#xD;&#x9;--on inv.company_code = ds.company_code and inv.inventory_sts = ds.param_code and ds.condition = &#39;INVENTORY_STS&#39; and ds.flag_active = &#39;1&#39;&#xD;&#x9;left join mst_ar_sts ar (NOLOCK)&#xD;&#x9;on agg.ar_sts = ar.ar_sts and isnull(agg.sub_sts, &#39;&#39;) = isnull(ar.sub_sts, &#39;&#39;)&#xD;&#x9;left join trx_aggr_asset ass (NOLOCK)&#xD;&#x9;on inv.company_code = ass.company_code and agg.aggr_no = ass.aggr_no&#xD;&#x9;left join mst_brand brd (NOLOCK)&#xD;&#x9;on inv.company_code = brd.company_code and ass.brand_code = brd.brand_code&#xD;&#x9;--left join mst_global_param ct&#xD;&#x9;--on inv.company_code = ct.company_code and brd.category_code = ct.param_code and ct.condition = &#39;CATEGORY_CODE&#39; and ct.flag_active = &#39;1&#39;&#xD;&#x9;left join mst_type tp (NOLOCK)&#xD;&#x9;on inv.company_code = tp.company_code and ass.type_code = tp.type_code&#xD;&#x9;left join mst_kind kn (NOLOCK)&#xD;&#x9;on inv.company_code = kn.company_code and tp.kind_code = kn.kind_code&#xD;&#x9;left join mst_model md (NOLOCK)&#xD;&#x9;on inv.company_code = md.company_code and ass.model_code = md.model_code&#xD;&#x9;left join mst_color cl (NOLOCK)&#xD;&#x9;on inv.company_code = cl.company_code and ass.color_code = cl.color_code&#xD;&#x9;left join mst_bpkb bpk (NOLOCK)&#xD;&#x9;on inv.aggr_no = bpk.aggr_no&#xD;&#x9;left join mst_branch bpkl (NOLOCK)&#xD;&#x9;on bpk.bpkb_location_name = bpkl.branch_code&#xD;&#x9;left join trx_sttu stt (NOLOCK)&#xD;&#x9;on stt.sttu_no = dep.sttu_no&#xD;&#x9;left join mst_employee pt1 (NOLOCK)&#xD;&#x9;on stt.pic1_code = pt1.nik&#xD;&#x9;left join mst_employee pt2 (NOLOCK)&#xD;&#x9;on stt.pic2_code = pt2.nik&#xD;&#x9;where inv.inventory_sts in (&#39;1&#39;) and agg.ar_sts != &#39;LN&#39;&#xD;) a WHERE :search :filter</sql>
         <result element="count" rowName="" useColumnNumbers="false">
            <element column="total" name="total" xsdType="string"></element>
         </result>
         <param name="search" sqlType="QUERY_STRING"></param>
