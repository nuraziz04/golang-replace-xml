      <query id="getCountLovNomorKontrak" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>DECLARE @DKH_DATE  VARCHAR(50) = :dkhDate&#xD;&#xD;SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT companyCode, branchCode, productCode, collCode, aggrNo, aggrNoFil, customerFullname, customerCode, applNo, woDate, tuType, groupCode, arSts, isnull(subSts, &#39;&#39;) subSts, spNo, spType, spTypeName, sttuNo, collAddressType,&#xD;&#x9;address, hpNo1, phoneArea1, phoneNo1, phoneArea2, phoneNo2, rt, rw, villageName, subdistrictName, cityName, provinceName, zipCode, isnull(dayOverdue, 0) dayOverdue, instFrom, instTo, tagihanAngsuran,&#xD;&#x9;tagihanDenda, (biaya + mstCollfee) biayaTagih, (tagihanAngsuran + tagihanDenda + biaya + mstCollfee) totalTagihan, sisaUtj, angsuranBelumJatuhTempo, dueDate, angsuran, bucketCode, bucketName, collName, collDueDate, tenor, teamNo, dsoCode, mstCollfee as collFeeRate FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNoFil,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;&#x9;A.APPL_NO applNo,&#xD;&#x9;&#x9;&#x9;A.WO_DATE woDate,&#xD;&#x9;&#x9;&#x9;J.TU_TYPE tuType,&#xD;&#x9;&#x9;&#x9;A.GROUP_CODE groupCode,&#xD;&#x9;&#x9;&#x9;A.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;A.SUB_STS subSts,&#xD;&#x9;&#x9;&#x9;G.SP_NO spNo,&#xD;&#x9;&#x9;&#x9;G.SP_TYPE spType,&#xD;&#x9;&#x9;&#x9;H.PARAM_DESCRIP spTypeName,&#xD;&#x9;&#x9;&#x9;I.STTU_NO sttuNo,&#xD;&#x9;&#x9;&#x9;B.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as phoneArea1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as phoneNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as rt,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RW &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as rw,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as villageName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as subdistrictName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COVC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as cityName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as provinceName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as zipCode,&#x9;&#xD;&#x9;&#x9;&#x9;(&#x9;&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dueDate FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT DUE_DATE dueDate, MIN(COLL_DUE_DATE) collDueDate FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; GROUP BY DUE_DATE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dueDate,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as instFrom,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;&#x9;(&#xD;&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID)), 0)&#xD;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;INST_NO &gt;= (SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) a) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;AND&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;INST_NO &lt;= (SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END RESULT)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#xD;&#x9;&#x9;&#x9;) as tagihanAngsuran,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SUM( isnull((PINALTY_VALUE), 0)- isnull((PINALTY_PAID), 0) ) total&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;) as tagihanDenda,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SUM( isnull((COLL_FEE), 0) - isnull((COLL_FEE_PAID), 0) )&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;total&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;) as biaya,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select collFee from (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;select top 1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;coll_fee collFee, &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;EFFECTIVE_DATE effectiveDate&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;from mst_coll_fee(nolock)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;where company_code = A.COMPANY_CODE AND product_code = A.PRODUCT_CODE AND coll_kind_fee = &#39;ADMCOL&#39; AND EFFECTIVE_DATE &lt;= GETDATE() AND (category_code is null or category_code = &#39;&#39;) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as mstCollfee,&#xD;&#x9;&#x9;&#x9;isnull((K.BEGIN_BAL - USAGE_VALUE - BOOK_VALUE + PLUS_VALUE),0) as sisaUtj,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PRINCIPAL_VALUE+INT_VALUE-PRINCIPAL_PAID-INT_PAID),0) angsuranBelumJatuhTempo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND INST_NO &gt; (SELECT MIN(INST_NO) FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PRINCIPAL_VALUE+INT_VALUE-PRINCIPAL_PAID-INT_PAID),0) angsuranBelumJatuhTempo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND INST_NO &gt; (SELECT MAX(INST_NO) instNo FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) &#xD;&#x9;&#x9;&#x9;&#x9;END RESULT &#xD;&#x9;&#x9;&#x9;) angsuranBelumJatuhTempo,&#xD;&#x9;&#x9;&#x9;A.INST_VALUE angsuran,&#xD;&#x9;&#x9;&#x9;A.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;&#x9;L.DESCRIPTION bucketName,&#xD;&#x9;&#x9;&#x9;M.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select top 1 coll_due_date from trx_aggr_schedule(nolock) where aggr_no = A.AGGR_NO and payment_sts = &#39;UNPAID&#39;&#xD;&#x9;&#x9;&#x9;) as collDueDate,&#xD;&#x9;&#x9;&#x9;A.TENOR tenor,&#xD;&#x9;&#x9;&#x9;O.TEAM_NO teamNo,&#xD;&#x9;&#x9;&#x9;P.DSO_CODE dsoCode&#xD;&#x9;&#x9;FROM TRX_AGGR A (nolock)&#xD;&#x9;&#x9;JOIN TRX_APPL B (nolock)&#xD;&#x9;&#x9;ON A.APPL_NO = B.APPL_NO AND A.COMPANY_CODE=B.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN (SELECT *&#xD;&#x9;&#x9;&#x9;FROM (SELECT *, ROW_NUMBER() OVER(PARTITION BY COMPANY_CODE,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;AGGR_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER BY AGGR_NO, CREATED_DATE DESC) RN&#xD;&#x9;&#x9;&#x9;FROM TRX_SP(nolock)&#xD;&#x9;&#x9;&#x9;WHERE EXPIRED_DATE &gt;= &#39;2021-05-03&#39; AND SP_STS = &#39;PROC&#39; AND COMPANY_CODE=&#39;EDV&#39;&#xD;&#x9;&#x9;&#x9;) X&#xD;&#x9;&#x9;&#x9;WHERE RN = 1) G&#xD;&#x9;&#x9;ON A.[COMPANY_CODE]&#x9;&#x9;&#x9;= G.[COMPANY_CODE] &#xD;&#x9;&#x9;AND A.[AGGR_NO]&#x9;&#x9;&#x9;&#x9;= G.[AGGR_NO] &#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM H (nolock)&#xD;&#x9;&#x9;ON G.SP_TYPE = H.PARAM_CODE AND H.CONDITION = &#39;SP_TYPE&#39; AND H.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;LEFT JOIN TRX_STTU I (nolock)&#xD;&#x9;&#x9;ON A.AGGR_NO = I.AGGR_NO AND I.EXPIRED_DATE &gt; @DKH_DATE AND I.STTU_STS = &#39;PROC&#39; AND I.COMPANY_CODE=&#39;EDV&#39;&#xD;&#x9;&#x9;LEFT JOIN TRX_TAGIH_ULANG J (nolock)&#xD;&#x9;&#x9;ON A.AGGR_NO = J.AGGR_NO AND J.TU_TYPE = &#39;002&#39; AND J.COMPANY_CODE=&#39;EDV&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_UTJ K (nolock)&#xD;&#x9;&#x9;ON A.AGGR_NO = K.AGGR_NO AND A.COMPANY_CODE=K.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_BUCKET L (nolock)&#xD;&#x9;&#x9;ON A.BUCKET_CODE = L.BUCKET_CODE AND A.PRODUCT_CODE = L.PRODUCT_CODE AND A.BUCKET_TYPE = L.BUCKET_TYPE AND A.COMPANY_CODE=L.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE M (nolock)&#xD;&#x9;&#x9;ON A.COLL_CODE = M.NIK&#xD;&#x9;&#x9;LEFT JOIN MST_GROUP_HDR N (nolock)&#xD;&#x9;&#x9;ON A.GROUP_CODE = N.GROUP_CODE AND A.COMPANY_CODE = N.COMPANY_CODE AND A.BRANCH_CODE = N.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_GROUP_DTL O (nolock)&#xD;&#x9;&#x9;ON N.GROUP_CODE = O.GROUP_CODE AND N.COMPANY_CODE = O.COMPANY_CODE AND N.BRANCH_CODE = O.BRANCH_CODE AND B.APPL_NO = O.APPL_NO&#xD;&#x9;&#x9;LEFT JOIN getViewBranch() P&#xD;&#x9;&#x9;ON A.COMPANY_CODE = P.COMPANY_CODE AND A.BRANCH_CODE = P.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = CS.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE EG (nolock)&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = EG.CUSTOMER_CODE AND EG.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;&#x9;ON EG.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB (nolock)&#xD;&#x9;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY BC (nolock)&#xD;&#x9;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE BP (nolock)&#xD;&#x9;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE COV (nolock)&#xD;&#x9;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT COSUB (nolock)&#xD;&#x9;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY COVC (nolock)&#xD;&#x9;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE COP (nolock)&#xD;&#x9;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE&#xD;&#x9;&#x9;WHERE (A.WO_DATE IS NULL OR A.WO_DATE = &#39;1900-01-01&#39;)&#xD;&#x9;) a &#xD;) a WHERE :search :filter and instFrom is not null</sql>
         <result element="count" rowName="" useColumnNumbers="false">
            <element column="total" name="total" xsdType="string"></element>
         </result>
         <param name="search" sqlType="QUERY_STRING"></param>
         <param name="filter" sqlType="QUERY_STRING"></param>
         <param name="dkhDate" sqlType="STRING"></param>
      </query>
      <query id="getDataLovNomorKonrak" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>DECLARE @DKH_DATE  VARCHAR(50) = :dkhDate&#xD;&#xD;SELECT companyCode, branchCode, productCode, collCode, aggrNo, aggrNoFil, customerFullname, customerCode, applNo, woDate, tuType, groupCode, arSts, subSts, spNo, spType, spTypeName, sttuNo, collAddressType,&#xD;address, hpNo1, phoneArea1, phoneNo1, phoneArea2, phoneNo2, rt, rw, villageName, subdistrictName, cityName, provinceName, zipCode, isnull(dayOverdue, 0) dayOverdue, instFrom, instTo, tagihanAngsuran,&#xD;tagihanDenda, biayaTagih, totalTagihan, sisaUtj, angsuranBelumJatuhTempo, dueDate, angsuran, bucketCode, bucketName, collName, collDueDate, tenor, teamNo, dsoCode, collFeeRate FROM (&#xD;&#x9;SELECT companyCode, branchCode, productCode, collCode, aggrNo, aggrNoFil, customerFullname, customerCode, applNo, woDate, tuType, groupCode, arSts, isnull(subSts, &#39;&#39;) subSts, spNo, spType, spTypeName, sttuNo, collAddressType,&#xD;&#x9;address, hpNo1, phoneArea1, phoneNo1, phoneArea2, phoneNo2, rt, rw, villageName, subdistrictName, cityName, provinceName, zipCode, isnull(dayOverdue, 0) dayOverdue, instFrom, instTo, tagihanAngsuran,&#xD;&#x9;tagihanDenda, (biaya + mstCollfee) biayaTagih, (tagihanAngsuran + tagihanDenda + biaya + mstCollfee) totalTagihan, sisaUtj, angsuranBelumJatuhTempo, dueDate, angsuran, bucketCode, bucketName, collName, collDueDate, tenor, teamNo, dsoCode, mstCollfee as collFeeRate FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNoFil,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;&#x9;A.APPL_NO applNo,&#xD;&#x9;&#x9;&#x9;A.WO_DATE woDate,&#xD;&#x9;&#x9;&#x9;J.TU_TYPE tuType,&#xD;&#x9;&#x9;&#x9;A.GROUP_CODE groupCode,&#xD;&#x9;&#x9;&#x9;A.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;A.SUB_STS subSts,&#xD;&#x9;&#x9;&#x9;G.SP_NO spNo,&#xD;&#x9;&#x9;&#x9;G.SP_TYPE spType,&#xD;&#x9;&#x9;&#x9;H.PARAM_DESCRIP spTypeName,&#xD;&#x9;&#x9;&#x9;I.STTU_NO sttuNo,&#xD;&#x9;&#x9;&#x9;B.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; OR A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as phoneArea1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&