      <query id="getColumnMonitorPersediaan" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT 1 as branchName, 1 locName, 1 aggrNo, 1 brandName, 1 typeName, 1 modelName, 1 kindName, 1 nopol, 1 tanggalValidasiTerima, 1 nilaiAngsuranPokok, 1 nilaiBiayaPenyelesaian, 1 nilaiSisaAr,&#xD;&#x9;1 statusKontrak, 1 subStatusKontrak, 1 statusPersediaan, 1 keterangan, 1 woDate, 1 colorName</sql>
         <result element="Entries" rowName="" useColumnNumbers="false">
            <element column="branchName" name="branchName" xsdType="string"></element>
            <element column="locName" name="locName" xsdType="string"></element>
            <element column="aggrNo" name="aggrNo" xsdType="string"></element>
