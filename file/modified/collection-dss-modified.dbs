&#x9;&#x9;ON A.APPL_NO = B.APPL_NO AND A.COMPANY_CODE=B.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN (SELECT *&#xD;&#x9;&#x9;&#x9;FROM (SELECT *, ROW_NUMBER() OVER(PARTITION BY COMPANY_CODE,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;AGGR_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER BY AGGR_NO, CREATED_DATE DESC) RN&#xD;&#x9;&#x9;&#x9;FROM TRX_SP(nolock)&#xD;&#x9;&#x9;&#x9;WHERE EXPIRED_DATE &gt;= &#39;2021-05-03&#39; AND SP_STS = &#39;PROC&#39; AND COMPANY_CODE=&#39;EDV&#39;&#xD;&#x9;&#x9;&#x9;) X&#xD;&#x9;&#x9;&#x9;WHERE RN = 1) G&#xD;&#x9;&#x9;ON A.[COMPANY_CODE]&#x9;&#x9;&#x9;= G.[COMPANY_CODE] &#xD;&#x9;&#x9;AND A.[AGGR_NO]&#x9;&#x9;&#x9;&#x9;= G.[AGGR_NO] &#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM H (nolock)&#xD;&#x9;&#x9;ON G.SP_TYPE = H.PARAM_CODE AND H.CONDITION = &#39;SP_TYPE&#39; AND H.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;LEFT JOIN TRX_STTU I (nolock)&#xD;&#x9;&#x9;ON A.AGGR_NO = I.AGGR_NO AND I.EXPIRED_DATE &gt; @DKH_DATE AND I.STTU_STS = &#39;PROC&#39; AND I.COMPANY_CODE=&#39;EDV&#39;&#xD;&#x9;&#x9;LEFT JOIN TRX_TAGIH_ULANG J (nolock)&#xD;&#x9;&#x9;ON A.AGGR_NO = J.AGGR_NO AND J.TU_TYPE = &#39;002&#39; AND J.COMPANY_CODE=&#39;EDV&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_UTJ K (nolock)&#xD;&#x9;&#x9;ON A.AGGR_NO = K.AGGR_NO AND A.COMPANY_CODE=K.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_BUCKET L (nolock)&#xD;&#x9;&#x9;ON A.BUCKET_CODE = L.BUCKET_CODE AND A.PRODUCT_CODE = L.PRODUCT_CODE AND A.BUCKET_TYPE = L.BUCKET_TYPE AND A.COMPANY_CODE=L.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE M (nolock)&#xD;&#x9;&#x9;ON A.COLL_CODE = M.NIK&#xD;&#x9;&#x9;LEFT JOIN MST_GROUP_HDR N (nolock)&#xD;&#x9;&#x9;ON A.GROUP_CODE = N.GROUP_CODE AND A.COMPANY_CODE = N.COMPANY_CODE AND A.BRANCH_CODE = N.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_GROUP_DTL O (nolock)&#xD;&#x9;&#x9;ON N.GROUP_CODE = O.GROUP_CODE AND N.COMPANY_CODE = O.COMPANY_CODE AND N.BRANCH_CODE = O.BRANCH_CODE AND B.APPL_NO = O.APPL_NO&#xD;&#x9;&#x9;LEFT JOIN getViewBranch() P&#xD;&#x9;&#x9;ON A.COMPANY_CODE = P.COMPANY_CODE AND A.BRANCH_CODE = P.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = CS.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE EG (nolock)&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = EG.CUSTOMER_CODE AND EG.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;&#x9;ON EG.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB (nolock)&#xD;&#x9;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY BC (nolock)&#xD;&#x9;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE BP (nolock)&#xD;&#x9;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE COV (nolock)&#xD;&#x9;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT COSUB (nolock)&#xD;&#x9;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY COVC (nolock)&#xD;&#x9;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE COP (nolock)&#xD;&#x9;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE&#xD;&#x9;&#x9;WHERE (A.WO_DATE IS NULL OR A.WO_DATE = &#39;1900-01-01&#39;)&#xD;&#x9;) a &#xD;) a WHERE :search :filter and instFrom is not null ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
