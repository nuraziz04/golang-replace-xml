      <query id="getCountTrxTuMutasi" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;B.BRANCH_NAME branchName,&#xD;&#x9;&#x9;A.TRX_NO trxNo,&#xD;&#x9;&#x9;A.TRX_STS trxSts,&#xD;&#x9;&#x9;--C.PARAM_DESCRIP trxStsName,&#xD;(select C.PARAM_DESCRIP from mst_global_param C (NOLOCK) where A.TRX_STS = C.PARAM_CODE AND C.CONDITION = &#39;MUTASI_STS&#39; AND C.FLAG_ACTIVE = &#39;1&#39;) trxStsName,&#xD;&#x9;&#x9;A.TRX_DATE trxDate,&#xD;&#x9;&#x9;A.BRANCH_FROM branchFrom,&#xD;&#x9;&#x9;BF.BRANCH_NAME branchFromName,&#xD;&#x9;&#x9;A.BRANCH_TO branchTo,&#xD;&#x9;&#x9;BT.BRANCH_NAME branchToName,&#xD;&#x9;&#x9;A.NOTES notes,&#xD;&#x9;&#x9;A.PIC_CODE picCode,&#xD;&#x9;&#x9;D.FULLNAME picName,&#xD;&#x9;&#x9;A.PIC_POSITION picPosition,&#xD;&#x9;&#x9;E.POSITION_NAME picPositionName,&#xD;&#x9;&#x9;A.RECEIVED_DATE receivedDate,&#xD;&#x9;&#x9;A.RECEIVED_BY receivedBy,&#xD;&#x9;&#x9;F.FULLNAME receivedByName,&#xD;&#x9;&#x9;A.RECEIVED_POSITION receivedPosition,&#xD;&#x9;&#x9;G.POSITION_NAME receivedPositionName&#xD;&#x9;FROM TRX_TU_MUTASI_HDR A (NOLOCK)&#xD;&#x9;LEFT JOIN MST_BRANCH B (NOLOCK)&#xD;&#x9;ON A.BRANCH_CODE = B.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH BT (NOLOCK)&#xD;&#x9;ON A.BRANCH_TO = BT.BRANCH_CODE&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM C &#xD;&#x9;--ON A.TRX_STS = C.PARAM_CODE AND C.CONDITION = &#39;MUTASI_STS&#39; AND C.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_EMPLOYEE D (NOLOCK)&#xD;&#x9;ON A.PIC_CODE = D.NIK&#xD;&#x9;LEFT JOIN MST_POSITION E (NOLOCK)&#xD;&#x9;ON A.PIC_POSITION = E.POSITION_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE F (NOLOCK)&#xD;&#x9;ON A.RECEIVED_BY = F.NIK&#xD;&#x9;LEFT JOIN MST_POSITION G (NOLOCK)&#xD;&#x9;ON A.RECEIVED_POSITION = G.POSITION_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH BF (NOLOCK)&#xD;&#x9;ON A.BRANCH_FROM = BF.BRANCH_CODE&#xD;) a WHERE :search :filter</sql>
