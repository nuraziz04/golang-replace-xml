      <query id="getCountLovNomorKontrakSpUkm" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;&#x9;SELECT DISTINCT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;A.APPL_NO applNo,&#xD;&#x9;&#x9;A.WO_DATE woDate,&#xD;&#x9;&#x9;A.TENOR tenor,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as instFrom,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;(&#xD;&#xD;&#x9;&#x9;&#x9;SELECT SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID))&#xD;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND &#xD;&#x9;&#x9;&#x9;&#x9;INST_NO &gt;= (SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;) a) &#xD;&#x9;&#x9;&#x9;&#x9;AND&#xD;&#x9;&#x9;&#x9;&#x9;INST_NO &lt;= (SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END RESULT)&#xD;&#x9;&#x9;&#x9;&#x9;AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#xD;&#x9;&#x9;) as tagihanAngsuran,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RT &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RT&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rt,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RW &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RW&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rw,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as villageName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as subdistrictName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COVC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as cityName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as provinceName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as zipCode,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1 dueDate FROM (&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;DUE_DATE dueDate&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;GROUP BY DUE_DATE&#xD;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as dueDate,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(ISNULL(PINALTY_VALUE, 0) - ISNULL(PINALTY_PAID, 0)) as penaltyFee&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as penaltyFee,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(ISNULL(COLL_FEE, 0) - ISNULL(COLL_FEE_PAID, 0)) as collFee&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as collFee,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(ISNULL(PRINCIPAL_PAID, 0) - ISNULL(INT_PAID, 0)) as installmentPaid&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as installmentPaid,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;ISNULL(BEGIN_BAL,0) - ISNULL(USAGE_VALUE,0) - ISNULL(BOOK_VALUE,0) + ISNULL(PLUS_VALUE,0)&#xD;&#x9;&#x9;&#x9;FROM MST_UTJ (nolock) WHERE COMPANY_CODE = A.COMPANY_CODE AND AGGR_NO = A.AGGR_NO&#xD;&#x9;&#x9;) as utjBalance,&#xD;&#x9;&#x9;A.AR_STS arSts,&#xD;&#x9;&#x9;isnull(A.SUB_STS, &#39;&#39;) subSts,&#xD;&#x9;&#x9;A.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;D.FULLNAME collName,&#xD;&#x9;&#x9;E.DSO_CODE dsoCode&#xD;&#x9;FROM TRX_AGGR A (nolock)&#xD;&#x9;LEFT JOIN TRX_AGGR_SCHEDULE B (nolock)&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN MST_EMPLOYEE D (nolock)&#xD;&#x9;ON A.COLL_CODE = D.NIK&#xD;&#x9;LEFT JOIN MST_BRANCH E (nolock)&#xD;&#x9;ON A.COMPANY_CODE = E.COMPANY_CODE AND A.BRANCH_CODE = E.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = CS.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE G (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = G.CUSTOMER_CODE AND G.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;ON G.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB (nolock)&#xD;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY BC (nolock)&#xD;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE BP (nolock)&#xD;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (nolock)&#xD;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE COV (nolock)&#xD;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT COSUB (nolock)&#xD;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY COVC (nolock)&#xD;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE COP (nolock)&#xD;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE&#xD;&#x9;WHERE B.PAYMENT_STS = &#39;UNPAID&#39; AND B.COLL_DUE_DATE &lt;= GETDATE() AND A.PRODUCT_CODE != &#39;PC02&#39; AND NOT EXISTS (SELECT 1 FROM TRX_SP (nolock) WHERE EXPIRED_DATE &gt; GETDATE() AND SP_STS != &#39;CANC&#39; and AGGR_NO=A.AGGR_NO)&#xD;) a WHERE (ISNULL(woDate, &#39;&#39;) = &#39;&#39; OR woDate = &#39;1900-01-01&#39;) AND dayOverdue &gt; 1 AND :search :filter</sql>
         <result element="count" rowName="" useColumnNumbers="false">
            <element column="total" name="total" xsdType="string"></element>
         </result>
         <param name="search" sqlType="QUERY_STRING"></param>
         <param name="filter" sqlType="QUERY_STRING"></param>
      </query>
      <query id="getDataLovNomorKontrakSpUkm" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT DISTINCT companyCode, branchCode, productCode, collCode, aggrNo, customerFullname, customerCode, applNo, woDate, dayOverdue, instFrom, instTo, tagihanAngsuran, address, rt, rw, villageName, &#xD;subdistrictName, cityName, provinceName, zipCode, tenor, dueDate, arSts, subSts, bucketCode, bucketType, collName, dsoCode, penaltyFee, collFee, installmentPaid, isnull(utjBalance, 0) as utjBalance FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;A.APPL_NO applNo,&#xD;&#x9;&#x9;A.WO_DATE woDate,&#xD;&#x9;&#x9;A.TENOR tenor,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as instFrom,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;(&#xD;&#xD;&#x9;&#x9;&#x9;SELECT SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID))&#xD;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND &#xD;&#x9;&#x9;&#x9;&#x9;INST_NO &gt;= (SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;) a) &#xD;&#x9;&#x9;&#x9;&#x9;AND&#xD;&#x9;&#x9;&#x9;&#x9;INST_NO &lt;= (SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END RESULT)&#xD;&#x9;&#x9;&#x9;&#x9;AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#xD;&#x9;&#x9;) as tagihanAngsuran,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RT &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RT&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rt,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RW &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RW&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rw,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as villageName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as subdistrictName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COVC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as cityName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as provinceName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as zipCode,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1 dueDate FROM (&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;DUE_DATE dueDate&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;GROUP BY DUE_DATE&#xD;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as dueDate,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(ISNULL(PINALTY_VALUE, 0) - ISNULL(PINALTY_PAID, 0)) as penaltyFee&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as penaltyFee,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(ISNULL(COLL_FEE, 0) - ISNULL(COLL_FEE_PAID, 0)) as collFee&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as collFee,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(ISNULL(PRINCIPAL_PAID, 0) - ISNULL(INT_PAID, 0)) as installmentPaid&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as installmentPaid,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;ISNULL(BEGIN_BAL,0) - ISNULL(USAGE_VALUE,0) - ISNULL(BOOK_VALUE,0) + ISNULL(PLUS_VALUE,0)&#xD;&#x9;&#x9;&#x9;FROM MST_UTJ (nolock) WHERE COMPANY_CODE = A.COMPANY_CODE AND AGGR_NO = A.AGGR_NO&#xD;&#x9;&#x9;) as utjBalance,&#xD;&#x9;&#x9;A.AR_STS arSts,&#xD;&#x9;&#x9;isnull(A.SUB_STS, &#39;&#39;) subSts,&#xD;&#x9;&#x9;A.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;D.FULLNAME collName,&#xD;&#x9;&#x9;E.DSO_CODE dsoCode&#xD;&#x9;FROM TRX_AGGR A (nolock)&#xD;&#x9;LEFT JOIN TRX_AGGR_SCHEDULE B (nolock)&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN MST_EMPLOYEE D (nolock)&#xD;&#x9;ON A.COLL_CODE = D.NIK&#xD;&#x9;LEFT JOIN MST_BRANCH E (nolock)&#xD;&#x9;ON A.COMPANY_CODE = E.COMPANY_CODE AND A.BRANCH_CODE = E.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = CS.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE G (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = G.CUSTOMER_CODE AND G.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;ON G.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB (nolock)&#xD;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY BC (nolock)&#xD;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE BP (nolock)&#xD;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (nolock)&#xD;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE COV (nolock)&#xD;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT COSUB (nolock)&#xD;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY COVC (nolock)&#xD;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE COP (nolock)&#xD;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE&#xD;&#x9;WHERE B.PAYMENT_STS = &#39;UNPAID&#39; AND B.COLL_DUE_DATE &lt;= GETDATE() AND A.PRODUCT_CODE != &#39;PC02&#39; AND NOT EXISTS (SELECT 1 FROM TRX_SP (nolock) WHERE EXPIRED_DATE &gt; GETDATE() AND SP_STS != &#39;CANC&#39; and AGGR_NO=A.AGGR_NO)&#xD;) a WHERE (ISNULL(woDate, &#39;&#39;) = &#39;&#39; OR woDate = &#39;1900-01-01&#39;) AND dayOverdue &gt; 1 AND &#xD;:search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
