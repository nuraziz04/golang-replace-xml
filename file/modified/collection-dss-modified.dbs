      <query id="listMonitoringdebiturparamcode6" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @BRANCH_CODE varchar(10) = :branchCode&#xD;declare @VALUESFROM numeric(5,0) = :valuesFrom&#xD;declare @VALUESTO numeric(5,0) = :valuesTo&#xD;&#xD;SELECT companyCode, branchCode, aggrNo, customerFullname, dueDate, tenor, dayOverdue, instFrom, instTo, installBill, penaltyValue, collFee, totBill, villageName FROM (&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;C.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;A.DUE_DATE dueDate,&#xD;&#x9;&#x9;C.TENOR tenor,&#xD;&#x9;&#x9;A.DAY_OVERDUE dayOverdue,&#xD;&#x9;&#x9;A.INST_FROM instFrom,&#xD;&#x9;&#x9;A.INST_TO instTo,&#xD;&#x9;&#x9;A.INSTALL_BILL installBill,&#xD;&#x9;&#x9;A.PENALTY_VALUE penaltyValue,&#xD;&#x9;&#x9;A.COLL_FEE collFee,&#xD;&#x9;&#x9;A.TOT_BILL totBill,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN D.COLL_ADDRESS_TYPE = &#39;I&#39; THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Q.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM MST_CUSTOMER X &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN MST_VILLAGE Q&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON X.ID_VILLAGE_CODE = Q.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE X.CUSTOMER_CODE = C.CUSTOMER_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN D.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Q.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x