      <query id="getCountMonitorUtj" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;(SELECT PRODUCT_CODE FROM TRX_AGGR (NOLOCK) WHERE AGGR_NO=A.AGGR_NO_RECIPIENT AND COMPANY_CODE=A.COMPANY_CODE) productCode,&#xD;&#x9;&#x9;A.SEQ_NO seqNo,&#xD;&#x9;&#x9;A.REFF_DATE reffDate,&#xD;&#x9;&#x9;A.REFF_NO reffNo,&#xD;&#x9;&#x9;A.UTJ_TRANS_CODE utjTransCode,&#xD;&#x9;&#x9;--B.PARAM_DESCRIP utjTransName,&#xD;(SELECT B.PARAM_DESCRIP FROM MST_GLOBAL_PARAM B (nolock) WHERE A.COMPANY_CODE=B.COMPANY_CODE AND A.UTJ_TRANS_CODE = B.PARAM_CODE AND B.CONDITION = &#39;UTJ_TRANS_CODE&#39; AND B.FLAG_ACTIVE = &#39;1&#39;) utjTransName,&#xD;&#x9;&#x9;A.BEGINNING_VALUE beginningValue,&#xD;&#x9;&#x9;A.PLUS_VALUE plusValue,&#xD;&#x9;&#x9;A.BOOK_VALUE bookValue,&#xD;&#x9;&#x9;A.USAGE_VALUE usageValue,&#xD;&#x9;&#x9;A.BALANCE_VALUE balanceValue,&#xD;&#x9;&#x9;A.AGGR_NO_RECIPIENT noKontrakPenerimaUtj,&#xD;&#x9;&#x9;--C.CUSTOMER_CODE customerCode,&#xD;(SELECT C.CUSTOMER_CODE FROM TRX_AGGR C (NOLOCK) WHERE A.AGGR_NO = C.AGGR_NO AND A.COMPANY_CODE=C.COMPANY_CODE) customerCode,&#xD;(SELECT CUSTOMER_FULLNAME FROM TRX_AGGR (NOLOCK) WHERE AGGR_NO=A.AGGR_NO_RECIPIENT AND COMPANY_CODE=A.COMPANY_CODE) namaDebiturPenerimaUtj,&#xD;(SELECT GROUP_CODE FROM TRX_AGGR (NOLOCK) WHERE AGGR_NO=A.AGGR_NO_RECIPIENT AND COMPANY_CODE=A.COMPANY_CODE) groupCode,&#xD;(SELECT MG.GROUP_NAME FROM TRX_AGGR AG (NOLOCK)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;JOIN MST_GROUP_HDR MG (NOLOCK) ON MG.GROUP_CODE = AG.GROUP_CODE AND MG.COMPANY_CODE=AG.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE AG.AGGR_NO=A.AGGR_NO_RECIPIENT)&#xD;&#x9;&#x9; groupName,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;A.CREATED_DATE createdDate&#xD;&#x9;FROM TRX_UTJ A (NOLOCK)&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM B&#xD;&#x9;--ON A.UTJ_TRANS_CODE = B.PARAM_CODE AND B.CONDITION = &#39;UTJ_TRANS_CODE&#39; AND B.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;--LEFT JOIN TRX_AGGR C (NOLOCK)&#xD;&#x9;--ON A.AGGR_NO = C.AGGR_NO AND A.COMPANY_CODE=C.COMPANY_CODE AND A.BRANCH_CODE=C.BRANCH_CODE&#xD;&#x9;--LEFT JOIN MST_GROUP_HDR D&#xD;&#x9;--ON C.GROUP_CODE = D.GROUP_CODE&#xD;) a WHERE :search :filter</sql>
