      <query id="riwayatTransaksiTaskToDo" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT trxSeq, subject, nik, positionCode, positionName, branchCode, branchName, decisionSts, reasonLog, fullName, dicisionDate FROM (&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.TRX_SEQ trxSeq,&#xD;&#x9;&#x9;A.SUBJECT subject,&#xD;&#x9;&#x9;A.NIK nik,&#xD;&#x9;&#x9;A.POSITION_CODE positionCode,&#xD;&#x9;&#x9;B.POSITION_NAME positionName,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;C.BRANCH_NAME branchName,&#xD;&#x9;&#x9;A.DECISION_STS decisionSts,&#xD;                A.DECISION_DATE dicisionDate,&#xD;&#x9;&#x9;A.REASON_LOG reasonLog,&#xD;&#x9;&#x9;D.FULLNAME fullName&#xD;&#x9;FROM TRX_APPROVAL A&#xD;&#x9;LEFT JOIN MST_POSITION B&#xD;&#x9;ON A.POSITION_CODE = B.POSITION_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH C&#xD;&#x9;ON A.BRANCH_CODE = C.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE D&#xD;&#x9;ON A.NIK = D.NIK&#xD;&#x9;WHERE A.TRX_CODE = :trxCode AND A.COMPANY_CODE = :companyCode&#xD;) a ORDER BY trxSeq ASC</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="trxSeq" name="trxSeq" xsdType="string"></element>
            <element column="subject" name="subject" xsdType="string"></element>
            <element column="nik" name="nik" xsdType="string"></element>
            <element column="positionCode" name="positionCode" xsdType="string"></element>
            <element column="positionName" name="positionName" xsdType="string"></element>
            <element column="branchCode" name="branchCode" xsdType="string"></element>
            <element column="branchName" name="branchName" xsdType="string"></element>
            <element column="decisionSts" name="decisionSts" xsdType="string"></element>
            <element column="reasonLog" name="reasonLog" xsdType="string"></element>
            <element column="fullName" name="fullName" xsdType="string"></element>
            <element column="dicisionDate" name="dicisionDate" xsdType="string"></element>
         </result>
         <param name="trxCode" sqlType="STRING"></param>
         <param name="companyCode" sqlType="STRING"></param>
      </query>
      <query id="printDateTrxSp" returnUpdatedRowCount="true" useConfig="UI_SELECT">
         <sql>DECLARE @UPDATED_BY  VARCHAR(50) = :updatedBy&#xD;DECLARE @SP_NO  VARCHAR(50) = :spNo&#xD;DECLARE @PRINTED_DATE VARCHAR(30) = :printedDate&#xD;&#xD;IF(@PRINTED_DATE = &#39;1900-01-01&#39; OR @PRINTED_DATE = &#39;NULL&#39;)&#xD;BEGIN&#xD;&#x9;UPDATE TRX_SP SET&#xD;&#x9;&#x9;PRINTED_DATE = GETDATE(),&#xD;&#x9;&#x9;PRINTED_BY = @UPDATED_BY,&#xD;&#x9;&#x9;UPDATED_BY = @UPDATED_BY,&#xD;&#x9;&#x9;UPDATED_DATE = GETDATE()&#xD;&#x9;WHERE&#xD;&#x9;&#x9;SP_NO = @SP_NO&#xD;END&#xD;ELSE&#xD;BEGIN&#xD;&#x9;UPDATE TRX_SP SET&#xD;&#x9;&#x9;REPRINTED_COUNT = (SELECT ISNULL(MAX(REPRINTED_COUNT), 0) + 1 FROM TRX_SP WHERE SP_NO = @SP_NO),&#xD;&#x9;&#x9;UPDATED_BY = @UPDATED_BY,&#xD;&#x9;&#x9;UPDATED_DATE = GETDATE()&#xD;&#x9;WHERE&#xD;&#x9;&#x9;SP_NO = @SP_NO&#xD;END</sql>
         <result element="UpdatedRowCount" rowName="" useColumnNumbers="true">
            <element column="1" name="Value" xsdType="integer"></element>
         </result>
         <param name="updatedBy" sqlType="STRING"></param>
         <param name="spNo" sqlType="STRING"></param>
         <param name="printedDate" sqlType="STRING"></param>
      </query>
