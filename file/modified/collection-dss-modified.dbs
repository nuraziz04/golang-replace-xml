      <query id="getDataTrxSp" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT spReceiptType, spReceiptName, spNotes, companyCode, branchCode, spNo, spDate, aggrNo, customerCode, customerFullname, idAddress, idRt, idRw, zipCode, villageName, subdistrictName, cityName, provinceName, productName,&#xD;spType, spTypeName, spSolvingDay, expiredDate, collCode, collName, instFrom, instTo, osInstallment, penaltyFee, dueDate, dayOverdue, spSts, spStsName, printedDate, sentDate, returnedDate, canceledDate,&#xD;branchName, addressBranch, rtBranch, rwBranch, villageNameBranch, zipCodeBranch, subdistrictNameBranch, cityNameBranch, provinceNameBranch, phoneArea1Branch, phoneNo1Branch, managerCode, &#xD;collFee, utjBalance, installmentPaid, totalTagihanUkm, tenor, groupCode, groupName, groupAddress, sisaAngsuran, companyName, fullNameBM, addressCustomer, rtCustomer, rwCustomer, villageCodeCustomer, &#xD;villageNameCustomer, zipCodeCustomer, subdistrictNameCustomer, cityNameCustomer, provinceNameCustomer, createdDate, productCode, bucketType, bucketCode, dateDisscussion, timeDisscussion, branchCodeAggr, reprintedCount, printedBy FROM (&#xD;    SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;        A.BRANCH_CODE branchCode,&#xD;        A.SP_NO spNo,&#xD;        A.SP_DATE spDate,&#xD;        A.AGGR_NO aggrNo,&#xD;        A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;B.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;D.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;end as idAddress,&#xD;&#xD;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.HP_NO&#xD;&#x9;&#x9;&#x9;end as hpNo1,&#xD;&#xD;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;end as phoneArea1,&#xD;&#xD;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.PHONE_NO1&#xD;&#x9;&#x9;&#x9;end as phoneNo1,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA2&#xD;&#x9;&#x9;&#x9;end as phoneArea2,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO2&#xD;&#x9;&#x9;&#x9;END as phoneNo2,&#xD;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RT &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RT&#xD;&#x9;&#x9;&#x9;END as idRt,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RW &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RW&#xD;&#x9;&#x9;&#x9;END as idRw,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END as villageName,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;END as subdistrictName,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COVC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;END as cityName,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;END as provinceName,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;END as zipCode,&#xD;&#x9;&#x9;(SELECT H.PARAM_DESCRIP FROM MST_GLOBAL_PARAM H (NOLOCK) WHERE H.COMPANY_CODE = &#39;EDV&#39; AND B.PRODUCT_CODE = H.PARAM_CODE AND H.CONDITION = &#39;PRODUCT_CODE&#39; AND H.FLAG_ACTIVE = &#39;1&#39; ) productName,&#xD;        A.SP_TYPE spType,&#xD;&#x9;&#x9;I.PARAM_DESCRIP spTypeName,&#xD;&#x9;&#x9;I.VALUES_PARAM valuesParam,&#xD;        A.SP_SOLVING_DAY spSolvingDay,&#xD;        A.EXPIRED_DATE expiredDate,&#xD;        A.COLL_CODE collCode,&#xD;&#x9;&#x9;(SELECT K.FULLNAME from MST_EMPLOYEE K (nolock) where K.company_code = &#39;EDV&#39; and K.nik=A.COLL_CODE) collName,&#xD;        A.INST_FROM instFrom,&#xD;        A.INST_TO instTo,&#xD;        A.OS_INSTALLMENT osInstallment,&#xD;        A.PENALTY_FEE penaltyFee,&#xD;        A.DUE_DATE dueDate,&#xD;        A.DAY_OVERDUE dayOverdue,&#xD;        A.SP_STS spSts,&#xD;&#x9;&#x9;(SELECT J.PARAM_DESCRIP from MST_GLOBAL_PARAM J (nolock) where A.SP_STS = J.PARAM_CODE AND J.CONDITION = &#39;SP_STS&#39; AND J.FLAG_ACTIVE = &#39;1&#39; AND J.COMPANY_CODE = A.COMPANY_CODE) spStsName,&#xD;        ISNULL(A.PRINTED_DATE, &#39;1900-01-01&#39;) printedDate,&#xD;        A.SENT_DATE sentDate,&#xD;        ISNULL(A.RETURNED_DATE, &#39;1900-01-01&#39;) returnedDate,&#xD;        A.CANCELED_DATE canceledDate,&#xD;&#x9;&#x9;L.BRANCH_NAME branchName,&#xD;&#x9;&#x9;L.ADDRESS addressBranch,&#xD;&#x9;&#x9;L.RT rtBranch,&#xD;&#x9;&#x9;L.RW rwBranch,&#xD;&#x9;&#x9;VB.VILLAGE_NAME villageNameBranch,&#xD;&#x9;&#x9;VB.ZIPCODE zipCodeBranch,&#xD;&#x9;&#x9;SB.SUBDISTRICT_NAME subdistrictNameBranch,&#xD;&#x9;&#x9;CB.CITY_NAME cityNameBranch,&#xD;&#x9;&#x9;PB.PROVINCE_NAME provinceNameBranch,&#xD;&#x9;&#x9;L.PHONE_AREA1 phoneArea1Branch,&#xD;&#x9;&#x9;L.PHONE_NO1 phoneNo1Branch,&#xD;&#x9;&#x9;L.MANAGER_CODE managerCode,&#xD;&#x9;&#x9;(select top 1 M.FULLNAME from MST_EMPLOYEE M (nolock) where M.COMPANY_CODE = &#39;EDV&#39; AND L.MANAGER_CODE = M.POSITION_CODE AND L.BRANCH_CODE = M.BRANCH_CODE AND M.STATUS=1) fullNameBM,&#xD;&#x9;&#x9;A.COLL_FEE collFee,&#xD;&#x9;&#x9;A.UTJ_BALANCE utjBalance,&#xD;&#x9;&#x9;A.INSTALLMENT_PAID installmentPaid,&#xD;&#x9;&#x9;(A.OS_INSTALLMENT + A.PENALTY_FEE + A.COLL_FEE) as totalTagihanUkm,&#xD;&#x9;&#x9;A.TENOR tenor,&#xD;&#x9;&#x9;B.GROUP_CODE groupCode,&#xD;&#x9;&#x9;N.GROUP_NAME groupName,&#xD;&#x9;&#x9;N.GROUP_ADDRESS groupAddress,&#xD;&#x9;&#x9;(A.OS_INSTALLMENT - A.UTJ_BALANCE - A.INSTALLMENT_PAID) as sisaAngsuran,&#xD;&#x9;&#x9;O.COMPANY_NAME companyName,&#xD;&#x9;&#x9;P.ID_ADDRESS addressCustomer,&#xD;&#x9;&#x9;P.ID_RT rtCustomer,&#xD;&#x9;&#x9;P.ID_RW rwCustomer,&#xD;&#x9;&#x9;P.ID_VILLAGE_CODE villageCodeCustomer,&#xD;&#x9;&#x9;Q.VILLAGE_NAME villageNameCustomer,&#xD;&#x9;&#x9;Q.ZIPCODE zipCodeCustomer,&#xD;&#x9;&#x9;R.SUBDISTRICT_NAME subdistrictNameCustomer,&#xD;&#x9;&#x9;S.CITY_NAME cityNameCustomer,&#xD;&#x9;&#x9;T.PROVINCE_NAME provinceNameCustomer,&#xD;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;A.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;A.DATE_DISSCUSSION dateDisscussion,&#xD;&#x9;&#x9;A.TIME_DISSCUSSION timeDisscussion,&#xD;        B.BRANCH_CODE branchCodeAggr,&#xD;&#x9;&#x9;ISNULL(A.REPRINTED_COUNT, 0) as reprintedCount,&#xD;&#x9;&#x9;A.PRINTED_BY printedBy,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;select top 1 param_descrip from trx_dkh_dtl x (nolock) left join mst_global_param y (nolock)&#xD;&#x9;&#x9;&#x9;on x.company_code = y.company_code and&#xD;&#x9;&#x9;&#x9;x.sp_receipt_type = y.param_code and&#xD;&#x9;&#x9;&#x9;y.condition = &#39;SP_RECEIPT_TYPE&#39;&#xD;&#x9;&#x9;&#x9;where x.company_code = &#39;EDV&#39; and sp_no = A.SP_NO and aggr_no = A.AGGR_NO order by x.created_date desc&#xD;&#x9;&#x9;) as spReceiptType,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;select top 1 sp_receipt_name from trx_dkh_dtl (nolock) where company_code = &#39;EDV&#39; and sp_no = A.SP_NO and aggr_no = A.AGGR_NO order by created_date desc&#xD;&#x9;&#x9;) as spReceiptName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;select top 1 sp_notes from trx_dkh_dtl (nolock) where company_code = &#39;EDV&#39; and sp_no = A.SP_NO and aggr_no = A.AGGR_NO order by created_date desc&#xD;&#x9;&#x9;) as spNotes&#xD;&#x9;FROM TRX_SP A (nolock)&#xD;    LEFT JOIN TRX_AGGR B (nolock)&#xD;    ON A.AGGR_NO = B.AGGR_NO AND A.COMPANY_CODE = B.COMPANY_CODE&#xD;&#x9;LEFT JOIN TRX_APPL D (nolock)&#xD;&#x9;ON B.APPL_NO = D.APPL_NO AND B.COMPANY_CODE = D.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM I (nolock)&#xD;&#x9;ON A.SP_TYPE = I.PARAM_CODE AND I.CONDITION = &#39;SP_TYPE&#39; AND I.FLAG_ACTIVE = &#39;1&#39; AND A.COMPANY_CODE = I.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH L (nolock)&#xD;&#x9;ON A.BRANCH_CODE = L.BRANCH_CODE AND A.COMPANY_CODE = L.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE VB (nolock)&#xD;&#x9;ON L.VILLAGE_CODE = VB.VILLAGE_CODE AND L.COMPANY_CODE = VB.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT SB (nolock)&#xD;&#x9;ON VB.SUBDISTRICT_CODE = SB.SUBDISTRICT_CODE AND VB.COMPANY_CODE = SB.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY CB (nolock)&#xD;&#x9;ON VB.CITY_CODE = CB.CITY_CODE AND VB.COMPANY_CODE = CB.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE PB (nolock)&#xD;&#x9;ON VB.PROVINCE_CODE = PB.PROVINCE_CODE AND VB.COMPANY_CODE = PB.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_GROUP_HDR N (nolock)&#xD;&#x9;ON B.GROUP_CODE = N.GROUP_CODE AND B.COMPANY_CODE = N.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_COMPANY O (nolock)&#xD;&#x9;ON L.COMPANY_CODE = O.COMPANY_CODE AND L.COMPANY_CODE = O.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER P (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = P.CUSTOMER_CODE AND B.COMPANY_CODE = P.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE Q (nolock)&#xD;&#x9;ON P.ID_VILLAGE_CODE = Q.VILLAGE_CODE AND P.COMPANY_CODE = Q.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT R (nolock)&#xD;&#x9;ON Q.SUBDISTRICT_CODE = R.SUBDISTRICT_CODE AND Q.COMPANY_CODE = R.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY S (nolock)&#xD;&#x9;ON Q.CITY_CODE = S.CITY_CODE AND Q.COMPANY_CODE = S.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE T (nolock)&#xD;&#x9;ON Q.PROVINCE_CODE = T.PROVINCE_CODE AND Q.COMPANY_CODE = T.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = CS.CUSTOMER_CODE AND B.COMPANY_CODE = CS.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE AND CS.COMPANY_CODE = IV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE AND ISUB.COMPANY_CODE = IV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE AND IV.COMPANY_CODE = IC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE AND IV.COMPANY_CODE = IP.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE AND VB.COMPANY_CODE = CB.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE AND DSUB.COMPANY_CODE = DV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE AND DV.COMPANY_CODE = DC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE AND DV.COMPANY_CODE = DP.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39; AND A.COMPANY_CODE = F.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE AND F.COMPANY_CODE = KV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE AND KSUB.COMPANY_CODE = KV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE AND KV.COMPANY_CODE = KC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE AND KV.COMPANY_CODE = KP.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE G (nolock)&#xD;&#x9;ON A.CUSTOMER_CODE = G.CUSTOMER_CODE AND G.EMPLOYEE_TYPE = &#39;B&#39; AND A.COMPANY_CODE = G.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;ON G.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE AND G.COMPANY_CODE = BV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB (nolock)&#xD;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE AND BSUB.COMPANY_CODE = BV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY BC (nolock)&#xD;&#x9;ON BV.CITY_CODE = BC.CITY_CODE AND BV.COMPANY_CODE = BC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE BP (nolock)&#xD;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE AND BV.COMPANY_CODE = BP.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (nolock)&#xD;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE COV (nolock)&#xD;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE AND CO.COMPANY_CODE = COV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT COSUB (nolock)&#xD;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE AND COV.COMPANY_CODE = COSUB.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY COVC (nolock)&#xD;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE AND COV.COMPANY_CODE = COVC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE COP (nolock)&#xD;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE AND COV.COMPANY_CODE = COP.COMPANY_CODE&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
