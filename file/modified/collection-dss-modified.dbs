;:extCollCode,&#xD;        @AGGR_NO,&#xD;        :balPrincipalValue,&#xD;        :balIntValue,&#xD;&#x9;&#x9;:collFee,&#xD;&#x9;&#x9;:penaltyValue,&#xD;        (&#xD;&#x9;&#x9;&#x9;SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN MIN(INST_NO) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE MIN(INST_NO)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;END instNo&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE WHERE AGGR_NO = @AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = @COMPANY_CODE AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;),&#xD;        :balTenor,&#xD;&#x9;&#x9;:daysOverdue,&#xD;        :expiredDate,&#xD;        :stnkExpiredDate,&#xD;        :km,&#xD;&#x9;&#x9;&#39;0&#39;,&#xD;        :reqCostNo,&#xD;        :tuCost,&#xD;        :penaltyTuValue,&#xD;        :notesPicReceive1,&#xD;        :notesPicReceive2,&#xD;        :locCode,&#xD;        :locReceiptCode,&#xD;&#x9;&#x9;:locNotes,&#xD;        :printedBy,&#xD;        :printedDate,&#xD;        :appvSts,&#xD;        :appvDate,&#xD;        :solvingType,&#xD;        :solvingDate,&#xD;        :solvingNotes,&#xD;        :extCount,&#xD;        :vehcOutDate,&#xD;        :vehcOutBy,&#xD;        :otrValue,&#xD;        :taksasiValue,&#xD;        :totMinusValue,&#xD;        :totAddValue,&#xD;        :depositSts,&#xD;        :voucherNo,&#xD;        :voucherDate,&#xD;        :unitCondition,&#xD;        GETDATE(),&#xD;        :createdBy,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE WHERE AGGR_NO = @AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = @COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE WHERE AGGR_NO = @AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = @COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;)&#xD;)&#xD;END&#xD;ELSE&#xD;BEGIN&#xD;UPDATE TRX_STTU SET UPDATED_DATE=GETDATE() where COMPANY_CODE=@COMPANY_CODE AND STTU_NO=@STTUNO&#xD;END</sql>
