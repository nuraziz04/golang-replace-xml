.BPKB_LOCATION bpkbLocation,&#xD;&#x9;&#x9;O.PARAM_DESCRIP bpkbSts,&#xD;&#x9;&#x9;(&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN P.AGGR_NO IS NULL THEN &#39;Tidak&#39;&#xD;&#x9;&#x9;&#x9;&#x9;ELSE &#39;Iya&#39;&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as stsOutUnit&#xD;&#x9;FROM TRX_SELL_UNIT A (nolock)&#xD;&#x9;LEFT JOIN TRX_TU_DEPOSIT B (nolock)&#xD;&#x9;ON A.TU_DEPOSIT_NO = B.TU_DEPOSIT_NO&#xD;&#x9;LEFT JOIN TRX_AGGR C (nolock)&#xD;&#x9;ON A.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET D (nolock)&#xD;&#x9;ON A.AGGR_NO = D.AGGR_NO&#xD;&#x9;LEFT JOIN MST_BRAND E (nolock)&#xD;&#x9;ON D.BRAND_CODE = E.BRAND_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM F (nolock)&#xD;&#x9;ON E.CATEGORY_CODE = F.PARAM_CODE AND F.CONDITION = &#39;CATEGORY_CODE&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_TYPE G (nolock)&#xD;&#x9;ON D.TYPE_CODE = G.TYPE_CODE&#xD;&#x9;LEFT JOIN MST_KIND H (nolock)&#xD;&#x9;ON G.KIND_CODE = H.KIND_CODE&#xD;&#x9;LEFT JOIN MST_MODEL I (nolock)&#xD;&#x9;ON D.MODEL_CODE = I.MODEL_CODE&#xD;&#x9;LEFT JOIN MST_COLOR J (nolock)&#xD;&#x9;ON D.COLOR_CODE = J.COLOR_CODE&#xD;&#x9;LEFT JOIN MST_AR_STS K (nolock)&#xD;&#x9;ON C.AR_STS = K.AR_STS AND ISNULL(C.SUB_STS, &#39;&#39;) = ISNULL(K.SUB_STS, &#39;&#39;)&#xD;&#x9;LEFT JOIN MST_BRANCH L (nolock)&#xD;&#x9;ON B.LOC_CODE = L.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH M (nolock)&#xD;&#x9;ON B.LOC_RECEIPT_CODE = M.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BPKB N (nolock)&#xD;&#x9;ON A.COMPANY_CODE = N.COMPANY_CODE AND A.AGGR_NO = N.AGGR_NO&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM O (nolock)&#xD;&#x9;ON N.COMPANY_CODE = O.COMPANY_CODE AND N.BPKB_STS = O.PARAM_CODE AND O.CONDITION = &#39;BPKB_STS&#39; AND O.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_TU_OUT_DTL P (nolock)&#xD;&#x9;ON A.COMPANY_CODE = P.COMPANY_CODE AND A.AGGR_NO = P.AGGR_NO&#xD;) a WHERE :search :filter</sql>
