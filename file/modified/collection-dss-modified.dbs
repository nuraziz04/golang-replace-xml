      <query id="getCountLovNopol" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT DISTINCT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;J.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.TU_DEPOSIT_NO tuDepositNo,&#xD;&#x9;&#x9;A.TU_DEPOSIT_DATE tuDepositDate,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;B.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;C.NOPOL nopol,&#xD;&#x9;&#x9;C.BRAND_CODE brandCode,&#xD;&#x9;&#x9;D.BRAND_NAME brandName,&#xD;&#x9;&#x9;D.CATEGORY_CODE categoryCode,&#xD;&#x9;&#x9;E.PARAM_DESCRIP categoryName,&#xD;&#x9;&#x9;C.TYPE_CODE typeCode,&#xD;&#x9;&#x9;F.TYPE_NAME typeName,&#xD;&#x9;&#x9;F.KIND_CODE kindCode,&#xD;&#x9;&#x9;G.KIND_NAME kindName,&#xD;&#x9;&#x9;C.MODEL_CODE modelCode,&#xD;&#x9;&#x9;H.MODEL_NAME modelName,&#xD;&#x9;&#x9;I.COLOR_NAME colorName,&#xD;&#x9;&#x9;C.YEAR_MFG yearMfg,&#xD;&#x9;&#x9;C.CHASSIS_NO chassisNo,&#xD;&#x9;&#x9;C.MACHINE_NO machineNo,&#xD;&#x9;&#x9;C.BPKB_NAME stnkName,&#xD;&#x9;&#x9;A.KM km,&#xD;&#x9;&#x9;A.STNK_EXPIRED_DATE stnkExpiredDate,&#xD;&#x9;&#x9;A.LOC_RECEIPT_CODE locReceiptCode,&#xD;&#x9;&#x9;LRC.BRANCH_NAME locReceiptCodeName,&#xD;&#x9;&#x9;A.LOC_CODE locCode,&#xD;&#x9;&#x9;LC.BRANCH_NAME locCodeName,&#xD;&#x9;&#x9;(A.BAL_PRINCIPAL_VALUE) as sisaAngsuranPokok,&#xD;&#x9;&#x9;(A.TU_COST + A.PENALTY_TU_VALUE) as nilaiBiayaPenyelesaian,&#xD;&#x9;&#x9;(A.BAL_PRINCIPAL_VALUE) + (A.TU_COST + A.PENALTY_TU_VALUE) as perkiraanHargaJual,&#xD;&#x9;&#x9;C.BPKB_NO bpkbNo,&#xD;&#x9;&#x9;K.TRX_NAME arStsName,&#xD;&#x9;&#x9;K.NOTES subStsName,&#xD;&#x9;&#x9;A.PENALTY_TU_VALUE penaltyTuValue,&#xD;&#x9;&#x9;A.TU_COST tuCost,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PRICE_VALUE&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY PC.EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as priceValue,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.PRICE_AREA_CODE&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY PC.EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketPriceAreaCode,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.MODEL_CODE&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY PC.EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketModelCode,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.NEW_USED&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY PC.EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketNewUsed,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.YEAR_MFG&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY PC.EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketYearMfg,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.EFFECTIVE_DATE&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD