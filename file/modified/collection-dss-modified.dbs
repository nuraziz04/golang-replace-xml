      <query id="getDkhDtl" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @GROUP_CODE varchar(50) = :groupCode&#xD;declare @DKH_NO varchar(50) = :dkhNo&#xD;&#xD;SELECT companyCode, branchCode, branchCodeAggr, dkhNo, aggrNo, dkhTime, groupCode, groupName, customerCode, promiseeSts, dueDate, instFrom, instTo, dayOverdue, totUndueInstall, installment, collFee, penaltyValue,&#xD;installBill, totBill, visitCode, visitTime, paidSts, receiptNo, jbDate, jbTime, tuDate, tuTime, meetCode, meetName, addrSts, unitSts, paymentDate, presenceSts, prepaymentSts, paidCollFee, paidPenalty,&#xD;paidInstall, paidCash, discount, attCode, utjBalance, utjPlusValue, utjUsageValue, utjUsageReason, utjAppvSts, utjAppvDate, utjPosted, utjPostedDate, spNo, spType, spReceiptType, spReceiptName,&#xD;spNotes, sttuNo, collNotes, flagSend, flagSendDate, longtitude, latitude, cashierCode, billSts, customerFullname, rt, rw, hpNo1, &#xD;case when phoneArea1 is null then &#39;null&#39; else phoneArea1 end phoneArea1,&#xD;case when phoneNo1 is null then &#39;null&#39; else phoneNo1 end phoneNo1, &#xD;case when phoneArea2 is null then &#39;null&#39; else phoneArea2 end phoneArea2,&#xD;case when phoneNo2 is null then &#39;null&#39; else phoneNo2 end phoneNo2, &#xD;villageName, zipCode, subdistrictName, cityName, provinceName, collCode, collName, spTypeName, nopol, customerPaymentDate, address, branchName, tenor, iif(collFeeSchedule is null, 0,collFeeSchedule) collFeeSchedule, visitTimeValue, tuTimeValue, voucherNo, ratingCode, teamNo, billStsName, positionGroupName, dkhDate, attDesc, waktuDebitur FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;B.BRANCH_CODE branchCodeAggr,&#xD;&#x9;&#x9;A.DKH_NO dkhNo,&#xD;&#x9;&#x9;CONCAT(&#39; &#39;,A.AGGR_NO) aggrNo,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.DKH_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.DKH_TIME is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else A.DKH_TIME&#xD;&#x9;&#x9;end dkhTime,&#xD;&#x9;&#x9;CONCAT(A.GROUP_CODE, &#39; &#39;) groupCode,&#xD;&#x9;&#x9;L.GROUP_NAME groupName,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.CUSTOMER_CODE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.CUSTOMER_CODE is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else CONCAT(A.CUSTOMER_CODE, &#39; &#39;)&#xD;&#x9;&#x9;end customerCode,&#xD;&#x9;&#x9;A.PROMISEE_STS promiseeSts,&#xD;&#x9;&#x9;A.DUE_DATE dueDate,&#xD;&#x9;&#x9;A.INST_FROM instFrom,&#xD;&#x9;&#x9;A.INST_TO instTo,&#xD;&#x9;&#x9;case &#xD;&#x9;&#x9;&#x9;when A.DAY_OVERDUE = 0 then 0&#xD;&#x9;&#x9;&#x9;when A.DAY_OVERDUE is null then 0&#xD;&#x9;&#x9;&#x9;else A.DAY_OVERDUE&#xD;&#x9;&#x9;end dayOverdue,&#xD;&#x9;&#x9;A.TOT_UNDUE_INSTALL totUndueInstall,&#xD;&#x9;&#x9;A.INSTALLMENT installment,&#xD;&#x9;&#x9;A.COLL_FEE collFee,&#xD;&#x9;&#x9;A.PENALTY_VALUE penaltyValue,&#xD;&#x9;&#x9;A.INSTALL_BILL installBill,&#xD;&#x9;&#x9;A.TOT_BILL totBill,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.VISIT_CODE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.VISIT_CODE is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else A.VISIT_CODE&#xD;&#x9;&#x9;end visitCode,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.VISIT_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.VISIT_TIME is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else CONCAT(A.VISIT_TIME, &#39; &#39;)&#xD;&#x9;&#x9;end visitTime,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.PAID_STS = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.PAID_STS is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else A.PAID_STS&#xD;&#x9;&#x9;end paidSts,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.RECEIPT_NO = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.RECEIPT_NO is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else CONCAT(A.RECEIPT_NO, &#39; &#39;)&#xD;&#x9;&#x9;end receiptNo,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.JB_DATE is null then &#39;1900-01-01&#39; &#xD;&#x9;&#x9;&#x9;else A.JB_DATE&#xD;&