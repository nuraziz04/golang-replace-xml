= &#39;B&#39; THEN IIF(BV.ZIPCODE = &#39;&#39; OR BV.ZIPCODE IS NULL, &#39;null&#39;, CONCAT(BV.ZIPCODE, &#39; &#39;))&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as zipCode,&#xD;&#x9;&#x9;CONCAT(H.COLL_CODE, &#39; &#39;) collCode,&#xD;&#x9;&#x9;I.FULLNAME collName,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when (SELECT K.PARAM_DESCRIP FROM MST_GLOBAL_PARAM K (nolock) WHERE A.SP_TYPE = K.PARAM_CODE AND K.CONDITION = &#39;SP_TYPE&#39; AND K.FLAG_ACTIVE = &#39;1&#39;) = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when (SELECT K.PARAM_DESCRIP FROM MST_GLOBAL_PARAM K (nolock) WHERE A.SP_TYPE = K.PARAM_CODE AND K.CONDITION = &#39;SP_TYPE&#39; AND K.FLAG_ACTIVE = &#39;1&#39;) is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else (SELECT K.PARAM_DESCRIP FROM MST_GLOBAL_PARAM K (nolock) WHERE A.SP_TYPE = K.PARAM_CODE AND K.CONDITION = &#39;SP_TYPE&#39; AND K.FLAG_ACTIVE = &#39;1&#39;)&#xD;&#x9;&#x9;end spTypeName,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when N.NOPOL = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when N.NOPOL is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else N.NOPOL&#xD;&#x9;&#x9;end nopol,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.CUSTOMER_PAYMENT_DATE is null then &#39;1900-01-01&#39;&#xD;&#x9;&#x9;&#x9;else A.CUSTOMER_PAYMENT_DATE&#xD;&#x9;&#x9;end customerPaymentDate,&#xD;&#x9;&#x9;O.BRANCH_NAME branchName,&#xD;&#x9;&#x9;B.TENOR&#x9;tenor,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM( isnull((COLL_FEE), 0) - isnull((COLL_FEE_PAID), 0) )&#xD;&#x9;&#x9;&#x9;&#x9;total&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;) as collFeeSchedule,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.VISIT_TIME_VALUE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.VISIT_TIME_VALUE is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else CONCAT(A.VISIT_TIME_VALUE, &#39; &#39;)&#xD;&#x9;&#x9;end visitTimeValue,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.TU_TIME_VALUE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.TU_TIME_VALUE is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else A.TU_TIME_VALUE&#xD;&#x9;&#x9;end tuTimeValue,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when ISNULL(A.VOUCHER_NO, &#39;&#39;) = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.VOUCHER_NO is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else ISNULL(A.VOUCHER_NO, &#39;&#39;)&#xD;&#x9;&#x9;end voucherNo,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when A.RATING_CODE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when A.RATING_CODE is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else A.RATING_CODE&#xD;&#x9;&#x9;end ratingCode,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when CAST(A.TEAM_NO AS VARCHAR(50)) = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when CAST(A.TEAM_NO AS VARCHAR(50)) is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else CAST(A.TEAM_NO AS VARCHAR(50))&#xD;&#x9;&#x9;end teamNo,&#xD;&#x9;&#x9;P.PARAM_DESCRIP billStsName,&#xD;&#x9;&#x9;GH.GROUP_CODE positionGroupCode,&#xD;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;WHEN ISNULL(GH.GROUP_CODE, &#39;&#39;) != &#39;&#39; THEN (SELECT P1.PARAM_DESCRIP FROM MST_GLOBAL_PARAM P1 (nolock) WHERE A.COMPANY_CODE = P1.COMPANY_CODE AND P1.PARAM_CODE = &#39;P1&#39; AND P1.CONDITION = &#39;POSITION_GROUP_CODE&#39;)&#xD;&#x9;&#x9;&#x9;WHEN ISNULL(GH.GROUP_CODE, &#39;&#39;) = &#39;&#39; THEN (SELECT P2.PARAM_DESCRIP FROM MST_GLOBAL_PARAM P2 (nolock) WHERE A.COMPANY_CODE = P2.COMPANY_CODE AND P2.PARAM_CODE = &#39;P2&#39; AND P2.CONDITION = &#39;POSITION_GROUP_CODE&#39;)&#xD;&#x9;&#x9;END positionGroupName,&#xD;&#x9;&#x9;H.DKH_DATE dkhDate,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when ATT.ATT_NAME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when ATT.ATT_NAME is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else ATT.ATT_NAME&#xD;&#x9;&#x9;end attDesc,&#xD;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;when DGR.COLL_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;when DGR.COLL_TIME is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;else CONCAT(DGR.COLL_TIME, &#39; &#39;)&#xD;&#x9;&#x9;end waktuDebitur &#xD;&#x9;FROM TRX_DKH_DTL A (nolock)&#xD;&#x9;LEFT JOIN TRX_AGGR B (nolock)&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO and A.COMPANY_CODE=B.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_GROUP_HDR GH (nolock)&#xD;&#x9;ON A.GROUP_CODE = GH.GROUP_CODE AND B.APPL_NO = GH.GROUP_LEADER AND A.COMPANY_CODE=GH.COMPANY_CODE&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM P1 (nolock)&#xD;&#x9;--ON A.COMPANY_CODE = P1.COMPANY_CODE AND P1.PARAM_CODE = &#39;P1&#39; AND P1.CONDITION = &#39;POSITION_GROUP_CODE&#39;&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM P2 (nolock)&#xD;&#x9;--ON A.COMPANY_CODE = P2.COMPANY_CODE AND P2.PARAM_CODE = &#39;P2&#39; AND P2.CONDITION = &#39;POSITION_GROUP_CODE&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER C (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = C.CUSTOMER_CODE and B.COMPANY_CODE = C.COMPANY_CODE&#xD;&#x9;LEFT JOIN TRX_DKH_HDR H (nolock)&#xD;&#x9;ON A.DKH_NO = H.DKH_NO AND A.COMPANY_CODE=H.COMPANY_CODE AND A.BRANCH_CODE=H.BRANCH_CODE&#xD;&#x9;LEFT JOIN TRX_DKH_GROUP DGR (nolock)&#xD;&#x9;ON A.COMPANY_CODE = DGR.COMPANY_CODE AND A.DKH_NO = DGR.DKH_NO AND A.GROUP_CODE = DGR.GROUP_CODE AND A.BRANCH_CODE=DGR.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE I (nolock)&#xD;&#x9;ON H.COLL_CODE = I.NIK&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM K (nolock)&#xD;&#x9;--ON A.SP_TYPE = K.PARAM_CODE AND K.CONDITION = &#39;SP_TYPE&#39; AND K.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GROUP_HDR L (nolock)&#xD;&#x9;ON A.GROUP_CODE = L.GROUP_CODE AND A.COMPANY_CODE=L.COMPANY_CODE&#xD;&#x9;LEFT JOIN TRX_APPL M (nolock)&#xD;&#x9;ON B.APPL_NO = M.APPL_NO AND B.COMPANY_CODE=M.COMPANY_CODE&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET N (nolock)&#xD;&#x9;ON B.AGGR_NO = N.AGGR_NO AND B.COMPANY_CODE=N.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH O (nolock)&#xD;&#x9;ON A.BRANCH_CODE = O.BRANCH_CODE AND A.COMPANY_CODE=O.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM P (nolock)&#xD;&#x9;ON A.COMPANY_CODE = P.COMPANY_CODE AND A.BILL_STS = P.PARAM_CODE AND P.CONDITION = &#39;BILL_STS&#39; AND P.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_COLL_ATTENDANCE ATT (nolock)&#xD;&#x9;ON A.COMPANY_CODE = ATT.COMPANY_CODE AND A.ATT_CODE = ATT.ATT_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = CS.CUSTOMER_CODE AND B.COMPANY_CODE=CS.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE and CS.COMPANY_CODE=IV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE AND ISUB.COMPANY_CODE = IV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE AND IV.COMPANY_CODE=IC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE AND IV.COMPANY_CODE=IP.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE AND CS.COMPANY_CODE=DV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE AND DSUB.COMPANY_CODE=DV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE AND DV.COMPANY_CODE=DC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE AND DV.COMPANY_CODE=DP.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39; AND B.COMPANY_CODE=F.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE AND F.COMPANY_CODE=KV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE AND KSUB.COMPANY_CODE=KV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE AND KV.COMPANY_CODE=KC.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROV