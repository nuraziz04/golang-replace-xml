      <query id="getCountLovNomorKontrakSttu" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT distinct companyCode, branchCode, branchCodeAggr, productCode, collCode, aggrNo, customerFullname, customerCode, applNo, woDate, groupCode, arSts, isnull(subSts, &#39;&#39;) subSts, collAddressType,&#xD;&#x9;address, hpNo1, phoneArea1, phoneNo1, phoneArea2, phoneNo2, rt, rw, villageName, subdistrictName, cityName, provinceName, zipCode, isnull(dayOverdue, 0) dayOverdue, instFrom, instTo, tagihanAngsuran,&#xD;&#x9;tagihanDenda, (biaya + mstCollfee) biayaTagih, (tagihanAngsuran + tagihanDenda + biaya + mstCollfee) totalTagihan, angsuranBelumJatuhTempo, dueDate, angsuran, categoryCode, categoryName, brandCode, brandName, &#xD;&#x9;typeCode, typeName, modelCode, modelName, kindCode, kindName, yearMfg, nopol, chassisNo, machineNo, stnkName, colorName, villageCode, dsoCode, arStsName, bucketCode FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCodeAggr,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE_COLL_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;&#x9;A.APPL_NO applNo,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.WO_DATE IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.WO_DATE = &#39;1900-01-01&#39; THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE &#39;1&#39;&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as woDate,&#xD;&#x9;&#x9;&#x9;A.GROUP_CODE groupCode,&#xD;&#x9;&#x9;&#x9;A.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;A.SUB_STS subSts,&#xD;&#x9;&#x9;&#x9;B.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN EF.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN EF.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN EF.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN EF.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN EF.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as rt,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RW &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN EF.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN EG.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as rw,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as villageCode,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as villageName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as subdistrictName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COVC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as cityName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as provinceName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;P&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; AND A.CUSTOMER_TYPE = &#39;C&#39; THEN COV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as zipCode,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dueDate FROM (&#xD;&#x9;&#x9;&#x9;&#x9;SELECT DUE_DATE dueDate, MIN(COLL_DUE_DATE) collDueDate FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; group by DUE_DATE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dueDate,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN MIN(INST_NO) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE MIN(INST_NO)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as instFrom,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;&#x9;(&#xD;&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID)), 0)&#xD;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;INST_NO &gt;= (SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN MIN(INST_NO) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE MIN(INST_NO)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) a) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;AND&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;INST_NO &lt;= (SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END RESULT)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#xD;&#x9;&#x9;&#x9;) as tagihanAngsuran,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PINALTY_VALUE-PINALTY_PAID),0) total&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;) as tagihanDenda,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM(COLL_FEE-COLL_FEE_PAID), 0) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;total&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;) as biaya,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select collFee from (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;select top 1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;coll_fee collFee, &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;EFFECTIVE_DATE effectiveDate&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;from mst_coll_fee(nolock)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;where company_code = A.COMPANY_CODE AND product_code = A.PRODUCT_CODE AND coll_kind_fee = &#39;ADMCOL&#39; AND EFFECTIVE_DATE &lt;= GETDATE() AND (category_code is null or category_code = &#39;&#39;) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as mstCollfee,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PRINCIPAL_VALUE+INT_VALUE-PRINCIPAL_PAID-INT_PAID),0) angsuranBelumJatuhTempo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND INST_NO &gt; (SELECT MIN(INST_NO) FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PRINCIPAL_VALUE+INT_VALUE-PRINCIPAL_PAID-INT_PAID),0) angsuranBelumJatuhTempo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND INST_NO &gt; (SELECT MAX(INST_NO) instNo FROM TRX_AGGR_SCHEDULE(nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) &#xD;&#x9;&#x9;&#x9;&#x9;END RESULT &#xD;&#x9;&#x9;&#x9;) angsuranBelumJatuhTempo,&#xD;&#x9;&#x9;&#x9;A.INST_VALUE angsuran,&#xD;&#x9;&#x9;&#x9;d.category_code categoryCode,&#xD;&#x9;&#x9;&#x9;e.param_descrip categoryName,&#xD;&#x9;&#x9;&#x9;c.brand_code brandCode,&#xD;&#x9;&#x9;&#x9;d.brand_name brandName,&#xD;&#x9;&#x9;&#x9;c.type_code typeCode,&#xD;&#x9;&#x9;&#x9;f.type_name typeName,&#xD;&#x9;&#x9;&#x9;c.model_code modelCode,&#xD;&#x9;&#x9;&#x9;g.model_name modelName,&#xD;&#x9;&#x9;&#x9;f.kind_code kindCode,&#xD;&#x9;&#x9;&#x9;h.kind_name kindName,&#xD;&#x9;&#x9;&#x9;c.year_mfg yearMfg,&#xD;&#x9;&#x9;&#x9;c.nopol nopol,&#xD;&#x9;&#x9;&#x9;c.chassis_no chassisNo,&#xD;&#x9;&#x9;&#x9;c.machine_no machineNo,&#xD;&#x9;&#x9;&#x9;c.bpkb_name stnkName,&#xD;&#x9;&#x9;&#x9;i.color_name colorName,&#xD;&#x9;&#x9;&#x9;j.dso_code dsoCode,&#xD;&#x9;&#x9;&#x9;k.notes arStsName,&#xD;&#x9;&#x9;&#x9;A.bucket_code bucketCode&#xD;&#x9;&#x9;FROM TRX_AGGR(nolock) A&#xD;&#x9;&#x9;JOIN TRX_APPL(nolock) B&#xD;&#x9;&#x9;ON A.APPL_NO = B.APPL_NO&#xD;&#x9;&#x9;LEFT JOIN trx_aggr_asset(nolock) c&#xD;&#x9;&#x9;on A.aggr_no = c.aggr_no&#xD;&#x9;&#x9;left join mst_brand(nolock) d&#xD;&#x9;&#x9;on c.brand_code = d.brand_code&#xD;&#x9;&#x9;left join mst_global_param(nolock) e&#xD;&#x9;&#x9;on d.category_code = e.param_code&#xD;&#x9;&#x9;left join mst_type(nolock) f&#xD;&#x9;&#x9;on c.type_code = f.type_code&#xD;&#x9;&#x9;left join mst_model(nolock) g&#xD;&#x9;&#x9;on c.model_code = g.model_code&#xD;&#x9;&#x9;left join mst_kind(nolock) h&#xD;&#x9;&#x9;on f.kind_code = h.kind_code&#xD;&#x9;&#x9;left join mst_color(nolock) i&#xD;&#x9;&#x9;on c.color_code = i.color_code&#xD;&#x9;&#x9;left join mst_branch(nolock) j&#xD;&#x9;&#x9;on A.company_code = j.company_code and A.branch_code = j.branch_code &#xD;&#x9;&#x9;left join mst_ar_sts(nolock) k&#xD;&#x9;&#x9;ON a.ar_sts = k.ar_sts and ISNULL(k.sub_sts, &#39;&#39;) = ISNULL(a.sub_sts, &#39;&#39;)&#xD;&#x9;&#x9;LEFT JOIN MST_COLLECTOR(nolock) L&#xD;&#x9;&#x9;ON A.COMPANY_CODE = L.COMPANY_CODE&#xD;&#x9;&#x9;AND A.COLL_CODE = L.COLL_CODE &#xD;&#x9;&#x9;AND A.PRODUCT_CODE = L.PRODUCT_CODE &#xD;&#x9;&#x9;AND A.BUCKET_TYPE = L.BUCKET_TYPE&#xD;&#x9;&#x9;AND A.BUCKET_CODE = L.BUCKET_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER(nolock) CS&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = CS.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE(nolock) IV&#xD;&#x9;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT(nolock) ISUB&#xD;&#x9;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY(nolock) IC&#xD;&#x9;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE(nolock) IP&#xD;&#x9;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE(nolock) DV&#xD;&#x9;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT(nolock) DSUB&#xD;&#x9;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY(nolock) DC&#xD;&#x9;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE(nolock) DP&#xD;&#x9;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE(nolock) EF&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = EF.CUSTOMER_CODE AND EF.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE(nolock) KV&#xD;&#x9;&#x9;ON EF.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT(nolock) KSUB&#xD;&#x9;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY(nolock) KC&#xD;&#x9;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE(nolock) KP&#xD;&#x9;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE(nolock) EG&#xD;&#x9;&#x9;ON A.CUSTOMER_CODE = EG.CUSTOMER_CODE AND EG.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE(nolock) BV&#xD;&#x9;&#x9;ON EG.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT(nolock) BSUB&#xD;&#x9;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY(nolock) BC&#xD;&#x9;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE(nolock) BP&#xD;&#x9;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_CO(nolock) CO&#xD;&#x9;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE(nolock) COV&#xD;&#x9;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT(nolock) COSUB&#xD;&#x9;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY(nolock) COVC&#xD;&#x9;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE(nolock) COP&#xD;&#x9;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE&#xD;&#x9;&#x9;LEFT JOIN TRX_PREPAYMENT_HDR(nolock) PRE ON A.AGGR_NO = PRE.AGGR_NO AND PRE.APPV_STS IN (&#39;OPEN&#39;, &#39;APPR&#39;, &#39;APPD&#39;)&#xD;&#x9;&#x9;LEFT JOIN TRX_RESCHEDULE(nolock)  RES ON A.AGGR_NO = RES.AGGR_NO AND RES.APPV_STS IN (&#39;OPEN&#39;, &#39;APPR&#39;, &#39;APPD&#39;)&#xD;&#x9;&#x9;LEFT JOIN TRX_APPL(nolock) APPL ON A.AGGR_NO = APPL.REF_AGGR_NO AND APPL.APPL_STS in (&#39;OPEN&#39;, &#39;REVS&#39;)&#xD;&#x9;&#x9;LEFT JOIN TRX_STTU(nolock) STU ON A.AGGR_NO=STU.AGGR_NO AND STU.STTU_STS IN (&#39;OPEN&#39;,&#39;APPR&#39;,&#39;PROC&#39;) AND EXPIRED_DATE &gt; GETDATE()&#xD;&#x9;&#x9;LEFT JOIN (SELECT X.AGGR_NO FROM TRX_TU_DEPOSIT(nolock) X JOIN TRX_STTU(nolock) Y ON X.STTU_NO = Y.STTU_NO AND X.AGGR_NO = Y.AGGR_NO AND Y.STTU_STS = &#39;SUCC&#39;) Z ON A.AGGR_NO=Z.AGGR_NO&#xD;&#x9;&#x9;WHERE STU.AGGR_NO IS NULL AND Z.AGGR_NO IS NULL AND PRE.AGGR_NO IS NULL AND RES.AGGR_NO IS NULL AND APPL.REF_AGGR_NO IS NULL&#xD;&#x9;) a where instFrom != 0&#xD;) a WHERE :search :filter</sql>
