      <query id="getCountTrxSttuCost" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT DISTINCT&#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.REQ_COST_NO reqCostNo,&#xD;&#x9;&#x9;&#x9;A.REQ_COST_DATE reqCostDate,&#xD;&#x9;&#x9;&#x9;A.STTU_NO sttuNo,&#xD;&#x9;&#x9;&#x9;B.STTU_DATE sttuDate,&#xD;&#x9;&#x9;&#x9;B.EXPIRED_DATE expiredDate,&#xD;&#x9;&#x9;&#x9;A.TYPE_COST typeCost,&#xD;&#x9;&#x9;&#x9;--C.PARAM_DESCRIP typeCostName,&#xD;(SELECT C.PARAM_DESCRIP FROM MST_GLOBAL_PARAM C (NOLOCK) WHERE A.TYPE_COST = C.PARAM_CODE AND C.CONDITION = &#39;TYPE_COST&#39; AND C.FLAG_ACTIVE = &#39;1&#39;) typeCostName,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;F.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;A.TOT_REQ_VALUE totReqValue,&#xD;&#x9;&#x9;&#x9;A.TOT_REAL_VALUE totRealValue,&#xD;&#x9;&#x9;&#x9;A.PIC_CODE picCode,&#xD;&#x9;&#x9;&#x9;D.FULLNAME picFullname,&#xD;&#x9;&#x9;&#x9;A.PIC_POSITION picPosition,&#xD;&#x9;&#x9;&#x9;E.POSITION_NAME positionName,&#xD;&#x9;&#x9;&#x9;A.SUCCESS_FLAG successFlag,&#xD;&#x9;&#x9;&#x9;A.REQ_COST_STS reqCostSts,&#xD;&#x9;&#x9;&#x9;--UPPER(G.PARAM_DESCRIP) reqCostStsName,&#xD;(SELECT UPPER(G.PARAM_DESCRIP) FROM MST_GLOBAL_PARAM G (NOLOCK) WHERE A.REQ_COST_STS = G.PARAM_CODE AND G.CONDITION = &#39;REQ_COST_STS&#39; AND G.FLAG_ACTIVE = &#39;1&#39;) reqCostStsName,&#xD;&#x9;&#x9;&#x9;A.REQ_COST_STS_DATE reqCostStsDate,&#xD;&#x9;&#x9;&#x9;A.APPV_DATE appvDate,&#xD;&#x9;&#x9;&#x9;A.DISB_DATE disbDate,&#xD;&#x9;&#x9;&#x9;A.REAL_DATE realDate,&#xD;&#x9;&#x9;&#x9;A.CLOSE_DATE closeDate,&#xD;&#x9;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;B.STTU_STS sttuSts,&#xD;&#x9;&#x9;&#x9;H.DSO_CODE dsoCode,&#xD;&#x9;&#x9;&#x9;A.CREATED_BY requestNik,&#xD;&#x9;&#x9;&#x9;I.FULLNAME requestName,&#xD;&#x9;&#x9;&#x9;I.POSITION_CODE requestPosition,&#xD;&#x9;&#x9;&#x9;B.PIC_PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when DEP.TU_DEPOSIT_NO IS NULL then &#39;Tidak&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else &#39;Ya&#39;&#xD;&#x9;&#x9;&#x9;end flagBerhasilTarik,&#xD;&#x9;&#x9;&#x9;A.CHRONOLOGICAL chronological&#xD;&#x9;FROM TRX_STTU_COST_HDR A (NOLOCK)&#xD;&#x9;LEFT JOIN TRX_STTU B (NOLOCK)&#xD;&#x9;ON A.STTU_NO = B.STTU_NO&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM C (NOLOCK)&#xD;&#x9;--ON A.TYPE_COST = C.PARAM_CODE AND C.CONDITION = &#39;TYPE_COST&#39; AND C.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_EMPLOYEE D (NOLOCK)&#xD;&#x9;ON A.PIC_CODE = D.NIK&#xD;&#x9;LEFT JOIN MST_POSITION E (NOLOCK)&#xD;&#x9;ON A.PIC_POSITION = E.POSITION_CODE&#xD;&#x9;LEFT JOIN TRX_AGGR F (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = F.AGGR_NO&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM G (NOLOCK)&#xD;&#x9;--ON A.REQ_COST_STS = G.PARAM_CODE AND G.CONDITION = &#39;REQ_COST_STS&#39; AND G.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_BRANCH H (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = H.COMPANY_CODE AND A.BRANCH_CODE = H.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE I (NOLOCK)&#xD;&#x9;ON A.CREATED_BY = I.NIK&#xD;&#x9;LEFT JOIN TRX_TU_DEPOSIT DEP (NOLOCK)&#xD;&#x9;ON A.STTU_NO = DEP.STTU_NO AND B.AGGR_NO = DEP.AGGR_NO&#xD;) a WHERE :search :filter</sql>
