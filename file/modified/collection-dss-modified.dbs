         <sql>SELECT companyCode, branchCode, branchName, aggrNo, groupCode, customerCode, customerFullname, collAddressType, villageName, idNo FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;E.BRANCH_NAME branchName,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;A.GROUP_CODE groupCode,&#xD;&#x9;&#x9;A.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;A.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as villageName,&#xD;&#x9;&#x9;B.ID_NO idNo&#xD;&#x9;FROM TRX_AGGR A (NOLOCK)&#xD;&#x9;LEFT JOIN MST_CUSTOMER B (NOLOCK)&#xD;&#x9;ON A.CUSTOMER_CODE = B.CUSTOMER_CODE AND A.COMPANY_CODE=B.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV (NOLOCK)&#xD;&#x9;ON B.ID_VILLAGE_CODE = IV.VILLAGE_CODE AND B.COMPANY_CODE=IV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV (NOLOCK)&#xD;&#x9;ON B.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE AND B.COMPANY_CODE=DV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE C (NOLOCK)&#xD;&#x9;ON A.CUSTOMER_CODE = C.CUSTOMER_CODE AND C.EMPLOYEE_TYPE = &#39;O&#39; AND A.COMPANY_CODE=C.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE KV (NOLOCK)&#xD;&#x9;ON C.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE AND C.COMPANY_CODE=KV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE D (NOLOCK)&#xD;&#x9;ON A.CUSTOMER_CODE = D.CUSTOMER_CODE AND D.EMPLOYEE_TYPE = &#39;B&#39; AND A.COMPANY_CODE=D.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE BV (NOLOCK)&#xD;&#x9;ON D.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE AND D.COMPANY_CODE=BV.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH E (NOLOCK)&#xD;&#x9;ON A.BRANCH_CODE = E.BRANCH_CODE AND A.COMPANY_CODE=E.COMPANY_CODE&#xD;        WHERE A.PRODUCT_CODE = &#39;PC02&#39;&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="companyCode" name="companyCode" xsdType="string"></element>
            <element column="branchCode" name="branchCode" xsdType="string"></element>
            <element column="branchName" name="branchName" xsdType="string"></element>
            <element column="aggrNo" name="aggrNo" xsdType="string"></element>
            <element column="groupCode" name="groupCode" xsdType="string"></element>
            <element column="customerCode" name="customerCode" xsdType="string"></element>
            <element column="customerFullname" name="customerFullname" xsdType="string"></element>
            <element column="collAddressType" name="collAddressType" xsdType="string"></element>
            <element column="villageName" name="villageName" xsdType="string"></element>
            <element column="idNo" name="idNo" xsdType="string"></element>
         </result>
         <param name="search" sqlType="QUERY_STRING"></param>
         <param name="filter" sqlType="QUERY_STRING"></param>
         <param name="orderBy" sqlType="QUERY_STRING"></param>
         <param name="direction" sqlType="QUERY_STRING"></param>
         <param name="page" sqlType="INTEGER"></param>
         <param name="limit" sqlType="INTEGER"></param>
      </query>
