      <query id="generateMstCoverAreaDtlDso" returnUpdatedRowCount="false" useConfig="UI_SUBMIT">
         <sql>declare @companyCode&#x9;varchar(10) = :companyCode&#xD;declare @productCode&#x9;varchar(10) = :productCode&#xD;declare @branchCode&#x9;&#x9;varchar(20) = :branchCode&#xD;&#xD;select &#xD;&#x9;company_code companyCode&#xD;&#x9;, village_code villageCode&#xD;&#x9;, village_name villageName&#xD;&#x9;, product_code productCode&#xD;&#x9;, product_name productName&#xD;&#x9;, CLUSTER cluster&#xD;&#x9;, SUM(TOT_AGGR) as totalAggr&#xD;&#x9;, subdistrict_name subdistrictName&#xD;from ( &#xD;&#x9;select distinct&#xD;&#x9;&#x9;c.company_code,&#xD;&#x9;&#x9;br.branch_code,&#xD;&#x9;&#x9;j.branch_name,&#xD;&#x9;&#x9;c.village_code,&#xD;&#x9;&#x9;c.village_name,&#xD;&#x9;&#x9;k.PARAM_CODE as product_code,&#xD;&#x9;&#x9;k.PARAM_DESCRIP as product_name,&#xD;&#x9;&#x9;&#39;&#39; CLUSTER,&#xD;&#x9;&#x9;ISNULL(l.TOT_AGGR,0) AS TOT_AGGR,&#xD;&#x9;&#x9;e.subdistrict_name&#xD;&#x9;from [dbo].[mst_village] c&#xD;&#xD;&#x9;left join [dbo].[mst_branch] br&#xD;&#x9;on c.company_code = br.company_code&#xD;&#x9;and br.branch_code = @branchCode&#xD;&#xD;&#x9;join dbo.getviewbranch() d&#xD;&#x9;on d.company_code = br.company_code&#xD;&#x9;and (&#xD;&#x9;case &#xD;&#x9;when br.structure_type = &#39;ST05&#39; then d.branch_code&#xD;&#x9;when br.structure_type = &#39;ST04&#39; then d.dso_code end&#xD;&#x9;) = br.branch_code&#xD;&#xD;&#x9;left join [dbo].[trx_aggr] a&#xD;&#x9;on a.company_code = c.company_code&#xD;&#x9;and a.branch_code = d.branch_code&#xD;&#x9;and a.coll_village_code = c.VILLAGE_CODE&#xD;&#x9;and (a.wo_date is null or a.wo_date = &#39;1900-01-01&#39;)&#xD;&#x9;and a.ar_sts in (&#39;ar&#39;,&#39;bd&#39;)&#xD;&#xD;&#x9;left join [dbo].[MST_TOTAL_AGGR] l&#xD;&#x9;on l.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and l.BRANCH_CODE = d.BRANCH_CODE&#xD;&#x9;and l.VILLAGE_CODE = c.VILLAGE_CODE&#xD;&#xD;&#x9;left join [dbo].[MST_BRANCH] j&#xD;&#x9;on j.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and j.BRANCH_CODE = d.BRANCH_CODE&#xD;&#xD;&#x9;left join [dbo].[MST_GLOBAL_PARAM] k&#xD;&#x9;on k.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and k.CONDITION = &#39;PRODUCT_CODE&#39;&#xD;&#x9;and k.PARAM_CODE = @productCode&#xD;&#xD;&#x9;left join [dbo].[mst_subdistrict] e&#xD;&#x9;on e.company_code = c.company_code&#xD;&#x9;and e.SUBDISTRICT_CODE = c.subdistrict_code&#xD;&#xD;&#x9;left join [dbo].[mst_cover_area_dtl] f&#xD;&#x9;on f.company_code = c.COMPANY_CODE&#xD;&#x9;and f.village_code = c.VILLAGE_CODE&#xD;&#x9;and f.BRANCH_CODE = d.BRANCH_CODE&#xD;&#x9;and f.PRODUCT_CODE = @productCode&#xD;&#xD;&#x9;left join [dbo].[MST_COVER_AREA_HDR] g&#xD;&#x9;on g.COMPANY_CODE = f.COMPANY_CODE&#xD;&#x9;and g.BRANCH_CODE = f.BRANCH_CODE&#xD;&#x9;and g.FLAG_ACTIVE = 1&#xD;&#xD;&#xD;&#x9;where (a.coll_village_code is not null or (f.village_code is not null and g.branch_code is not null)) &#xD;&#x9;and c.company_code = @companyCode&#xD;) a&#xD;group by &#xD;&#x9;COMPANY_CODE&#xD;&#x9;, village_code&#xD;&#x9;, village_name&#xD;&#x9;, product_code&#xD;&#x9;, product_name&#xD;&#x9;, CLUSTER&#xD;&#x9;, subdistrict_name&#xD;order by village_code asc</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="companyCode" name="companyCode" xsdType="string"></element>
