      <query id="getCountLovDkhNo" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>DECLARE @COLL_CODE VARCHAR(50) = :collCode&#xD;&#xD;SELECT COUNT(0) as total FROM (&#xD;&#x9;SELECT DISTINCT companyCode, branchCode, productCode, collCode, collName, dkhNo, qtyCustomer, positionName, qtyGroup, collHeadCode, bucketType FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;B.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;A.COLL_HEAD_CODE collHeadCode,&#xD;&#x9;&#x9;&#x9;C.DKH_NO dkhNo,&#xD;&#x9;&#x9;&#x9;C.QTY_CUSTOMER qtyCustomer,&#xD;&#x9;&#x9;&#x9;C.QTY_GROUP qtyGroup,&#xD;&#x9;&#x9;&#x9;B.POSITION_CODE positionCode,&#xD;&#x9;&#x9;&#x9;D.POSITION_NAME positionName,&#xD;&#x9;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;&#x9;C.DKH_DATE dkhDate&#xD;&#x9;&#x9;FROM MST_COLLECTOR A (NOLOCK)&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE B (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = B.NIK&#xD;&#x9;&#x9;JOIN TRX_DKH_HDR C (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = C.COLL_CODE AND C.DKH_STS NOT IN (&#39;CLOSE&#39;, &#39;CANC&#39;)&#xD;&#x9;&#x9;LEFT JOIN MST_POSITION D (NOLOCK)&#xD;&#x9;&#x9;ON B.POSITION_CODE = D.POSITION_CODE&#xD;&#x9;&#x9;WHERE ( A.FLAG_ACTIVE = &#39;1&#39; AND C.DKH_STS IN (&#39;OPEN&#39;, &#39;PROC&#39;) AND A.BUCKET_TYPE = &#39;FCOL&#39;) AND B.STATUS = &#39;1&#39;&#xD;&#x9;) a where dkhDate = :dkhDate and collCode not in (@COLL_CODE)&#xD;&#xD;&#x9;UNION ALL&#xD;&#xD;&#x9;SELECT DISTINCT companyCode, branchCode, productCode, collCode, collName, dkhNo, qtyCustomer, positionName, qtyGroup, collHeadCode, bucketType FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;B.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;A.COLL_HEAD_CODE collHeadCode,&#xD;&#x9;&#x9;&#x9;&#39;&#39; as dkhNo,&#xD;&#x9;&#x9;&#x9;0 as qtyCustomer,&#xD;&#x9;&#x9;&#x9;0 as qtyGroup,&#xD;&#x9;&#x9;&#x9;B.POSITION_CODE positionCode,&#xD;&#x9;&#x9;&#x9;D.POSITION_NAME positionName,&#xD;&#x9;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;&#x9;C.DKH_DATE dkhDate&#xD;&#x9;&#x9;FROM MST_COLLECTOR A (NOLOCK)&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE B (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = B.NIK&#xD;&#x9;&#x9;LEFT JOIN TRX_DKH_HDR C (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = C.COLL_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_POSITION D (NOLOCK)&#xD;&#x9;&#x9;ON B.POSITION_CODE = D.POSITION_CODE&#xD;&#x9;&#x9;WHERE A.FLAG_ACTIVE = &#39;1&#39; AND A.BUCKET_TYPE = &#39;FCOL&#39; AND C.DKH_STS NOT IN (&#39;OPEN&#39;, &#39;PROC&#39;) AND B.STATUS = &#39;1&#39;&#xD;&#x9;&#x9;AND A.COLL_CODE NOT IN (SELECT COLL_CODE FROM TRX_DKH_HDR WHERE DKH_STS IN (&#39;OPEN&#39;,&#39;PROC&#39;))&#xD;&#x9;) a where collCode not in (@COLL_CODE)&#xD;&#xD;&#x9;UNION ALL&#xD;&#xD;&#x9;SELECT DISTINCT companyCode, branchCode, productCode, collCode, collName, dkhNo, qtyCustomer, positionName, qtyGroup, collHeadCode, bucketType FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;B.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;A.COLL_HEAD_CODE collHeadCode,&#xD;&#x9;&#x9;&#x9;ISNULL(C.DKH_NO, &#39;&#39;) dkhNo,&#xD;&#x9;&#x9;&#x9;ISNULL(C.QTY_CUSTOMER, 0) qtyCustomer,&#xD;&#x9;&#x9;&#x9;ISNULL(C.QTY_GROUP, 0) qtyGroup,&#xD;&#x9;&#x9;&#x9;B.POSITION_CODE positionCode,&#xD;&#x9;&#x9;&#x9;D.POSITION_NAME positionName,&#xD;&#x9;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;&#x9;C.DKH_DATE dkhDate&#xD;&#x9;&#x9;FROM MST_COLLECTOR A (NOLOCK)&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE B (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = B.NIK&#xD;&#x9;&#x9;LEFT JOIN TRX_DKH_HDR C (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = C.COLL_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_POSITION D (NOLOCK)&#xD;&#x9;&#x9;ON B.POSITION_CODE = D.POSITION_CODE&#xD;&#x9;&#x9;WHERE A.FLAG_ACTIVE = &#39;1&#39; AND A.BUCKET_TYPE = &#39;FCOL&#39; AND C.DKH_NO IS NULL AND B.STATUS = &#39;1&#39;&#xD;&#x9;) a where collCode not in (@COLL_CODE)&#xD;) t WHERE :search :filter</sql>
