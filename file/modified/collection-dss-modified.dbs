      <query id="getDkhGroup" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @DKH_NO varchar(50) = :dkhNo&#xD;&#xD;SELECT companyCode, branchCode, dkhNo, groupCode, groupName, groupAddress, groupRt, groupRw, villageCode, villageName, subdistrictName, cityName, provinceName, collCode, collName,&#xD;dkhDate, collTime, visitCode, visitTime, jbDate, jbTime, tuDate, tuTime, reasonUnvisit, receiptNo, qtyCustomerPaid, totCollFee, totPenaltyValue, totInstallBill, totBill, totPaidCollFee,&#xD;totPaidPenalty, totPaidInstallBill, totUtjPlusValue, totPaidCash, totUtjUsageValue, billSts, flagSend, flagSendDate, dkhSts, dkhStsName, janjiTagih, billStsName, collTimeDescrip, flagRevitalization, flagGroup FROM (&#xD;&#x9;SELECT companyCode, branchCode, dkhNo, groupCode, groupName, groupAddress, groupRt, groupRw, villageCode, villageName, subdistrictName, cityName, provinceName, collCode, collName,&#xD;&#x9;dkhDate, collTime, visitCode, visitTime, jbDate, jbTime, tuDate, tuTime, reasonUnvisit, receiptNo, qtyCustomerPaid, totCollFee, totPenaltyValue, totInstallBill, totBill, totPaidCollFee,&#xD;&#x9;totPaidPenalty, totPaidInstallBill, totUtjPlusValue, totPaidCash, totUtjUsageValue, billSts, flagSend, flagSendDate, dkhSts, dkhStsName, janjiTagih, billStsName, collTimeDescrip, jumlahDebitur, jumlahDebiturHapus, flagRevitalization, flagGroup FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.DKH_NO dkhNo,&#xD;&#x9;&#x9;&#x9;CONCAT(A.GROUP_CODE, &#39; &#39;) groupCode,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_NAME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_NAME is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else B.GROUP_NAME&#xD;&#x9;&#x9;&#x9;end groupName,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_ADDRESS = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_ADDRESS is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else B.GROUP_ADDRESS&#xD;&#x9;&#x9;&#x9;end groupAddress,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_RT = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_RT is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else B.GROUP_RT&#xD;&#x9;&#x9;&#x9;end groupRt,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_RW = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_RW is null then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else B.GROUP_RW&#xD;&#x9;&#x9;&#x9;end groupRw,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_VILLAGE_CODE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when B.GROUP_VILLAGE_CODE IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else CONCAT(B.GROUP_VILLAGE_CODE, &#39; &#39;)&#xD;&#x9;&#x9;&#x9;end villageCode,&#xD;&#x9;&#x9;&#x9;B.FLAG_REVITALIZATION flagRevitalization,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when C.VILLAGE_NAME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when C.VILLAGE_NAME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else C.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;end villageName,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when D.SUBDISTRICT_NAME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when D.SUBDISTRICT_NAME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else D.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;end subdistrictName,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when E.CITY_NAME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when E.CITY_NAME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else E.CITY_NAME&#xD;&#x9;&#x9;&#x9;end cityName,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when F.PROVINCE_NAME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when F.PROVINCE_NAME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else F.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;end provinceName,&#xD;&#x9;&#x9;&#x9;CONCAT(G.COLL_CODE, &#39; &#39;) collCode,&#xD;&#x9;&#x9;&#x9;H.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;G.DKH_DATE dkhDate,&#xD;&#x9;&#x9;&#x9;G.DKH_STS dkhSts,&#xD;&#x9;&#x9;&#x9;IIF(I.PARAM_DESCRIP = &#39;&#39; OR I.PARAM_DESCRIP IS NULL, &#39;null&#39;, I.PARAM_DESCRIP) dkhStsName,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.COLL_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.COLL_TIME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else CONCAT(A.COLL_TIME, &#39; &#39;)&#xD;&#x9;&#x9;&#x9;end collTime,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when K.PARAM_DESCRIP = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when K.PARAM_DESCRIP IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else K.PARAM_DESCRIP&#xD;&#x9;&#x9;&#x9;end collTimeDescrip,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.VISIT_CODE = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.VISIT_CODE IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else A.VISIT_CODE&#xD;&#x9;&#x9;&#x9;end visitCode,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.VISIT_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.VISIT_TIME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else CONCAT(A.VISIT_TIME, &#39; &#39;)&#xD;&#x9;&#x9;&#x9;end visitTime,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.JB_DATE is null then &#39;1900-01-01&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else A.JB_DATE&#xD;&#x9;&#x9;&#x9;end jbDate,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.JB_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.JB_TIME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else A.JB_TIME&#xD;&#x9;&#x9;&#x9;end jbTime,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.TU_DATE is null then &#39;1900-01-01&#39; &#xD;&#x9;&#x9;&#x9;&#x9;else A.TU_DATE&#xD;&#x9;&#x9;&#x9;end tuDate,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.TU_TIME = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.TU_TIME IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else A.TU_TIME&#xD;&#x9;&#x9;&#x9;end tuTime,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.REASON_UNVISIT = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.REASON_UNVISIT IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else A.REASON_UNVISIT&#xD;&#x9;&#x9;&#x9;end reasonUnvisit,&#xD;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;when A.RECEIPT_NO = &#39;&#39; then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;when A.RECEIPT_NO IS NULL then &#39;null&#39;&#xD;&#x9;&#x9;&#x9;&#x9;else CONCAT(A.RECEIPT_NO, &#39; &#39;)&#xD;&#x9;&#x9;&#x9;end receiptNo,&#xD;&#x9;&#x9;&#x9;A.QTY_CUSTOMER_PAID qtyCustomerPaid,&#xD;&#x9;&#x9;&#x9;A.TOT_COLL_FEE totCollFee,&#xD;&#x9;&#x9;&#x9;A.TOT_PENALTY_VALUE totPenaltyValue,&#xD;&#x9;&#x9;&#x9;A.TOT_INSTALL_BILL totInstallBill,&#xD;&#x9;&#x9;&#x9;A.TOT_BILL totBill,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_COLL_FEE totPaidCollFee,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_PENALTY totPaidPenalty,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_INSTALL_BILL totPaidInstallBill,&#xD;&#x9;&#x9;&#x9;A.TOT_UTJ_PLUS_VALUE totUtjPlusValue,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_CASH totPaidCash,&#xD;&#x9;&#x9;&#x9;A.TOT_UTJ_USAGE_VALUE totUtjUsageValue,&#xD;&#x9;&#x9;&#x9;IIF(A.BILL_STS = &#39;&#39; OR A.BILL_STS IS NULL, &#39;null&#39;, A.BILL_STS) billSts,&#xD;&#x9;&#x9;&#x9;A.FLAG_SEND flagSend,&#xD;&#x9;&#x9;&#x9;A.FLAG_SEND_DATE flagSendDate,&#xD;&#x9;&#x9;&#x9;(SELECT ISNULL(COUNT(0),0) GROUP_CODE &#xD;&#x9;&#x9;&#x9;FROM TRX_TAGIH_ULANG Q (nolock) &#xD;&#x9;&#x9;&#x9;LEFT JOIN TRX_DKH_GROUP R (nolock)&#xD;&#x9;&#x9;&#x9;ON Q.GROUP_CODE = R.GROUP_CODE WHERE Q.COMPANY_CODE = A.COMPANY_CODE AND Q.GROUP_CODE = A.GROUP_CODE AND Q.TU_DATE = CONVERT(DATE, GETDATE())) as janjiTagih,&#xD;&#x9;&#x9;&#x9;IIF(J.PARAM_DESCRIP = &#39;&#39; OR J.PARAM_DESCRIP IS NULL, &#39;null&#39;, J.PARAM_DESCRIP) billStsName,&#xD;&#x9;&#x9;&#x9;(select count(0) as total from trx_dkh_dtl(nolock) where COMPANY_CODE = A.COMPANY_CODE and BRANCH_CODE = A.BRANCH_CODE and dkh_no = A.DKH_NO and group_code = A.GROUP_CODE) jumlahDebitur,&#xD;&#x9;&#x9;&#x9;(select count(0) as total from trx_dkh_dtl(nolock) where COMPANY_CODE = A.COMPANY_CODE and BRANCH_CODE = A.BRANCH_CODE and dkh_no = A.DKH_NO and group_code = A.GROUP_CODE and paid_sts = &#39;999&#39;) jumlahDebiturHapus,&#xD;&#x9;&#x9;&#x9;&#39;0&#39; as flagGroup&#xD;&#x9;&#x9;FROM TRX_DKH_GROUP A (nolock)&#xD;&#x9;&#x9;LEFT JOIN MST_GROUP_HDR B (nolock)&#xD;&#x9;&#x9;ON A.GROUP_CODE = B.GROUP_CODE AND A.COMPANY_CODE=B.COMPANY_CODE&#x9;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE C (nolock)&#xD;&#x9;&#x9;ON B.GROUP_VILLAGE_CODE = C.VILLAGE_CODE AND B.COMPANY_CODE = C.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT D (nolock)&#xD;&#x9;&#x9;ON C.SUBDISTRICT_CODE = D.SUBDISTRICT_CODE AND C.COMPANY_CODE=D.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY E (nolock)&#xD;&#x9;&#x9;ON C.CITY_CODE = E.CITY_CODE AND C.COMPANY_CODE=E.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE F (nolock)&#xD;&#x9;&#x9;ON C.PROVINCE_CODE = F.PROVINCE_CODE AND C.COMPANY_CODE=F.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN TRX_DKH_HDR G (nolock)&#xD;&#x9;&#x9;ON A.DKH_NO = G.DKH_NO AND A.COMPANY_CODE=G.COMPANY_CODE AND A.BRANCH_CODE=G.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE H (nolock)&#xD;&#x9;&#x9;ON G.COLL_CODE = H.NIK&#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM I (nolock)&#xD;&#x9;&#x9;ON G.DKH_STS = I.PARAM_CODE AND I.CONDITION = &#39;DKH_STS&#39; AND I.FLAG_ACTIVE = &#39;1&#39; AND G.COMPANY_CODE=I.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM J (nolock)&#xD;&#x9;&#x9;ON A.BILL_STS = J.PARAM_CODE AND J.CONDITION = &#39;BILL_STS&#39; AND J.FLAG_ACTIVE = &#39;1&#39; AND A.COMPANY_CODE=J.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM K (nolock)&#xD;&#x9;&#x9;ON A.COLL_TIME = K.PARAM_CODE AND K.CONDITION = &#39;VISIT_TIME&#39; AND K.FLAG_ACTIVE = &#39;1&#39; AND A.COMPANY_CODE=K.COMPANY_CODE&#xD;&#x9;&#x9;WHERE A.COMPANY_CODE = @COMPANY_CODE AND A.DKH_NO = @DKH_NO AND A.FLAG_SEND IN (&#39;1&#39;,&#39;2&#39;)&#xD;&#x9;) t WHERE jumlahDebitur != jumlahDebiturHapus&#xD;) a</sql>
