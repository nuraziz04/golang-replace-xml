      <query id="getCountLovKelompok" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>DECLARE @COLL_DAY VARCHAR(MAX) = :collDay&#xD;&#xD;SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT distinct companyCode, branchCode, collCode, groupCode, groupName, collDayName, groupAddress, flagRevitalization, villageName, subdistrictName, cityName, provinceName, totalDebitur, createdDate, groupLeader, collTime, collTimeDescrip FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;A.GROUP_CODE groupCode,&#xD;&#x9;&#x9;&#x9;A.GROUP_NAME groupName,&#xD;&#x9;&#x9;&#x9;case when A.FLAG_REVITALIZATION = &#39;0&#39; THEN A.COLL_DAY &#xD;&#x9;&#x9;&#x9;else cdtl.day_code&#xD;&#x9;&#x9;&#x9;end as collDay,&#xD;&#x9;&#x9;&#x9;CASE WHEN A.FLAG_REVITALIZATION = &#39;0&#39; THEN&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;1&#39; then &#39;Minggu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;2&#39; then &#39;Senin&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;3&#39; then &#39;Selasa&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;4&#39; then &#39;Rabu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;5&#39; then &#39;Kamis&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;6&#39; then &#39;Jumat&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when A.COLL_DAY = &#39;7&#39; then &#39;Sabtu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;end&#xD;&#x9;&#x9;&#x9;) else STUFF((SELECT &#39;, &#39; + &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when cdtl.day_code = &#39;1&#39; then &#39;Minggu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when cdtl.day_code = &#39;2&#39; then &#39;Senin&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when cdtl.day_code = &#39;3&#39; then &#39;Selasa&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when cdtl.day_code = &#39;4&#39; then &#39;Rabu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when cdtl.day_code = &#39;5&#39; then &#39;Kamis&#39;&#xD;&#x9;&#x9;