      <query id="updateTrxDkhGroup" returnUpdatedRowCount="true" useConfig="UI_SUBMIT">
         <sql>set transaction isolation level READ UNCOMMITTED&#xD;&#xD;declare @visitCode varchar(10) = :visitCode&#xD;declare @companycode varchar(10) = :companyCode&#xD;declare @branchcode varchar(10) = :branchCode&#xD;declare @dkhno varchar(50) = :dkhNo&#xD;declare @groupcode varchar(50) = :groupCode&#xD;declare @COLL_TIME varchar(10) = :collTime&#xD;declare @VISIT_TIME varchar(10) = :visitTime&#xD;declare @JB_DATE date = :jbDate&#xD;declare @JB_TIME varchar(10) = :jbTime&#xD;declare @TU_DATE date = :tuDate&#xD;declare @TU_TIME varchar(10) = :tuTime&#xD;declare @REASON_UNVISIT varchar(500) = :reasonUnvisit&#xD;declare @RECEIPT_NO varchar(50) = :receiptNo&#xD;declare @QTY_CUSTOMER_PAID numeric(5,0) = :qtyCustomerPaid&#xD;declare @TOT_COLL_FEE numeric(20,4) = :totCollFee&#xD;declare @TOT_PENALTY_VALUE numeric(20,4) = :totPenaltyValue&#xD;declare @TOT_INSTALL_BILL numeric(20,4) = :totInstallBill&#xD;declare @TOT_BILL numeric(20,4) = :totBill&#xD;declare @TOT_PAID_COLL_FEE numeric(20,4) = :totPaidCollFee&#xD;declare @TOT_PAID_PENALTY numeric(20,4) = :totPaidPenalty&#xD;declare @TOT_PAID_INSTALL_BILL numeric(20,4) = :totPaidInstallBill&#xD;declare @TOT_UTJ_PLUS_VALUE numeric(20,4) = :totUtjPlusValue&#xD;declare @TOT_PAID_CASH numeric(20,4) = :totPaidCash&#xD;declare @TOT_UTJ_USAGE_VALUE numeric(20,4) = :totUtjUsageValue&#xD;declare @BILL_STS varchar(10) = :billSts&#xD;declare @FLAG_SEND tinyint = :flagSend&#xD;declare @FLAG_SEND_DATE datetime = :flagSendDate&#xD;declare @UPDATED_BY varchar(30) = :updatedBy&#xD;&#xD;&#xD;UPDATE TRX_DKH_GROUP SET&#xD;&#x9;COLL_TIME = @COLL_TIME,&#xD;&#x9;VISIT_CODE = case when isnull(VISIT_CODE,&#39;&#39;) = &#39;&#39; AND flag_send not in (&#39;2&#39;)  then  @visitCode else VISIT_CODE end,&#xD;&#x9;VISIT_TIME = @VISIT_TIME,&#xD;&#x9;JB_DATE = @JB_DATE,&#xD;&#x9;JB_TIME = @JB_TIME,&#xD;&#x9;TU_DATE = @TU_DATE,&#xD;&#x9;TU_TIME = @TU_TIME,&#xD;&#x9;REASON_UNVISIT = @REASON_UNVISIT,&#xD;&#x9;RECEIPT_NO = case when isnull(RECEIPT_NO,&#39;&#39;) = &#39;&#39; then @RECEIPT_NO else RECEIPT_NO end,&#xD;&#x9;QTY_CUSTOMER_PAID = @QTY_CUSTOMER_PAID,&#xD;&#x9;TOT_COLL_FEE = @TOT_COLL_FEE,&#xD;&#x9;TOT_PENALTY_VALUE = @TOT_PENALTY_VALUE,&#xD;&#x9;TOT_INSTALL_BILL = @TOT_INSTALL_BILL,&#xD;&#x9;TOT_BILL = @TOT_BILL,&#xD;&#x9;TOT_PAID_COLL_FEE = @TOT_PAID_COLL_FEE,&#xD;&#x9;TOT_PAID_PENALTY = @TOT_PAID_PENALTY,&#xD;&#x9;TOT_PAID_INSTALL_BILL = @TOT_PAID_INSTALL_BILL,&#xD;&#x9;TOT_UTJ_PLUS_VALUE = @TOT_UTJ_PLUS_VALUE,&#xD;&#x9;TOT_PAID_CASH = @TOT_PAID_CASH,&#xD;&#x9;TOT_UTJ_USAGE_VALUE = @TOT_UTJ_USAGE_VALUE,&#xD;&#x9;BILL_STS = @BILL_STS,&#xD;&#x9;FLAG_SEND = case&#xD;&#x9;&#x9;when FLAG_SEND = &#39;2&#39; then &#39;2&#39;&#xD;&#x9;&#x9;when FLAG_SEND = &#39;1&#39; AND @visitCode = &#39;&#39; then &#39;1&#39;&#xD;&#x9;&#x9;else @FLAG_SEND&#xD;&#x9;end,&#xD;&#x9;FLAG_SEND_DATE = @FLAG_SEND_DATE,&#xD;&#x9;UPDATED_DATE = &#x9;GETDATE(),&#xD;&#x9;UPDATED_BY = @UPDATED_BY&#xD;WHERE&#xD;&#x9;COMPANY_CODE = @companycode  AND&#xD;&#x9;BRANCH_CODE = @branchcode AND&#xD;&#x9;DKH_NO = @dkhno AND&#xD;&#x9;GROUP_CODE = @groupcode</sql>
