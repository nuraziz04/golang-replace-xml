      <query id="getCountLovNomorKontrakIndex" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total from (&#xD;SELECT DISTINCT companyCode, aggrNo, customerFullname, address, villageName, isnull(dayOverdue, 0) dayOverdue  FROM (&#xD;&#x9;SELECT DISTINCT companyCode, branchCode, dsoCode, productCode, collCode, aggrNo, customerFullname, address, villageName, dayOverdue FROM (&#xD;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;SP.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;SP.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;VB.DSO_CODE dsoCode,&#xD;&#x9;&#x9;&#x9;SP.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;SP.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;SP.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;AGG.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CUS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;K&#39; THEN CE.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;END AS address,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;K&#39; THEN OV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN AGG.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END AS villageName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = SP.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = SP.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;&#x9;AGG.WO_DATE woDate,&#xD;&#x9;&#x9;&#x9;AGG.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;AGG.SUB_STS subSts&#xD;&#x9;&#x9;FROM TRX_SP SP (nolock)&#xD;&#x9;&#x9;LEFT JOIN TRX_AGGR AGG (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = AGG.COMPANY_CODE AND SP.AGGR_NO = AGG.AGGR_NO&#xD;&#x9;&#x9;LEFT JOIN getViewBranch() VB&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = VB.COMPANY_CODE AND SP.BRANCH_CODE = VB.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER CUS (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = CUS.COMPANY_CODE AND AGG.CUSTOMER_CODE = CUS.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = IV.COMPANY_CODE AND CUS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = DV.COMPANY_CODE AND CUS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CE (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = CE.COMPANY_CODE AND AGG.CUSTOMER_CODE = CE.CUSTOMER_CODE AND CE.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE OV (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = OV.COMPANY_CODE AND OV.VILLAGE_CODE = CE.OFFICE_VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CEB (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = CEB.COMPANY_CODE AND AGG.CUSTOMER_CODE = CEB.CUSTOMER_CODE AND CEB.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;&#x9;ON SP.COMPANY_CODE = BV.COMPANY_CODE AND BV.VILLAGE_CODE = CEB.OFFICE_VILLAGE_CODE&#xD;&#x9;&#x9;WHERE SP.PRODUCT_CODE = &#39;PC02&#39;&#xD;&#x9;) a&#xD;&#xD;&#x9;UNION ALL&#xD;&#xD;&#x9;SELECT DISTINCT companyCode, branchCode, dsoCode, productCode, collCode, aggrNo, customerFullname, address, villageName, dayOverdue FROM (&#xD;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;VB.DSO_CODE dsoCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;A.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CUS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; THEN CE.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;END AS address,&#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;K&#39; THEN OV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END AS villageName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT TOP 1 dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;&#x9;A.WO_DATE woDate,&#xD;&#x9;&#x9;&#x9;A.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;A.SUB_STS subSts&#xD;&#x9;&#x9;FROM TRX_AGGR A (nolock)&#xD;&#x9;&#x9;LEFT JOIN TRX_AGGR_SCHEDULE B (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = B.COMPANY_CODE AND A.AGGR_NO = B.AGGR_NO&#xD;&#x9;&#x9;LEFT JOIN getViewBranch() VB&#xD;&#x9;&#x9;ON A.COMPANY_CODE = VB.COMPANY_CODE AND A.BRANCH_CODE = VB.BRANCH_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER CUS (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = CUS.COMPANY_CODE AND A.CUSTOMER_CODE = CUS.CUSTOMER_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = IV.COMPANY_CODE AND CUS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = DV.COMPANY_CODE AND CUS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CE (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = CE.COMPANY_CODE AND A.CUSTOMER_CODE = CE.CUSTOMER_CODE AND CE.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE OV (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = OV.COMPANY_CODE AND OV.VILLAGE_CODE = CE.OFFICE_VILLAGE_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CEB (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = CEB.COMPANY_CODE AND A.CUSTOMER_CODE = CEB.CUSTOMER_CODE AND CEB.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = BV.COMPANY_CODE AND BV.VILLAGE_CODE = CEB.OFFICE_VILLAGE_CODE&#xD;&#x9;&#x9;WHERE B.PAYMENT_STS = &#39;UNPAID&#39; AND B.COLL_DUE_DATE &lt;= GETDATE() AND A.PRODUCT_CODE = &#39;PC02&#39; AND  NOT EXISTS (SELECT 1 FROM TRX_SP (nolock) WHERE EXPIRED_DATE &gt; GETDATE() AND SP_STS != &#39;CANC&#39; AND AGGR_NO=A.AGGR_NO)&#xD;&#x9;) a WHERE arSts in (&#39;AR&#39;) and subSts not in (&#39;TP&#39;,&#39;SU&#39;,&#39;CM&#39;,&#39;DL&#39;) and (ISNULL(woDate, &#39;&#39;) = &#39;&#39; OR woDate = &#39;1900-01-01&#39;) AND dayOverdue &gt; 1&#xD;) a WHERE :search :filter&#xD;) b</sql>
