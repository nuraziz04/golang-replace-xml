;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) villageName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) subdistrictName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) cityName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) provinceName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) zipCode&#xD;&#x9;FROM TRX_TU_DEPOSIT A&#xD;&#x9;LEFT JOIN getViewBranch() B&#xD;&#x9;ON A.BRANCH_CODE = B.BRANCH_CODE&#xD;&#x9;JOIN TRX_AGGR C&#xD;&#x9;ON A.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN MST_CUSTOMER D&#xD;&#x9;ON C.CUSTOMER_CODE = D.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV&#xD;&#x9;ON D.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB&#xD;&#x9;ON IV.SUBDISTRICT_CODE = ISUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV&#xD;&#x9;ON D.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB&#xD;&#x9;ON DV.SUBDISTRICT_CODE = DSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE E&#xD;&#x9;ON C.CUSTOMER_CODE = E.CUSTOMER_CODE AND E.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE KV&#xD;&#x9;ON E.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB&#xD;&#x9;ON KV.SUBDISTRICT_CODE = KSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F&#xD;&#x9;ON C.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE BV&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB&#xD;&#x9;ON BV.SUBDISTRICT_CODE = BSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY BC&#xD;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE BP&#xD;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#xD;&#xD;&#x9;UNION ALL&#xD;&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;B.DSO_CODE dsoCode,&#xD;&#x9;&#x9;C.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;C.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;A.DAY_OVERDUE daysOverudue,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN D.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN D.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN E.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) address,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN D.ID_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN D.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN E.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN F.OFFICE_RT&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) rt,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN D.ID_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN D.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN E.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN F.OFFICE_RW&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) rw,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) villageName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) subdistrictName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) cityName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) provinceName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) zipCode&#xD;&#x9;FROM TRX_STTU A&#xD;&#x9;LEFT JOIN getViewBranch() B&#xD;&#x9;ON A.BRANCH_CODE = B.BRANCH_CODE&#xD;&#x9;JOIN TRX_AGGR C&#xD;&#x9;ON A.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN MST_CUSTOMER D&#xD;&#x9;ON C.CUSTOMER_CODE = D.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV&#xD;&#x9;ON D.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB&#xD;&#x9;ON IV.SUBDISTRICT_CODE = ISUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV&#xD;&#x9;ON D.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB&#xD;&#x9;ON DV.SUBDISTRICT_CODE = DSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE E&#xD;&#x9;ON C.CUSTOMER_CODE = E.CUSTOMER_CODE AND E.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE KV&#xD;&#x9;ON E.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB&#xD;&#x9;ON KV.SUBDISTRICT_CODE = KSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F&#xD;&#x9;ON C.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE BV&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB&#xD;&#x9;ON BV.SUBDISTRICT_CODE = BSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY BC&#xD;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE BP&#xD;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;WHERE A.STTU_STS = &#39;PROC&#39;&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
