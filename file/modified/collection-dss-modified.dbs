      <query id="getCountTrxSttu" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT &#xD;        A.COMPANY_CODE companyCode,&#xD;        A.BRANCH_CODE branchCode,&#xD;        A.STTU_NO sttuNo,&#xD;        A.STTU_DATE sttuDate,&#xD;        A.EXPIRED_DATE expiredDate,&#xD;        A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;F.CUSTOMER_FULLNAME customerName,&#xD;        A.STTU_STS sttuSts,&#xD;&#x9;&#x9;I.PARAM_DESCRIP sttuStsName,&#xD;        A.STTU_STS_DATE sttuStsDate,&#xD;        A.CUSTOMER_CODE customerCode,&#xD;        A.VILLAGE_CODE villageCode,&#xD;&#x9;&#x9;B.VILLAGE_NAME villageName,&#xD;&#x9;&#x9;B.ZIPCODE zipCode,&#xD;&#x9;&#x9;C.SUBDISTRICT_NAME subdistrictName,&#xD;&#x9;&#x9;D.CITY_NAME cityName,&#xD;&#x9;&#x9;E.PROVINCE_NAME provinceName,&#xD;&#x9;&#x9;G.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; THEN (SELECT ID_ADDRESS FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT DOMICILE_ADDRESS FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;P&#39; THEN (SELECT OFFICE_ADDRESS FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;C&#39; THEN (SELECT OFFICE_ADDRESS FROM MST_CUSTOMER_CO X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;B&#39; THEN (SELECT OFFICE_ADDRESS FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; OR F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT HP_NO1 FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;P&#39; THEN (SELECT HP_NO FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;C&#39; THEN (SELECT HP_NO1 FROM MST_CUSTOMER_CO X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;B&#39; THEN (SELECT HP_NO FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; OR F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT PHONE_AREA1 FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;P&#39; THEN (SELECT PHONE_AREA1 FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;C&#39; THEN (SELECT PHONE_AREA1 FROM MST_CUSTOMER_CO X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;B&#39; THEN (SELECT PHONE_AREA1 FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneArea1,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; OR F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT PHONE_NO1 FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;P&#39; THEN (SELECT PHONE_NO1 FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;C&#39; THEN (SELECT PHONE_NO1 FROM MST_CUSTOMER_CO X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;B&#39; THEN (SELECT PHONE_NO1 FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneNo1,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; OR F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT PHONE_AREA2 FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneArea2,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; OR F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT PHONE_NO2 FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneNo2,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; THEN (SELECT ID_RT FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT DOMICILE_RT FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;P&#39; THEN (SELECT OFFICE_RT FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;C&#39; THEN (SELECT OFFICE_RT FROM MST_CUSTOMER_CO X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;B&#39; THEN (SELECT OFFICE_RT FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rt,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;I&#39; THEN (SELECT ID_RW FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (SELECT DOMICILE_RW FROM MST_CUSTOMER X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;P&#39; THEN (SELECT OFFICE_RW FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;K&#39; AND F.CUSTOMER_TYPE = &#39;C&#39; THEN (SELECT OFFICE_RW FROM MST_CUSTOMER_CO X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN F.COLL_ADDRESS_TYPE = &#39;B&#39; THEN (SELECT OFFICE_RW FROM MST_CUSTOMER_EMPLOYEE X (NOLOCK) WHERE X.CUSTOMER_CODE = A.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rw,&#xD;        A.PRINTED_DATE printedDate,&#xD;        A.REPRINT_COUNT printCount,&#xD;        A.AGENT_TYPE agentType,&#xD;&#x9;&#x9;H.PARAM_DESCRIP agentTypeName,&#xD;        A.PIC1_CODE pic1Code,&#xD;&#x9;&#x9;J.FULLNAME pic1Name,&#xD;&#x9;&#x9;J.USERNAME collectorUsername,&#xD;        A.PIC1_POSITION pic1Position,&#xD;&#x9;&#x9;K.POSITION_NAME position1Name,&#xD;        A.PIC2_CODE pic2Code,&#xD;&#x9;&#x9;L.FULLNAME pic2name,&#xD;        A.PIC2_POSITION pic2Position,&#xD;&#x9;&#x9;M.POSITION_NAME position2Name,&#xD;        A.PIC_HEAD picHead,&#xD;&#x9;&#x9;T.FULLNAME picHeadName,&#xD;        A.PIC_HEAD_POSITION picHeadPosition,&#xD;&#x9;&#x9;U.POSITION_NAME positionHeadName,&#xD;        A.TU_REASON tuReason,&#xD;&#x9;&#x9;V.PARAM_DESCRIP tuReasonName,&#xD;&#x9;&#x9;N.BRAND_CODE brandCode,&#xD;&#x9;&#x9;O.BRAND_NAME brandName,&#xD;&#x9;&#x9;N.TYPE_CODE typeCode,&#xD;&#x9;&#x9;P.TYPE_NAME typeName,&#xD;&#x9;&#x9;N.MODEL_CODE modelCode,&#xD;&#x9;&#x9;Q.MODEL_NAME modelName,&#xD;&#x9;&#x9;P.KIND_CODE kindCode,&#xD;&#x9;&#x9;R.KIND_NAME kindName,&#xD;&#x9;&#x9;S.COLOR_NAME colorName,&#xD;&#x9;&#x9;N.YEAR_MFG yearMfg,&#xD;&#x9;&#x9;N.CHASSIS_NO chassisNo,&#xD;&#x9;&#x9;N.MACHINE_NO machineNo,&#xD;&#x9;&#x9;N.NOPOL nopol,&#xD;&#x9;&#x9;N.BPKB_NAME stnkName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN MIN(INST_NO) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE MIN(INST_NO)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;END instNo&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as instFrom,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;isnull(SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID)), 0)&#xD;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND&#xD;&#x9;&#x9;&#x9;&#x9;INST_NO &gt;= (SELECT TOP 1 instNo FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;) a) &#xD;&#x9;&#x9;&#x9;&#x9;AND&#xD;&#x9;&#x9;&#x9;&#x9;INST_NO &lt;= (SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE())&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END RESULT)&#xD;&#x9;&#x9;&#x9;&#x9;AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#xD;&#x9;&#x9;) as tunggakanAngsuran,&#xD;&#x9;&#x9;W.EXT_COLL_NAME extCollName1,&#xD;&#x9;&#x9;F.AR_STS arSts,&#xD;&#x9;&#x9;Y.NOTES arStsName,&#xD;&#x9;&#x9;O.CATEGORY_CODE categoryCode,&#xD;&#x9;&#x9;Z.PARAM_DESCRIP categoryName,&#xD;&#x9;&#x9;A.APPV_DATE appvDate,&#xD;&#x9;&#x9;A.CHRONOLOGICAL chronological,&#xD;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(PRINCIPAL_VALUE-PRINCIPAL_PAID) balPrincipalValue&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as balPrincipalValue,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM(INT_VALUE-INT_PAID) balPrincipalValue&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;) as balIntValue,&#xD;&#x9;&#x9;IIF(A.PIC_PRODUCT_CODE=&#39;&#39;,F.PRODUCT_CODE,A.PIC_PRODUCT_CODE) picProductCode,&#xD;&#x9;&#x9;A.PIC_BUCKET_TYPE picBucketType,&#xD;&#x9;&#x9;A.PIC1_BUCKET_CODE pic1BucketCode,&#xD;&#x9;&#x9;A.PIC2_BUCKET_CODE pic2BucketCode,&#xD;&#x9;&#x9;A.EXT_COLL_CODE extCollCode,&#xD;&#x9;&#x9;A.DAY_OVERDUE daysOverdue,&#xD;&#x9;&#x9;DSO.DSO_CODE dsoCode,&#xD;        F.BRANCH_CODE branchCodeAggr&#xD;    FROM TRX_STTU A (NOLOCK)&#xD;&#x9;LEFT JOIN MST_VILLAGE B (NOLOCK)&#xD;&#x9;ON A.VILLAGE_CODE = B.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT C (NOLOCK)&#xD;&#x9;ON B.SUBDISTRICT_CODE = C.SUBDISTRICT_CODE &#xD;&#x9;LEFT JOIN MST_CITY D (NOLOCK)&#xD;&#x9;ON B.CITY_CODE = D.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE E (NOLOCK)&#xD;&#x9;ON B.PROVINCE_CODE = E.PROVINCE_CODE&#xD;&#x9;LEFT JOIN TRX_AGGR F (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = F.AGGR_NO AND A.COMPANY_CODE=F.COMPANY_CODE&#xD;&#x9;LEFT JOIN TRX_APPL G (NOLOCK)&#xD;&#x9;ON F.APPL_NO = G.APPL_NO AND F.COMPANY_CODE=G.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM H (NOLOCK)&#xD;&#x9;ON A.AGENT_TYPE = H.PARAM_CODE AND H.CONDITION = &#39;AGENT_TYPE&#39; AND H.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM I (NOLOCK)&#xD;&#x9;ON A.STTU_STS = I.PARAM_CODE AND I.CONDITION = &#39;STTU_STS&#39; AND I.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_EMPLOYEE J (NOLOCK)&#xD;&#x9;ON A.PIC1_CODE = J.NIK&#xD;&#x9;LEFT JOIN MST_POSITION K (NOLOCK)&#xD;&#x9;ON A.PIC1_POSITION = K.POSITION_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE L (NOLOCK)&#xD;&#x9;ON A.PIC2_CODE = L.NIK&#xD;&#x9;LEFT JOIN MST_POSITION M (NOLOCK)&#xD;&#x9;ON A.PIC2_POSITION = M.POSITION_CODE&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET N (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = N.AGGR_NO&#xD;&#x9;LEFT JOIN MST_BRAND O (NOLOCK)&#xD;&#x9;ON N.BRAND_CODE = O.BRAND_CODE&#xD;&#x9;LEFT JOIN MST_TYPE P (NOLOCK)&#xD;&#x9;ON N.TYPE_CODE = P.TYPE_CODE&#xD;&#x9;LEFT JOIN MST_MODEL Q (NOLOCK)&#xD;&#x9;ON N.MODEL_CODE = Q.MODEL_CODE&#xD;&#x9;LEFT JOIN MST_KIND R (NOLOCK)&#xD;&#x9;ON P.KIND_CODE = R.KIND_CODE&#xD;&#x9;LEFT JOIN MST_COLOR S (NOLOCK)&#xD;&#x9;ON N.COLOR_CODE = S.COLOR_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE T (NOLOCK)&#xD;&#x9;ON A.PIC_HEAD = T.NIK&#xD;&#x9;LEFT JOIN MST_POSITION U (NOLOCK)&#xD;&#x9;ON A.PIC_HEAD_POSITION = U.POSITION_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM V (NOLOCK)&#xD;&#x9;ON A.TU_REASON = V.PARAM_CODE AND V.CONDITION = &#39;TU_REASON&#39; AND V.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_EXT_COLL W (NOLOCK)&#xD;&#x9;ON A.EXT_COLL_CODE = W.EXT_COLL_CODE&#xD;&#x9;LEFT JOIN MST_AR_STS Y (NOLOCK)&#xD;&#x9;ON F.AR_STS = Y.AR_STS AND ISNULL(Y.SUB_STS, &#39;&#39;) = ISNULL(F.SUB_STS, &#39;&#39;)&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM Z (NOLOCK)&#xD;&#x9;ON O.CATEGORY_CODE = Z.PARAM_CODE AND Z.CONDITION = &#39;CATEGORY_CODE&#39; AND Z.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_BRANCH DSO (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = DSO.COMPANY_CODE AND A.BRANCH_CODE = DSO.BRANCH_CODE&#xD;) a WHERE :search :filter</sql>
