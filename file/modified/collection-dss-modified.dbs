      <query id="getDetailMstCollAreaDtl" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @companyCode varchar(10)&#x9;= :companyCode&#xD;declare @branchCode varchar(10)&#x9;&#x9;= :branchCode&#xD;declare @productCode varchar(10)&#x9;= :productCode&#xD;declare @bucketCode varchar(10)&#x9;&#x9;= :bucketCode&#xD;declare @EffDate varchar(10)&#x9;&#x9;= :effectiveDate&#xD;&#xD;select &#xD;&#x9;company_code companyCode&#xD;&#x9;, branch_code branchCode&#xD;&#x9;, branch_name branchName&#xD;&#x9;, product_code productCode&#xD;&#x9;, product_name productName&#xD;&#x9;, bucket_code bucketCode&#xD;&#x9;, bucket_name bucketName&#xD;&#x9;, village_code villageCode&#xD;&#x9;, village_name villageName&#xD;&#x9;, subdistrict_name subdistrictName&#xD;&#x9;, SUM(TOT_AGGR) as totalAggr&#xD;&#x9;, qty_aggr_no qtyAggrNo&#xD;&#x9;, effective_date effectiveDate&#xD;&#x9;, isnull(coll_code, &#39;&#39;) collCode&#xD;&#x9;, fullname_coll fullnameColl&#xD;&#x9;, isnull(coll_day_code, &#39;&#39;) collCodeDay&#xD;&#x9;, description collDayName&#xD;from ( &#xD;&#x9;select distinct&#xD;&#x9;&#x9;c.company_code,&#xD;&#x9;&#x9;br.branch_code,&#xD;&#x9;&#x9;br.branch_name,&#xD;&#x9;&#x9;c.village_code,&#xD;&#x9;&#x9;c.village_name,&#xD;&#x9;&#x9;k.PARAM_CODE as product_code,&#xD;&#x9;&#x9;k.PARAM_DESCRIP as product_name,&#xD;&#x9;&#x9;&#39;&#39; CLUSTER,&#xD;&#x9;&#x9;ISNULL(l.TOT_AGGR,0) AS TOT_AGGR,&#xD;&#x9;&#x9;e.subdistrict_name,&#xD;&#x9;&#x9;h.bucket_code bucket_code,&#xD;&#x9;&#x9;buc.DESCRIPTION bucket_name,&#xD;&#x9;&#x9;h.effective_date effective_date,&#xD;&#x9;&#x9;ISNULL(h.qty_aggr_no, 0) qty_aggr_no,&#xD;&#x9;&#x9;h.coll_code coll_code,&#xD;&#x9;&#x9;em.fullname fullname_coll,&#xD;&#x9;&#x9;h.coll_day_code coll_day_code,&#xD;&#x9;&#x9;cdh.description description&#xD;&#x9;from [dbo].[mst_village] c (nolock)&#xD;&#xD;&#x9;left join [dbo].[mst_branch] br (nolock)&#xD;&#x9;on c.company_code = br.company_code&#xD;&#x9;and br.branch_code = @branchCode&#xD;&#xD;&#x9;join dbo.getviewbranch() d&#xD;&#x9;on d.company_code = br.company_code&#xD;&#x9;and (&#xD;&#x9;case &#xD;&#x9;when br.structure_type = &#39;ST05&#39; then d.branch_code&#xD;&#x9;when br.structure_type = &#39;ST04&#39; then d.dso_code end&#xD;&#x9;) = br.branch_code&#xD;&#xD;&#x9;left join [dbo].[trx_aggr] a (nolock)&#xD;&#x9;on a.company_code = c.company_code&#xD;&#x9;and a.branch_code = d.branch_code&#xD;&#x9;and a.coll_village_code = c.VILLAGE_CODE&#xD;&#x9;and (a.wo_date is null or a.wo_date = &#39;1900-01-01&#39;)&#xD;&#x9;and a.ar_sts in (&#39;ar&#39;,&#39;bd&#39;)&#xD;&#xD;&#x9;left join [dbo].[MST_TOTAL_AGGR] l (nolock)&#xD;&#x9;on l.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and l.BRANCH_CODE = d.BRANCH_CODE&#xD;&#x9;and l.VILLAGE_CODE = c.VILLAGE_CODE&#xD;&#xD;&#x9;left join [dbo].[MST_BRANCH] j (nolock)&#xD;&#x9;on j.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and j.BRANCH_CODE = d.BRANCH_CODE&#xD;&#xD;&#x9;left join [dbo].[MST_GLOBAL_PARAM] k (nolock)&#xD;&#x9;on k.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and k.CONDITION = &#39;PRODUCT_CODE&#39;&#xD;&#x9;and k.PARAM_CODE = @productCode&#xD;&#xD;&#x9;left join [dbo].[mst_subdistrict] e (nolock)&#xD;&#x9;on e.company_code = c.company_code&#xD;&#x9;and e.SUBDISTRICT_CODE = c.subdistrict_code&#xD;&#xD;&#x9;left join [dbo].[mst_cover_area_dtl] f (nolock)&#xD;&#x9;on f.company_code = c.COMPANY_CODE&#xD;&#x9;and f.village_code = c.VILLAGE_CODE&#xD;&#x9;and f.BRANCH_CODE = d.BRANCH_CODE&#xD;&#x9;and f.PRODUCT_CODE = @productCode&#xD;&#xD;&#x9;left join [dbo].[MST_COVER_AREA_HDR] g (nolock)&#xD;&#x9;on g.COMPANY_CODE = f.COMPANY_CODE&#xD;&#x9;and g.BRANCH_CODE = f.BRANCH_CODE&#xD;&#x9;and g.FLAG_ACTIVE = 1&#xD;&#xD;&#x9;left join MST_COLL_AREA_DTL h (nolock)&#xD;&#x9;on h.COMPANY_CODE = c.COMPANY_CODE&#xD;&#x9;and @branchCode = h.BRANCH_CODE&#xD;&#x9;and h.PRODUCT_CODE = @productCode&#xD;&#x9;and h.BUCKET_CODE = @bucketCode&#xD;&#x9;and h.EFFECTIVE_DATE = @EffDate&#xD;&#x9;and h.VILLAGE_CODE = c.VILLAGE_CODE&#xD;&#xD;&#x9;left join MST_COLL_AREA_HDR i (nolock)&#xD;&#x9;on i.COMPANY_CODE=h.COMPANY_CODE&#xD;&#x9;and i.BRANCH_CODE = h.BRANCH_CODE&#xD;&#x9;and i.BUCKET_CODE=h.BUCKET_CODE&#xD;&#x9;and i.EFFECTIVE_DATE = h.EFFECTIVE_DATE&#xD;&#x9;and i.PRODUCT_CODE = h.PRODUCT_CODE&#xD;&#xD;&#x9;left join mst_bucket buc (nolock)&#xD;&#x9;on h.bucket_code = buc.bucket_code and buc.bucket_type = &#39;fcol&#39; and h.product_code = buc.product_code&#xD;&#xD;&#x9;left join mst_employee em (nolock)&#xD;&#x9;on h.coll_code = em.nik&#xD;&#xD;&#x9;left join mst_coll_day_hdr cdh (nolock)&#xD;&#x9;on h.coll_day_code = cdh.coll_day_code and h.product_code = cdh.product_code&#xD;&#xD;&#x9;where (a.coll_village_code is not null or (f.village_code is not null and g.branch_code is not null) or h.VILLAGE_CODE is not null) &#xD;&#x9;and c.company_code = @companyCode&#xD;) a&#xD;group by &#xD;&#x9;COMPANY_CODE&#xD;&#x9;, branch_code&#xD;&#x9;, branch_name&#xD;&#x9;, village_code&#xD;&#x9;, village_name&#xD;&#x9;, product_code&#xD;&#x9;, product_name&#xD;&#x9;, subdistrict_name&#xD;&#x9;, bucket_code&#xD;&#x9;, bucket_name&#xD;&#x9;, effective_date&#xD;&#x9;, qty_aggr_no&#xD;&#x9;, coll_code&#xD;&#x9;, fullname_coll&#xD;&#x9;, coll_day_code&#xD;&#x9;, description&#xD;order by village_code asc</sql>
