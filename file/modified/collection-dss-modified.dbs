      <query id="checkValidasiPenyelesaianPerpanjangan" returnUpdatedRowCount="false" useConfig="UI_SUBMIT">
         <sql>DECLARE @COMPANY_CODE VARCHAR(10) = :companyCode&#xD;DECLARE @TU_DEPOSIT_NO VARCHAR(50) = :tuDepositNo&#xD;&#xD;select&#xD;&#x9;(&#xD;&#x9;&#x9;select &#xD;&#x9;&#x9;&#x9;VALUES_PARAM&#xD;&#x9;&#x9;from MST_GLOBAL_PARAM gp&#xD;&#x9;&#x9;left join trx_tu_deposit x&#xD;&#x9;&#x9;on gp.CHAR_VALUES = x.PRODUCT_CODE &#xD;&#x9;&#x9;where gp.CONDITION = &#39;DEPOSIT_EXPIRED&#39; and gp.FLAG_ACTIVE = &#39;1&#39; and x.tu_deposit_no = @TU_DEPOSIT_NO&#xD;&#xD;&#x9;)  jumlah,&#xD;&#x9;IIF(count(sol.TU_DEPOSIT_NO) &gt;=&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;select &#xD;&#x9;&#x9;&#x9;&#x9;VALUES_PARAM&#xD;&#x9;&#x9;&#x9;from MST_GLOBAL_PARAM gp&#xD;&#x9;&#x9;&#x9;left join trx_tu_deposit x&#xD;&#x9;&#x9;&#x9;on gp.CHAR_VALUES = x.PRODUCT_CODE &#xD;&#x9;&#x9;&#x9;where gp.CONDITION = &#39;DEPOSIT_EXPIRED&#39; and gp.FLAG_ACTIVE = &#39;1&#39; and x.tu_deposit_no = @TU_DEPOSIT_NO&#xD;&#xD;&#x9;&#x9;) &#xD;&#x9;, &#39;1&#39;, &#39;0&#39;) as result&#xD;from trx_tu_solving sol&#xD;left join trx_tu_deposit dep&#xD;on sol.company_code = dep.company_code and sol.tu_deposit_no = dep.tu_deposit_no&#x9;&#xD;where dep.company_code = @COMPANY_CODE and dep.tu_deposit_no = @TU_DEPOSIT_NO and sol.tu_solving_type  = &#39;1&#39; and sol.appv_sts = &#39;APPD&#39;</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="result" name="result" xsdType="string"></element>
            <element column="jumlah" name="jumlah" xsdType="string"></element>
         </result>
