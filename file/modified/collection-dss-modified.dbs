      <query id="getCountLovNoPembayaran" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(*) as total FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.TU_DEPOSIT_NO reffNo,&#xD;&#x9;&#x9;A.VOUCHER_NO voucherNo,&#xD;&#x9;&#x9;B.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;C.PARAM_DESCRIP status,&#xD;&#x9;&#x9;A.VOUCHER_DATE voucherDate,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT COUNT(*) as total&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_ASSET WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as totalUnit,&#xD;&#x9;&#x9;&#39;&#39; as sellNo,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;D.DSO_CODE dsoCode&#xD;&#x9;FROM TRX_TU_DEPOSIT A&#xD;&#x9;LEFT JOIN TRX_AGGR B&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM C&#xD;&#x9;ON A.SOLVING_TYPE = C.PARAM_CODE AND C.CONDITION = &#39;SOLVING_TYPE&#39; AND C.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_BRANCH D&#xD;&#x9;ON A.COMPANY_CODE = D.COMPANY_CODE AND A.BRANCH_CODE = D.BRANCH_CODE&#xD;&#x9;WHERE A.SOLVING_TYPE IN (&#39;4&#39;,&#39;5&#39;,&#39;6&#39;) AND (A.VOUCHER_NO != &#39;&#39; OR A.VOUCHER_NO != NULL) AND A.VOUCHER_NO NOT IN (SELECT VOUCHER_NO FROM TRX_TU_OUT_HDR)&#xD;&#x9;UNION ALL&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.SELL_NO reffNo,&#xD;&#x9;&#x9;A.VOUCHER_NO voucherNo,&#xD;&#x9;&#x9;B.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#39;Penjualan Tarik Unit&#39; as status,&#xD;&#x9;&#x9;A.VOUCHER_DATE voucherDate,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT COUNT(*) as total &#xD;&#x9;&#x9;&#x9;FROM TRX_SELL_UNIT WHERE SELL_NO = A.SELL_NO AND FLAG_ACTIVE = &#39;1&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;) as totalUnit,&#xD;&#x9;&#x9;A.SELL_NO sellNo,&#xD;&#x9;&#x9;&#39;&#39; as aggrNo,&#xD;&#x9;&#x9;C.DSO_CODE dsoCode&#xD;&#x9;FROM TRX_SELL_HDR A&#xD;&#x9;LEFT JOIN TRX_SELL_BIDDER B&#xD;&#x9;ON A.SELL_NO = B.SELL_NO AND A.COMPANY_CODE = B.COMPANY_CODE &#xD;&#x9;LEFT JOIN MST_BRANCH C&#xD;&#x9;ON A.COMPANY_CODE = C.COMPANY_CODE AND A.BRANCH_CODE = C.BRANCH_CODE&#xD;&#x9;WHERE B.WINNER_STS = &#39;1&#39; AND A.SELL_STS = &#39;CLOS&#39; AND (A.VOUCHER_NO != &#39;&#39; OR A.VOUCHER_NO != NULL) AND A.VOUCHER_NO NOT IN (SELECT VOUCHER_NO FROM TRX_TU_OUT_HDR)&#xD;) t WHERE :search :filter</sql>
