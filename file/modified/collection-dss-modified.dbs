      <query id="listExpiredApprovalUtj" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @EODDATE date = :eodDate&#xD;&#xD;select companyCode,branchCode,aggrNo,dkhNo,trxDate,hiePositionVersion,subject from (&#x9;&#xD;&#x9;select&#xD;&#x9;&#x9;a.company_code companyCode,&#xD;&#x9;&#x9;a.branch_code branchCode,&#xD;&#x9;&#x9;a.AGGR_NO aggrNo,&#xD;&#x9;&#x9;a.DKH_NO dkhNo,&#xD;&#x9;&#x9;convert(date,a.Flag_send_Date) trxDate,&#xD;&#x9;&#x9;c.hie_position_version hiePositionVersion,&#xD;&#x9;&#x9;c.subject&#xD;&#x9;from trx_dkh_dtl a (nolock)&#xD;&#x9;join&#xD;&#x9;&#x9;( &#xD;&#x9;&#x9;&#x9;select distinct&#xD;&#x9;&#x9;&#x9;&#x9;company_code companyCode,&#xD;&#x9;&#x9;&#x9;&#x9;trx_code trxCode,&#xD;&#x9;&#x9;&#x9;&#x9;max(trx_seq) over(partition by company_code,trx_code) trxMax&#xD;&#x9;&#x9;&#x9;from trx_approval&#xD;&#x9;&#x9;&#x9;where menu_code = &#39;CLT112&#39; --AND subject =&#39;Approval Pemakaian UTJ&#39;&#xD;&#x9;&#x9;) b&#xD;&#x9;&#x9;on a.company_code = b.companyCode &#xD;&#x9;&#x9;and a.aggr_no= b.trxCode&#xD;&#x9;join trx_approval c (nolock)&#xD;&#x9;on c.company_code = b.companyCode and c.trx_code = b.trxCode and c.trx_seq = b.trxMax and c.decision_sts = &#39;APPR&#39;&#xD;&#x9;and a.company_code = c.company_code and a.aggr_no = c.trx_code&#xD;&#x9;where a.flag_send=2 and A.UTJ_APPV_STS =&#39;APPR&#39; and&#xD;&#x9;dateAdd(day, +3, cast(A.Flag_send_Date as date)) &lt;= DATEADD(dd,+1, @EODDATE)&#xD;&#x9;and a.company_code = :companyCode and a.branch_code = :branchCode&#xD;) a order by branchCode</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="companyCode" name="companyCode" xsdType="string"></element>
            <element column="branchCode" name="branchCode" xsdType="string"></element>
            <element column="aggrNo" name="aggrNo" xsdType="string"></element>
            <element column="dkhNo" name="dkhNo" xsdType="string"></element>
            <element column="trxDate" name="trxDate" xsdType="string"></element>
            <element column="hiePositionVersion" name="hiePositionVersion" xsdType="string"></element>
            <element column="subject" name="subject" xsdType="string"></element>
         </result>
         <param name="companyCode" sqlType="STRING"></param>
         <param name="branchCode" sqlType="STRING"></param>
         <param name="eodDate" sqlType="STRING"></param>
      </query>
