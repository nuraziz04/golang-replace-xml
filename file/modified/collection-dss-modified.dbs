      <query id="getCountTrxTuSolving" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(*) as total FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;BR.BRANCH_NAME branchName,&#xD;&#x9;&#x9;A.TU_DEPOSIT_NO tuDepositNo,&#xD;&#x9;&#x9;A.TU_DEPOSIT_DATE tuDepositDate,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;B.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;B.AR_STS arSts,&#xD;&#x9;&#x9;C.TRX_NAME arStsName,&#xD;&#x9;&#x9;D.TU_SOLVING_DATE tuSolvingDate,&#xD;&#x9;&#x9;D.TU_SOLVING_TYPE tuSolvingType,&#xD;&#x9;&#x9;E.PARAM_DESCRIP tuSolvingTypeName,&#xD;&#x9;&#x9;D.NOTES notes,&#xD;&#x9;&#x9;D.INVENTORY_DATE_OLD inventoryDateOld,&#xD;&#x9;&#x9;D.INVENTORY_DATE_NEW inventoryDateNew,&#xD;&#x9;&#x9;D.APPV_STS appvSts,&#xD;&#x9;&#x9;F.PARAM_DESCRIP appvStsName,&#xD;&#x9;&#x9;D.APPV_DATE appvDate,&#xD;&#x9;&#x9;(SELECT MIN(DUE_DATE) FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE) as dueDate,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;ISNULL(MIN(INST_NO), &#39;0&#39;) instNo&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;) as instFrom,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1 ISNULL(dayOverdue, &#39;0&#39;) dayOverdue FROM (&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;DAY_OVERDUE dayOverdue&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;GROUP BY INST_NO, DAY_OVERDUE&#xD;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;) as dayOverdue,&#xD;&#x9;&#x9;B.INST_VALUE nilaiAngsuran,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT total FROM (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID)) total&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) a)&#xD;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;) as tagihanAngsuran,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;isnull(SUM(COLL_FEE-COLL_FEE_PAID), 0)&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;) as biayaTagih,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PINALTY_VALUE-PINALTY_PAID), 0) &#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;) as tagihanDenda,&#xD;&#xD;&#x9;&#x9;A.PENALTY_TU_VALUE penaltyTuValue,&#xD;&#x9;&#x9;A.TU_COST tuCost,&#x9;&#x9;&#xD;&#x9;&#x9;D.TOT_PAYMENT totPayment,&#xD;&#x9;&#x9;D.PAY_INSTALL_BILL payInstallBill,&#xD;&#x9;&#x9;D.PAY_COLL_FEE payCollFee,&#xD;&#x9;&#x9;D.PAY_PENALTY payPenalty,&#xD;&#x9;&#x9;D.PAY_PENALTY_TU payPenaltyTu,&#xD;&#x9;&#x9;D.PAY_COST_TU payCostTu,&#xD;&#x9;&#x9;D.DISCOUNT_VALUE discountValue,&#xD;&#x9;&#x9;B.TENOR instToPelunasan,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM((PRINCIPAL_VALUE-PRINCIPAL_PAID)+(INT_VALUE-INT_PAID)) total&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;) tagihanAngsuranPelunasan,&#xD;&#x9;&#x9;A.DEPOSIT_STS depositSts,&#xD;&#x9;&#x9;A.EXPIRED_DATE expiredDate,&#xD;&#x9;&#x9;H.BRANCH_NAME bpkbLocation,&#xD;&#x9;&#x9;B.TENOR tenor,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;isnull(SUM(PRINCIPAL_VALUE+INT_VALUE-PRINCIPAL_PAID-INT_PAID),0) angsuranBelumJatuhTempo&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE where AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &gt; GETDATE()&#xD;&#x9;&#x9;) angsuranBelumJatuhTempo,&#xD;&#x9;&#x9;B.WO_DATE woDate&#xD;&#x9;FROM TRX_TU_DEPOSIT A (nolock)&#xD;&#x9;LEFT JOIN TRX_AGGR B (nolock)&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN MST_AR_STS C (nolock)&#xD;&#x9;ON B.AR_STS = C.AR_STS AND ISNULL(C.SUB_STS, &#39;&#39;) = ISNULL(B.SUB_STS, &#39;&#39;)&#xD;&#x9;LEFT JOIN TRX_TU_SOLVING D (nolock)&#xD;&#x9;ON A.TU_DEPOSIT_NO = D.TU_DEPOSIT_NO AND A.SOLVING_TYPE = D.TU_SOLVING_TYPE AND A.SOLVING_DATE = D.TU_SOLVING_DATE AND D.BRANCH_CODE = B.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM E (nolock)&#xD;&#x9;ON D.TU_SOLVING_TYPE = E.PARAM_CODE AND E.CONDITION = &#39;SOLVING_TYPE&#39; AND E.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM F (nolock)&#xD;&#x9;ON D.APPV_STS = F.PARAM_CODE AND F.CONDITION = &#39;STATUS_APPROVAL&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_BPKB G (nolock)&#xD;&#x9;ON A.AGGR_NO = G.AGGR_NO AND A.COMPANY_CODE = G.COMPANY_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH H (nolock)&#xD;&#x9;ON G.BPKB_LOCATION_NAME = H.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH BR (nolock)&#xD;&#x9;ON A.COMPANY_CODE = BR.COMPANY_CODE AND A.BRANCH_CODE = BR.BRANCH_CODE&#xD;) t WHERE :search :filter</sql>
         <result element="count" rowName="" useColumnNumbers="false">
