      <query id="getCountMstCollFee" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(*) as total FROM (&#xD;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode, &#xD;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;B.PARAM_DESCRIP productName, &#xD;&#x9;&#x9;A.COLL_KIND_FEE collKindFee,&#xD;&#x9;&#x9;C.PARAM_DESCRIP collKindFeeName,&#xD;&#x9;&#x9;A.CATEGORY_CODE categoryCode,&#xD;&#x9;&#x9;D.PARAM_DESCRIP categoryName,&#xD;&#x9;&#x9;A.COLL_FEE collFee, &#xD;&#x9;&#x9;A.EFFECTIVE_DATE, &#xD;&#x9;&#x9; CONVERT(datetime, A.effective_date) as effectiveDate,&#xD;&#x9;&#x9; CONVERT(varchar, A.UPDATE_DATE, 121) as updatedDateStr&#xD;&#x9;FROM (&#xD;&#x9;SELECT A.*&#xD;&#x9;&#x9;FROM MST_COLL_FEE A&#xD;&#x9;&#x9;WHERE CONVERT(datetime, A.effective_date) &gt;= GETDATE()&#xD;&#x9;UNION ALL&#xD;&#x9;SELECT COMPANY_CODE, PRODUCT_CODE, COLL_KIND_FEE, CATEGORY_CODE, COLL_FEE, EFFECTIVE_DATE, CREATED_DATE, CREATED_BY, UPDATE_DATE, UPDATE_BY&#xD;&#x9;&#x9;  FROM (SELECT *, ROW_NUMBER() OVER(PARTITION BY [company_code],&#xD;&#x9;  &#x9;&#x9;&#x9;[product_code],&#xD;&#x9;  &#x9;&#x9;&#x9;[coll_kind_fee],&#xD;&#x9;  &#x9;&#x9;&#x9;[category_code]&#xD;&#x9;  &#x9;&#x9;&#x9;ORDER BY effective_date desc) RN&#xD;&#x9;&#x9;  FROM [dbo].[MST_COLL_FEE] WHERE effective_date &lt;= GETDATE()&#xD;&#x9;&#x9;  ) X&#xD;&#x9;&#x9;  WHERE RN = 1) A&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM B&#xD;&#x9;&#x9;ON A.product_code = B.param_code AND B.condition = &#39;PRODUCT_CODE&#39; AND B.flag_active = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM C&#xD;&#x9;&#x9;ON A.coll_kind_fee = C.param_code AND C.condition = &#39;COLL_KIND_FEE&#39; AND C.flag_active = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM D&#xD;&#x9;&#x9;ON A.category_code = D.param_code AND D.condition = &#39;CATEGORY_CODE&#39; AND D.flag_active = &#39;1&#39;&#xD;) t WHERE :search :filter</sql>
