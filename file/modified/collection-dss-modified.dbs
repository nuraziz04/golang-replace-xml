; AND D.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_AGGR E&#xD;&#x9;ON A.AGGR_NO = E.AGGR_NO&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM F&#xD;&#x9;ON A.PRODUCT_CODE = F.PARAM_CODE AND F.CONDITION = &#39;PRODUCT_CODE&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_BUCKET G&#xD;&#x9;ON A.BUCKET_CODE_FROM = G.BUCKET_CODE AND G.BUCKET_TYPE = &#39;FCOL&#39; AND G.FLAG_ACTIVE = &#39;1&#39; AND G.PRODUCT_CODE = E.PRODUCT_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER H&#xD;&#x9;ON E.CUSTOMER_CODE = H.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV&#xD;&#x9;ON H.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV&#xD;&#x9;ON H.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE I&#xD;&#x9;ON E.CUSTOMER_CODE = I.CUSTOMER_CODE AND I.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE KV&#xD;&#x9;ON I.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE J&#xD;&#x9;ON E.CUSTOMER_CODE = J.CUSTOMER_CODE AND J.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE BV&#xD;&#x9;ON J.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE EM&#xD;&#x9;ON A.CREATED_BY = EM.NIK&#xD;&#x9;left join mst_customer_co co&#xD;&#x9;on a.company_code = co.company_code and E.customer_code = co.customer_code&#xD;&#x9;left join mst_village cov&#xD;&#x9;on co.office_village_code = cov.village_code&#xD;) t WHERE :search :filter</sql>
