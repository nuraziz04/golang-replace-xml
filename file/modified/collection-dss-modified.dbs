lock) cd&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;left join mst_coll_day_dtl(nolock) cdtl&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;on cd.coll_day_code = cdtl.coll_day_code and cdtl.flag_active = &#39;1&#39; and cdtl.product_code = cd.product_code&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE A.COLL_DAY_CODE = cd.coll_day_code and cd.flag_active = &#39;1&#39; and cd.product_code = BR.PRODUCT_CODE and A.FLAG_REVITALIZATION=&#39;1&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;FOR XML PATH(&#39;&#39;)&#xD;&#x9;&#x9;&#x9;), 1, 1, &#39;&#39;) end as collDayName,&#xD;&#x9;&#x9;&#x9;A.GROUP_ADDRESS groupAddress,&#xD;&#x9;&#x9;&#x9;A.FLAG_REVITALIZATION flagRevitalization,&#xD;&#x9;&#x9;&#x9;B.VILLAGE_NAME villageName,&#xD;&#x9;&#x9;&#x9;C.SUBDISTRICT_NAME subdistrictName,&#xD;&#x9;&#x9;&#x9;D.CITY_NAME cityName,&#xD;&#x9;&#x9;&#x9;E.PROVINCE_NAME provinceName,&#xD;&#x9;&#x9;&#x9;(select count(0) from (select distinct group_code,aggr_no from TRX_COLLECTOR_SCHEDULE_WITH_AGGR_NO (nolock)&#xD;&#x9;&#x9;&#x9;where company_code=A.COMPANY_CODE and group_code =A.GROUP_CODE) as sq)  as totalDebitur,&#xD;&#x9;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;A.GROUP_LEADER groupLeader,&#xD;&#x9;&#x9;&#x9;A.COLL_TIME collTime,&#xD;&#x9;&#x9;&#x9;F.PARAM_DESCRIP collTimeDescrip,&#xD;&#x9;&#x9;&#x9;A.FUNDING_DATE fundingDate,&#xD;&#x9;&#x9;&#x9;case when A.FLAG_REVITALIZATION = &#39;1&#39; then &#39;&#39;&#xD;&#x9;&#x9;&#x9;else (select PF.PARAM_DESCRIP FROM MST_GLOBAL_PARAM PF (NOLOCK) WHERE A.COMPANY_CODE = PF.COMPANY_CODE AND PF.PARAM_CODE = A.PAYMENT_FREQ AND PF.CONDITION = &#39;PAYMENT_FREQ&#39;)&#xD;&#x9;&#x9;&#x9;end as paymentFreq&#xD;&#x9;&#x9;FROM MST_GROUP_HDR A (nolock)&#xD;&#x9;&#x9;LEFT JOIN MST_VILLAGE B (nolock)&#xD;&#x9;&#x9;ON A.GROUP_VILLAGE_CODE = B.VILLAGE_CODE AND A.COMPANY_CODE=B.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_SUBDISTRICT C (nolock)&#xD;&#x9;&#x9;ON B.SUBDISTRICT_CODE = C.SUBDISTRICT_CODE AND B.COMPANY_CODE=C.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_CITY D (nolock)&#xD;&#x9;&#x9;ON B.CITY_CODE = D.CITY_CODE AND B.COMPANY_CODE=D.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_PROVINCE E (nolock)&#xD;&#x9;&#x9;ON B.PROVINCE_CODE = E.PROVINCE_CODE AND B.COMPANY_CODE=E.COMPANY_CODE&#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM F (nolock)&#xD;&#x9;&#x9;ON A.COMPANY_CODE = F.COMPANY_CODE AND A.COLL_TIME = F.PARAM_CODE AND F.CONDITION = &#39;VISIT_TIME&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_BRANCH BR (nolock) ON A.BRANCH_CODE = BR.BRANCH_CODE AND A.COMPANY_CODE = BR.COMPANY_CODE&#xD;&#x9;&#x9;left join (select company_code,group_code,dkh_sts,rn&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;from (select dtl.company_code, dtl.group_code, hdr.dkh_sts, row_number() over(partition by dtl.company_code,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;dtl.group_code&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;order by hdr.created_date desc) rn&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from trx_dkh_dtl(nolock) dtl&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;join trx_dkh_hdr(nolock) hdr&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;on dtl.company_code = hdr.company_code&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and dtl.dkh_no = hdr.dkh_no&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and hdr.dkh_sts in (&#39;OPEN&#39;,&#39;PROC&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;) x&#xD;&#x9;&#x9;&#x9;where rn = 1) gr&#xD;&#x9;&#x9;&#x9;on A.company_code = gr.company_code and A.group_code = gr.group_code&#xD;&#x9;&#x9;WHERE A.GROUP_STS = &#39;1&#39; and gr.dkh_sts is null&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
