      <query id="findBranchCodeBiayaTarikUnit" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @REQ_COST_NO varchar(50) = :reqCostNo&#xD;&#xD;SELECT branchCode, branchName, aggrNo, customerFullname FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;B.BRANCH_NAME branchName,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;C.CUSTOMER_FULLNAME customerFullname&#xD;&#x9;FROM TRX_STTU_COST_HDR A (nolock)&#xD;&#x9;LEFT JOIN MST_BRANCH B (nolock)&#xD;&#x9;ON A.BRANCH_CODE = B.BRANCH_CODE AND A.COMPANY_CODE=B.COMPANY_CODE&#xD;&#x9;LEFT JOIN TRX_AGGR C (nolock)&#xD;&#x9;ON A.AGGR_NO = C.AGGR_NO AND A.COMPANY_CODE = C.COMPANY_CODE&#xD;&#x9;WHERE A.COMPANY_CODE = &#39;EDV&#39; AND A.REQ_COST_NO = @REQ_COST_NO&#xD;) a</sql>
