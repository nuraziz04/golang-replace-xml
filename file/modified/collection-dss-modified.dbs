      <query id="getDataTrxDkh" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT DISTINCT companyCode, branchCode, productCode, productName, dkhNo, nameDay, dkhDate, picHead, collCode, collName, dkhSts, dkhStsName, qtyCustomer, qtyGroup, qtyGroupVisit, qtyGroupCash, totCollFee,&#xD;totPenaltyValue, totInstallBill, totBill, totPaidCollFee, totPaidPenalty, totPaidInstallBill, totUtjPlus, totPaidCash, cashierDepositValue, paymentPointDeposit, collCashOnHand,&#xD;totUtjUsage, submittedDate, submittedBy, canceledDate, canceledBy, closedDate, closedBy, jumlahTertagih, jumlahTidakTertagih, sp1, sp2, sp3, sp1Mikro, sp2Mikro, sp3Mikro, createdDate, createdBy, qtyCustomerCash, updatedDateStr, dkhStatusDateFilter, totalPenugasan, totalDikunjungi, totalTidakDikunjungi, totalBelumDikunjungi from (&#xD;&#x9;SELECT DISTINCT companyCode, branchCode, productCode, productName, dkhNo, nameDay, dkhDate, picHead, collCode, collName, dkhSts, dkhStsName, qtyCustomer, qtyGroup, qtyGroupVisit, qtyGroupCash, totCollFee,&#xD;&#x9;totPenaltyValue, totInstallBill, totBill, totPaidCollFee, totPaidPenalty, totPaidInstallBill, totUtjPlus, totPaidCash, cashierDepositValue, paymentPointDeposit, collCashOnHand,&#xD;&#x9;totUtjUsage, submittedDate, submittedBy, canceledDate, canceledBy, closedDate, closedBy, jumlahTertagih, jumlahTidakTertagih, sp1, sp2, sp3, sp1Mikro, sp2Mikro, sp3Mikro, createdDate, createdBy, qtyCustomerCash, flagSend, flagSendDate, updatedDateStr, dkhStatusDateFilter, totalPenugasan, totalDikunjungi, totalTidakDikunjungi, totalBelumDikunjungi FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;E.PARAM_DESCRIP productName,&#xD;&#x9;&#x9;&#x9;A.DKH_NO dkhNo,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Sunday&#39; then &#39;Minggu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Monday&#39; then &#39;Senin&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Tuesday&#39; then &#39;Selasa&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Wednesday&#39; then &#39;Rabu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Thursday&#39; then &#39;Kamis&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Friday&#39; then &#39;Jumat&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Saturday&#39; then &#39;Sabtu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;end&#xD;&#x9;&#x9;&#x9;) as nameDay,&#xD;&#x9;&#x9;&#x9;A.DKH_DATE dkhDate,&#xD;&#x9;&#x9;&#x9;A.PIC_HEAD picHead,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;B.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;A.DKH_STS dkhSts,&#xD;&#x9;&#x9;&#x9;C.PARAM_DESCRIP dkhStsName,&#xD;&#x9;&#x9;&#x9;A.QTY_CUSTOMER qtyCustomer,&#xD;&#x9;&#x9;&#x9;A.QTY_GROUP qtyGroup,&#xD;&#x9;&#x9;&#x9;A.QTY_GROUP_VISIT qtyGroupVisit,&#xD;&#x9;&#x9;&#x9;A.QTY_GROUP_CASH qtyGroupCash,&#xD;&#x9;&#x9;&#x9;A.TOT_COLL_FEE totCollFee,&#xD;&#x9;&#x9;&#x9;A.TOT_PENALTY_VALUE totPenaltyValue,&#xD;&#x9;&#x9;&#x9;A.TOT_INSTALL_BILL totInstallBill,&#xD;&#x9;&#x9;&#x9;A.TOT_BILL totBill,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_COLL_FEE totPaidCollFee,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_PENALTY totPaidPenalty,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_INSTALL_BILL totPaidInstallBill,&#xD;&#x9;&#x9;&#x9;A.TOT_UTJ_PLUS totUtjPlus,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_CASH totPaidCash,&#xD;&#x9;&#x9;&#x9;A.CASHIER_DEPOSIT_VALUE cashierDepositValue,&#xD;&#x9;&#x9;&#x9;A.PAYMENT_POINT_DEPOSIT paymentPointDeposit,&#xD;&#x9;&#x9;&#x9;A.COLL_CASH_ON_HAND collCashOnHand,&#xD;&#x9;&#x9;&#x9;A.TOT_UTJ_USAGE totUtjUsage,&#xD;&#x9;&#x9;&#x9;A.SUBMITTED_DATE submittedDate,&#xD;&#x9;&#x9;&#x9;A.SUBMITTED_BY submittedBy,&#xD;&#x9;&#x9;&#x9;A.CANCELED_DATE canceledDate,&#xD;&#x9;&#x9;&#x9;A.CANCELED_BY canceledBy,&#xD;&#x9;&#x9;&#x9;A.CLOSED_DATE closedDate,&#xD;&#x9;&#x9;&#x9;A.CLOSED_BY closedBy,&#xD;&#x9;&#x9;&#x9;CASE WHEN A.PRODUCT_CODE = &#39;PC02&#39; THEN (&#xD;                SELECT &#xD;                  COUNT(0) PAID_CASH &#xD;                FROM &#xD;                  TRX_DKH_DTL F (NOLOCK) &#xD;                WHERE &#xD;&#x9;&#x9;&#x9;&#x9;  f.company_code = a.company_code&#xD;&#x9;&#x9;&#x9;&#x9;  and f.branch_code = a.branch_code&#xD;                  and F.DKH_NO = A.DKH_NO &#xD;                  AND F.PAID_CASH &gt; 0 &#xD;                  AND PAID_STS != &#39;999&#39;&#xD;              )+(&#xD;                SELECT &#xD;                  COUNT(0) PAID_CASH &#xD;                FROM &#xD;                  TRX_DKH_DTL F (NOLOCK) &#xD;                WHERE &#xD;                  f.company_code = a.company_code&#xD;&#x9;&#x9;&#x9;&#x9;  and f.branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and F.DKH_NO = A.DKH_NO &#xD;                  AND F.PAID_CASH = 0 &#xD;                  AND F.UTJ_APPV_STS = &#39;APPD&#39; &#xD;                  AND F.PAID_STS != &#39;999&#39;&#xD;              ) ELSE (&#xD;                SELECT &#xD;                  COUNT(0) PAID_CASH &#xD;                FROM &#xD;                  TRX_DKH_DTL F (NOLOCK) &#xD;                WHERE &#xD;                  f.company_code = a.company_code&#xD;&#x9;&#x9;&#x9;&#x9;  and f.branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and F.DKH_NO = A.DKH_NO &#xD;                  AND F.PAID_CASH &gt; 0 &#xD;                  AND F.PAID_STS != &#39;999&#39;&#xD;              ) END as jumlahTertagih, &#xD;              CASE WHEN A.PRODUCT_CODE = &#39;PC02&#39; THEN A.QTY_CUSTOMER - (&#xD;                SELECT &#xD;                  COUNT(0) PAID_CASH &#xD;                FROM &#xD;                  TRX_DKH_DTL F (NOLOCK) &#xD;                WHERE &#xD;                  f.company_code = a.company_code&#xD;&#x9;&#x9;&#x9;&#x9;  and f.branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and F.DKH_NO = A.DKH_NO &#xD;                  AND F.PAID_CASH &gt; 0 &#xD;                  AND PAID_STS != &#39;999&#39;&#xD;              )+(&#xD;                SELECT &#xD;                  COUNT(0) PAID_CASH &#xD;                FROM &#xD;                  TRX_DKH_DTL F (NOLOCK) &#xD;                WHERE &#xD;&#x9;&#x9;&#x9;&#x9;f.company_code = a.company_code&#xD;                  and f.branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and F.DKH_NO = A.DKH_NO &#xD;                  AND F.PAID_CASH = 0 &#xD;                  AND F.UTJ_APPV_STS = &#39;APPD&#39; &#xD;                  AND F.PAID_STS != &#39;999&#39;&#xD;              ) ELSE (&#xD;                A.QTY_CUSTOMER -(&#xD;                  SELECT &#xD;                    COUNT(0) PAID_CASH &#xD;                  FROM &#xD;                    TRX_DKH_DTL F (NOLOCK) &#xD;                  WHERE &#xD;                    f.company_code = a.company_code&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;and f.branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;and F.DKH_NO = A.DKH_NO &#xD;                    AND F.PAID_CASH &gt; 0 &#xD;                    AND F.PAID_STS != &#39;999&#39;&#xD;                )&#xD;              ) END jumlahTidakTertagih, &#xD;              (&#xD;                SELECT &#xD;                  COUNT(0) SP_NO &#xD;                FROM &#xD;                  TRX_DKH_DTL (NOLOCK) &#xD;                WHERE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;company_code = a.company_code&#xD;                  and branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and SP_TYPE = &#39;001&#39; &#xD;                  AND DKH_NO = A.DKH_NO&#xD;              ) as sp1, &#xD;              (&#xD;                SELECT &#xD;                  COUNT(0) SP_NO &#xD;                FROM &#xD;                  TRX_DKH_DTL (NOLOCK) &#xD;                WHERE &#xD;                  company_code = a.company_code&#xD;&#x9;&#x9;&#x9;&#x9;  and branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and SP_TYPE = &#39;002&#39; &#xD;                  AND DKH_NO = A.DKH_NO&#xD;              ) as sp2, &#xD;              (&#xD;                SELECT &#xD;                  COUNT(0) SP_NO &#xD;                FROM &#xD;                  TRX_DKH_DTL (NOLOCK) &#xD;                WHERE company_code = a.company_code&#xD;                  and branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and SP_TYPE = &#39;003&#39; &#xD;                  AND DKH_NO = A.DKH_NO&#xD;              ) as sp3, &#xD;              (&#xD;                SELECT &#xD;                  COUNT(0) SP_NO &#xD;                FROM &#xD;                  TRX_DKH_DTL (NOLOCK) &#xD;                WHERE company_code = a.company_code&#xD;                  and branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and SP_TYPE = &#39;004&#39; &#xD;                  AND DKH_NO = A.DKH_NO&#xD;              ) as sp1Mikro, &#xD;              (&#xD;                SELECT &#xD;                  COUNT(0) SP_NO &#xD;                FROM &#xD;                  TRX_DKH_DTL (NOLOCK) &#xD;                WHERE company_code = a.company_code&#xD;                  and branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and SP_TYPE = &#39;005&#39; &#xD;                  AND DKH_NO = A.DKH_NO&#xD;              ) as sp2Mikro, &#xD;              (&#xD;                SELECT &#xD;                  COUNT(0) SP_NO &#xD;                FROM &#xD;                  TRX_DKH_DTL (NOLOCK) &#xD;                WHERE company_code = a.company_code&#xD;                  and branch_code = a.branch_code&#xD;&#x9;&#x9;&#x9;&#x9;  and SP_TYPE = &#39;006&#39; &#xD;                  AND DKH_NO = A.DKH_NO&#xD;              ) as sp3Mikro,&#xD;&#x9;&#x9;&#x9;D.PAYMENT_DATE paymentDate,&#xD;&#x9;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;A.CREATED_BY createdBy,&#xD;&#x9;&#x9;&#x9;A.QTY_CUSTOMER_CASH qtyCustomerCash,&#xD;&#x9;&#x9;&#x9;ISNULL(D.FLAG_SEND, &#39;&#39;) flagSend,&#xD;&#x9;&#x9;&#x9;CONVERT(DATE,D.FLAG_SEND_DATE) flagSendDate,&#xD;&#x9;&#x9;&#x9;CONVERT(varchar, A.UPDATED_DATE, 121) as updatedDateStr,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;OPEN&#39; THEN CONVERT(DATE, A.CREATED_DATE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;PROC&#39; THEN CONVERT(DATE, A.SUBMITTED_DATE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;CLOS&#39; THEN CONVERT(DATE, A.CLOSED_DATE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;CANC&#39; THEN CONVERT(DATE, A.CANCELED_DATE)&#xD;&#x9;&#x9;&#x9;END dkhStatusDateFilter,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl (NOLOCK) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39;&#xD;&#x9;&#x9;&#x9;) as totalPenugasan,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl (NOLOCK) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39; and visit_code = &#39;1&#39;&#xD;&#x9;&#x9;&#x9;) as totalDikunjungi,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl (NOLOCK) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39; and visit_code = &#39;0&#39;&#xD;&#x9;&#x9;&#x9;) as totalTidakDikunjungi,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl (NOLOCK) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39; and isnull(visit_code, &#39;&#39;) = &#39;&#39;&#xD;&#x9;&#x9;&#x9;) as totalBelumDikunjungi&#xD;&#x9;&#x9;FROM TRX_DKH_HDR A (NOLOCK)&#xD;&#x9;&#x9;LEFT JOIN MST_EMPLOYEE B (NOLOCK)&#xD;&#x9;&#x9;ON A.COLL_CODE = B.NIK and a.company_code = b.company_code&#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM C (NOLOCK)&#xD;&#x9;&#x9;ON A.DKH_STS = C.PARAM_CODE AND C.CONDITION = &#39;DKH_STS&#39; AND C.FLAG_ACTIVE = &#39;1&#39; and c.company_code = a.company_code&#xD;&#x9;&#x9;LEFT JOIN TRX_DKH_DTL D (NOLOCK)&#xD;&#x9;&#x9;ON a.company_code = d.company_code and a.branch_code = d.branch_code and A.DKH_NO = D.DKH_NO AND (convert(date,D.FLAG_SEND_DATE) BETWEEN :dateFrom AND :dateUntil OR D.FLAG_SEND_DATE IS NULL)&#xD;&#x9;&#x9;LEFT JOIN MST_GLOBAL_PARAM E (NOLOCK)&#xD;&#x9;&#x9;ON e.company_code = a.company_code and A.PRODUCT_CODE = E.PARAM_CODE AND E.CONDITION = &#39;PRODUCT_CODE&#39; AND E.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;)  A &#xD;) a WHERE  :search :filter :customFilter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
