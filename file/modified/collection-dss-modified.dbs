      <query id="getDkhHdr" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @COLL_CODE varchar(30) = :collCode&#xD;declare @PRODUCT_CODE varchar(10) = :productCode&#xD;declare @BRANCH_CODE varchar(10) = :branchCode&#xD;&#xD;SELECT companyCode, branchCode, productCode, productName, dkhNo, collCode, collName, dkhSts, dkhStsName, nameDay, jumlahTertagih, jumlahTidakTertagih, sp1, sp2, sp3, sp1Mikro, sp2Mikro, sp3Mikro, createdDate, createdBy, qtyCustomerCash, updatedDateStr, dkhStatusDateFilter, totalPenugasan, totalDikunjungi, totalTidakDikunjungi, totalBelumDikunjungi, picHead, qtyCustomer, qtyGroup, qtyGroupVisit, qtyGroupCash, totCollFee, totPenaltyValue, totInstallBill, totBill, totPaidCollFee, totPaidPenalty, totPaidInstallBill, totUtjPlus, totPaidCash, cashierDepositValue, paymentPointDeposit, collCashOnHand, totUtjUsage FROM (&#xD;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.DKH_NO dkhNo,&#xD;&#x9;&#x9;&#x9;A.DKH_STS dkhSts,&#xD;&#x9;&#x9;&#x9;--C.PARAM_DESCRIP dkhStsName,&#xD;&#x9;&#x9;&#x9;(SELECT C.PARAM_DESCRIP FROM MST_GLOBAL_PARAM C (NOLOCK) WHERE A.DKH_STS = C.PARAM_CODE AND C.CONDITION = &#39;DKH_STS&#39; AND C.FLAG_ACTIVE = &#39;1&#39;) dkhStsName,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;--E.PARAM_DESCRIP productName,&#xD;&#x9;&#x9;&#x9;(SELECT D.PARAM_DESCRIP FROM MST_GLOBAL_PARAM D (NOLOCK) WHERE A.PRODUCT_CODE = D.PARAM_CODE AND D.CONDITION = &#39;PRODUCT_CODE&#39; AND D.FLAG_ACTIVE = &#39;1&#39;) productName,&#xD;&#x9;&#x9;&#x9;concat(A.COLL_CODE, &#39; &#39;) collCode,&#xD;&#x9;&#x9;&#x9;--B.FULLNAME collName,&#xD;&#x9;&#x9;&#x9;(SELECT B.FULLNAME FROM MST_EMPLOYEE B (NOLOCK) WHERE A.COLL_CODE=B.NIK) collName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Sunday&#39; then &#39;Minggu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Monday&#39; then &#39;Senin&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Tuesday&#39; then &#39;Selasa&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Wednesday&#39; then &#39;Rabu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Thursday&#39; then &#39;Kamis&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Friday&#39; then &#39;Jumat&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Saturday&#39; then &#39;Sabtu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;end&#xD;&#x9;&#x9;&#x9;) as nameDay,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.PRODUCT_CODE = &#39;PC02&#39; THEN &#xD;&#x9;&#x9;&#x9;&#x9;(SELECT COUNT(0) PAID_CASH FROM TRX_DKH_DTL F (nolock) WHERE F.DKH_NO = A.DKH_NO AND F.PAID_CASH &gt; 0 AND PAID_STS != &#39;999&#39;)+(SELECT COUNT(0) PAID_CASH FROM TRX_DKH_DTL F (nolock) WHERE F.DKH_NO = A.DKH_NO AND F.PAID_CASH = 0 AND F.UTJ_APPV_STS = &#39;APPD&#39; AND F.PAID_STS != &#39;999&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;ELSE (SELECT COUNT(0) PAID_CASH FROM TRX_DKH_DTL F (nolock) WHERE F.DKH_NO = A.DKH_NO AND F.PAID_CASH &gt; 0 AND F.PAID_STS != &#39;999&#39;)&#xD;&#x9;&#x9;&#x9;END as jumlahTertagih,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.PRODUCT_CODE = &#39;PC02&#39; THEN A.QTY_CUSTOMER-&#xD;&#x9;&#x9;&#x9;&#x9;(SELECT COUNT(0) PAID_CASH FROM TRX_DKH_DTL F (nolock) WHERE F.DKH_NO = A.DKH_NO AND F.PAID_CASH &gt; 0 AND PAID_STS != &#39;999&#39;)+(SELECT COUNT(0) PAID_CASH FROM TRX_DKH_DTL F (nolock) WHERE F.DKH_NO = A.DKH_NO AND F.PAID_CASH = 0 AND F.UTJ_APPV_STS = &#39;APPD&#39; AND F.PAID_STS != &#39;999&#39;)&#xD;&#x9;&#x9;&#x9;&#x9;ELSE (A.QTY_CUSTOMER-(SELECT COUNT(0) PAID_CASH FROM TRX_DKH_DTL F (nolock) WHERE F.DKH_NO = A.DKH_NO AND F.PAID_CASH &gt; 0 AND F.PAID_STS != &#39;999&#39;))&#xD;&#x9;&#x9;&#x9;END jumlahTidakTertagih,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(0) SP_NO FROM TRX_DKH_DTL(nolock)&#xD;&#x9;&#x9;&#x9;&#x9;WHERE SP_TYPE = &#39;001&#39; AND DKH_NO = A.DKH_NO &#xD;&#x9;&#x9;&#x9;) as sp1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(0) SP_NO FROM TRX_DKH_DTL(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;WHERE SP_TYPE = &#39;002&#39; AND DKH_NO = A.DKH_NO &#xD;&#x9;&#x9;&#x9;) as sp2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(0) SP_NO FROM TRX_DKH_DTL(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;WHERE SP_TYPE = &#39;003&#39; AND DKH_NO = A.DKH_NO &#xD;&#x9;&#x9;&#x9;) as sp3,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(0) SP_NO FROM TRX_DKH_DTL(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;WHERE SP_TYPE = &#39;004&#39; AND DKH_NO = A.DKH_NO &#xD;&#x9;&#x9;&#x9;) as sp1Mikro,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(0) SP_NO FROM TRX_DKH_DTL(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;WHERE SP_TYPE = &#39;005&#39; AND DKH_NO = A.DKH_NO &#xD;&#x9;&#x9;&#x9;) as sp2Mikro,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(0) SP_NO FROM TRX_DKH_DTL(nolock) &#xD;&#x9;&#x9;&#x9;&#x9;WHERE SP_TYPE = &#39;006&#39; AND DKH_NO = A.DKH_NO &#xD;&#x9;&#x9;&#x9;) as sp3Mikro,&#xD;&#x9;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;A.CREATED_BY createdBy,&#xD;&#x9;&#x9;&#x9;A.QTY_CUSTOMER_CASH qtyCustomerCash,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.UPDATED_DATE IS NULL THEN &#39;-&#39;&#xD;&#x9;&#x9;&#x9;&#x9;ELSE CONVERT(varchar, A.UPDATED_DATE, 121) &#xD;&#x9;&#x9;&#x9;END updatedDateStr,&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;OPEN&#39; THEN CONVERT(DATE, A.CREATED_DATE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;PROC&#39; THEN CONVERT(DATE, A.SUBMITTED_DATE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;CLOS&#39; THEN CONVERT(DATE, A.CLOSED_DATE)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN A.DKH_STS = &#39;CANC&#39; THEN CONVERT(DATE, A.CANCELED_DATE)&#xD;&#x9;&#x9;&#x9;END dkhStatusDateFilter,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl(nolock) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39;&#xD;&#x9;&#x9;&#x9;) as totalPenugasan,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl(nolock) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39; and visit_code = &#39;1&#39;&#xD;&#x9;&#x9;&#x9;) as totalDikunjungi,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl(nolock) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39; and visit_code = &#39;0&#39;&#xD;&#x9;&#x9;&#x9;) as totalTidakDikunjungi,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select count(aggr_no) as totalPenugasan from trx_dkh_dtl(nolock) where company_code = A.company_code and branch_code = A.branch_code and dkh_no = A.dkh_no and paid_sts != &#39;999&#39; and isnull(visit_code, &#39;&#39;) = &#39;&#39;&#xD;&#x9;&#x9;&#x9;) as totalBelumDikunjungi,&#xD;&#x9;&#x9;&#x9;concat(A.PIC_HEAD, &#39; &#39;) picHead,&#xD;&#x9;&#x9;&#x9;A.QTY_CUSTOMER qtyCustomer,&#xD;&#x9;&#x9;&#x9;A.QTY_GROUP qtyGroup,&#xD;&#x9;&#x9;&#x9;A.QTY_GROUP_VISIT qtyGroupVisit,&#xD;&#x9;&#x9;&#x9;A.QTY_GROUP_CASH qtyGroupCash,&#xD;&#x9;&#x9;&#x9;A.TOT_COLL_FEE totCollFee,&#xD;&#x9;&#x9;&#x9;A.TOT_PENALTY_VALUE totPenaltyValue,&#xD;&#x9;&#x9;&#x9;A.TOT_INSTALL_BILL totInstallBill,&#xD;&#x9;&#x9;&#x9;A.TOT_BILL totBill,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_COLL_FEE totPaidCollFee,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_PENALTY totPaidPenalty,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_INSTALL_BILL totPaidInstallBill,&#xD;&#x9;&#x9;&#x9;A.TOT_UTJ_PLUS totUtjPlus,&#xD;&#x9;&#x9;&#x9;A.TOT_PAID_CASH totPaidCash,&#xD;&#x9;&#x9;&#x9;A.CASHIER_DEPOSIT_VALUE cashierDepositValue,&#xD;&#x9;&#x9;&#x9;A.PAYMENT_POINT_DEPOSIT paymentPointDeposit,&#xD;&#x9;&#x9;&#x9;A.COLL_CASH_ON_HAND collCashOnHand,&#xD;&#x9;&#x9;&#x9;A.TOT_UTJ_USAGE totUtjUsage&#xD;&#x9;&#x9;FROM TRX_DKH_HDR A (nolock)&#xD;&#x9;&#x9;WHERE A.COMPANY_CODE = @COMPANY_CODE AND A.COLL_CODE = @COLL_CODE AND A.PRODUCT_CODE = @PRODUCT_CODE AND A.BRANCH_CODE = @BRANCH_CODE AND A.DKH_STS=&#39;PROC&#39;&#xD;)  A</sql>
