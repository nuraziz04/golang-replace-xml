         <param name="filter" sqlType="QUERY_STRING"></param>
      </query>
      <query id="getDataMonitorUtj" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT companyCode, branchCode, productCode, seqNo, reffDate, reffNo, utjTransCode, utjTransName, beginningValue, plusValue, bookValue, usageValue, balanceValue, noKontrakPenerimaUtj, customerCode, namaDebiturPenerimaUtj, groupCode, groupName&#xD;FROM (&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;(SELECT PRODUCT_CODE FROM TRX_AGGR (NOLOCK) WHERE AGGR_NO=A.AGGR_NO_RECIPIENT AND COMPANY_CODE=A.COMPANY_CODE) productCode,&#xD;&#x9;&#x9;A.SEQ_NO seqNo,&#xD;&#x9;&#x9;A.REFF_DATE reffDate,&#xD;&#x9;&#x9;A.REFF_NO reffNo,&#xD;&#x9;&#x9;A.UTJ_TRANS_CODE utjTransCode,&#xD;&#x9;&#x9;--B.PARAM_DESCRIP utjTransName,&#xD;(SELECT B.PARAM_DESCRIP FROM MST_GLOBAL_PARAM B (nolock) WHERE A.COMPANY_CODE=B.COMPANY_CODE AND A.UTJ_TRANS_CODE = B.PARAM_CODE AND B.CONDITION = &#39;UTJ_TRANS_CODE&#39; AND B.FLAG_ACTIVE = &#39;1&#39;) utjTransName,&#xD;&#x9;&#x9;A.BEGINNING_VALUE beginningValue,&#xD;&#x9;&#x9;A.PLUS_VALUE plusValue,&#xD;&#x9;&#x9;A.BOOK_VALUE bookValue,&#xD;&#x9;&#x9;A.USAGE_VALUE usageValue,&#xD;&#x9;&#x9;A.BALANCE_VALUE balanceValue,&#xD;&#x9;&#x9;A.AGGR_NO_RECIPIENT noKontrakPenerimaUtj,&#xD;&#x9;&#x9;--C.CUSTOMER_CODE customerCode,&#xD;(SELECT C.CUSTOMER_CODE FROM TRX_AGGR C (NOLOCK) WHERE A.AGGR_NO = C.AGGR_NO AND A.COMPANY_CODE=C.COMPANY_CODE) customerCode,&#xD;(SELECT CUSTOMER_FULLNAME FROM TRX_AGGR (NOLOCK) WHERE AGGR_NO=A.AGGR_NO_RECIPIENT AND COMPANY_CODE=A.COMPANY_CODE) namaDebiturPenerimaUtj,&#xD;(SELECT GROUP_CODE FROM TRX_AGGR (NOLOCK) WHERE AGGR_NO=A.AGGR_NO_RECIPIENT AND COMPANY_CODE=A.COMPANY_CODE) groupCode,&#xD;(SELECT MG.GROUP_NAME FROM TRX_AGGR AG (NOLOCK)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;JOIN MST_GROUP_HDR MG (NOLOCK) ON MG.GROUP_CODE = AG.GROUP_CODE AND MG.COMPANY_CODE=AG.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE AG.AGGR_NO=A.AGGR_NO_RECIPIENT)&#xD;&#x9;&#x9; groupName,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;A.CREATED_DATE createdDate&#xD;&#x9;FROM TRX_UTJ A (NOLOCK)&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM B&#xD;&#x9;--ON A.UTJ_TRANS_CODE = B.PARAM_CODE AND B.CONDITION = &#39;UTJ_TRANS_CODE&#39; AND B.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;--LEFT JOIN TRX_AGGR C (NOLOCK)&#xD;&#x9;--ON A.AGGR_NO = C.AGGR_NO AND A.COMPANY_CODE=C.COMPANY_CODE AND A.BRANCH_CODE=C.BRANCH_CODE&#xD;&#x9;--LEFT JOIN MST_GROUP_HDR D&#xD;&#x9;--ON C.GROUP_CODE = D.GROUP_CODE&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="companyCode" name="companyCode" xsdType="string"></element>
            <element column="branchCode" name="branchCode" xsdType="string"></element>
            <element column="productCode" name="productCode" xsdType="string"></element>
            <element column="seqNo" name="seqNo" xsdType="string"></element>
            <element column="reffDate" name="reffDate" xsdType="string"></element>
            <element column="reffNo" name="reffNo" xsdType="string"></element>
            <element column="utjTransCode" name="utjTransCode" xsdType="string"></element>
            <element column="utjTransName" name="utjTransName" xsdType="string"></element>
            <element column="beginningValue" name="beginningValue" xsdType="string"></element>
            <element column="plusValue" name="plusValue" xsdType="string"></element>
            <element column="bookValue" name="bookValue" xsdType="string"></element>
            <element column="usageValue" name="usageValue" xsdType="string"></element>
            <element column="balanceValue" name="balanceValue" xsdType="string"></element>
            <element column="noKontrakPenerimaUtj" name="noKontrakPenerimaUtj" xsdType="string"></element>
            <element column="customerCode" name="customerCode" xsdType="string"></element>
            <element column="namaDebiturPenerimaUtj" name="namaDebiturPenerimaUtj" xsdType="string"></element>
            <element column="groupCode" name="groupCode" xsdType="string"></element>
            <element column="groupName" name="groupName" xsdType="string"></element>
         </result>
         <param name="search" sqlType="QUERY_STRING"></param>
         <param name="filter" sqlType="QUERY_STRING"></param>
         <param name="orderBy" sqlType="QUERY_STRING"></param>
         <param name="direction" sqlType="QUERY_STRING"></param>
         <param name="page" sqlType="INTEGER"></param>
         <param name="limit" sqlType="INTEGER"></param>
      </query>
      <query id="getColumnLovNomorKontrakSp" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT 1 as aggrNo, 1 customerFullname, 1 address, 1 rt, 1 rw, 1 villageName, 1 subdistrictName, 1 cityName, 1 provinceName, 1 zipCode, 1 dayOverdue</sql>
         <result element="Entries" rowName="" useColumnNumbers="false">
            <element column="aggrNo" name="aggrNo" xsdType="string"></element>
            <element column="customerFullname" name="customerFullname" xsdType="string"></element>
            <element column="address" name="address" xsdType="string"></element>
