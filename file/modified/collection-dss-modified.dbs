      <query id="getCountTrxTuDeposit" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;BR.BRANCH_NAME branchName,&#xD;&#x9;&#x9;&#x9;A.TU_DEPOSIT_NO tuDepositNo,&#xD;&#x9;&#x9;&#x9;A.TU_DEPOSIT_DATE tuDepositDate,&#xD;&#x9;&#x9;&#x9;A.EXECUTION_DATE executionDate,&#xD;&#x9;&#x9;&#x9;A.STTU_NO sttuNo,&#xD;&#x9;&#x9;&#x9;B.STTU_DATE sttuDate,&#xD;&#x9;&#x9;&#x9;B.STTU_STS sttuSts,&#xD;&#x9;&#x9;&#x9;D.PARAM_DESCRIP sttuStsName,&#xD;&#x9;&#x9;&#x9;B.PIC1_CODE pic1Code,&#xD;&#x9;&#x9;&#x9;B.PIC2_CODE pic2Code,&#xD;&#x9;&#x9;&#x9;B.AGENT_TYPE agentType,&#xD;&#x9;&#x9;&#x9;F.PARAM_DESCRIP agentTypeName,&#xD;&#x9;&#x9;&#x9;PIC1.FULLNAME pic1Name,&#xD;&#x9;&#x9;&#x9;PIC2.FULLNAME pic2Name,&#xD;&#x9;&#x9;&#x9;A.RECEIPT_FROM receiptFrom,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;C.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;C.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;E.TRX_NAME arStsName,&#xD;&#x9;&#x9;&#x9;I.CATEGORY_CODE categoryCode,&#xD;&#x9;&#x9;&#x9;M.PARAM_DESCRIP categoryName,&#xD;&#x9;&#x9;&#x9;H.BRAND_CODE brandCode,&#xD;&#x9;&#x9;&#x9;I.BRAND_NAME brandName,&#xD;&#x9;&#x9;&#x9;H.TYPE_CODE typeCode,&#xD;&#x9;&#x9;&#x9;J.TYPE_NAME typeName,&#xD;&#x9;&#x9;&#x9;H.MODEL_CODE modelCode,&#xD;&#x9;&#x9;&#x9;K.MODEL_NAME modelName,&#xD;&#x9;&#x9;&#x9;J.KIND_CODE kindCode,&#xD;&#x9;&#x9;&#x9;L.KIND_NAME kindName,&#xD;&#x9;&#x9;&#x9;H.COLOR_CODE colorCode,&#xD;&#x9;&#x9;&#x9;N.COLOR_NAME colorName,&#xD;&#x9;&#x9;&#x9;H.NOPOL nopol,&#xD;&#x9;&#x9;&#x9;H.CHASSIS_NO chassisNo,&#xD;&#x9;&#x9;&#x9;H.MACHINE_NO machineNo,&#xD;&#x9;&#x9;&#x9;H.BPKB_NAME stnkName,&#xD;&#x9;&#x9;&#x9;A.BAL_PRINCIPAL_VALUE balPrincipalValue,&#xD;&#x9;&#x9;&#x9;A.BAL_INT_VALUE balIntValue,&#xD;&#x9;&#x9;&#x9;A.INST_NO instNo,&#xD;&#x9;&#x9;&#x9;A.BAL_TENOR balTenor,&#xD;&#x9;&#x9;&#x9;A.EXPIRED_DATE expiredDate,&#xD;&#x9;&#x9;&#x9;A.STNK_EXPIRED_DATE stnkExpiredDate,&#xD;&#x9;&#x9;&#x9;A.KM km,&#xD;&#x9;&#x9;&#x9;A.REQ_COST_NO reqCostNo,&#xD;&#x9;&#x9;&#x9;A.TU_COST tuCost,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select collFee from (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;select top 1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;coll_fee collFee, &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;EFFECTIVE_DATE effectiveDate&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;from mst_coll_fee (NOLOCK)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;where company_code = A.COMPANY_CODE AND product_code = C.PRODUCT_CODE AND CATEGORY_CODE = C.CATEGORY_CODE AND coll_kind_fee = &#39;ADMREP&#39; AND EFFECTIVE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dendaTarikUnit,&#xD;&#x9;&#x9;&#x9;A.PENALTY_TU_VALUE penaltyTuValue,&#xD;&#x9;&#x9;&#x9;A.NOTES_PIC_RECEIVE1 notesPicReceive1,&#xD;&#x9;&#x9;&#x9;A.NOTES_PIC_RECEIVE2 notesPicReceive2,&#xD;&#x9;&#x9;&#x9;A.LOC_CODE locCode,&#xD;&#x9;&#x9;&#x9;A.LOC_RECEIPT_CODE locReceiptCode,&#xD;&#x9;&#x9;&#x9;A.PRINTED_BY printedBy,&#xD;&#x9;&#x9;&#x9;A.PRINTED_DATE printedDate,&#xD;&#x9;&#x9;&#x9;A.APPV_STS appvSts,&#xD;&#x9;&#x9;&#x9;A.APPV_DATE appvDate,&#xD;&#x9;&#x9;&#x9;A.SOLVING_TYPE solvingType,&#xD;&#x9;&#x9;&#x9;G.PARAM_DESCRIP solvingTypeName,&#xD;&#x9;&#x9;&#x9;A.SOLVING_DATE solvingDate,&#xD;&#x9;&#x9;&#x9;A.SOLVING_NOTES solvingNotes,&#xD;&#x9;&#x9;&#x9;A.EXT_COUNT extCount,&#xD;&#x9;&#x9;&#x9;A.VEHC_OUT_DATE vehcOutDate,&#xD;&#x9;&#x9;&#x9;A.VEHC_OUT_BY vehcOutBy,&#xD;&#x9;&#x9;&#x9;A.OTR_VALUE otrValue,&#xD;&#x9;&#x9;&#x9;A.TAKSASI_VALUE taksasiValue,&#xD;&#x9;&#x9;&#x9;A.TOT_MINUS_VALUE totMinusValue,&#xD;&#x9;&#x9;&#x9;A.TOT_ADD_VALUE totAddValue,&#xD;&#x9;&#x9;&#x9;A.DEPOSIT_STS depositSts,&#xD;&#x9;&#x9;&#x9;A.VOUCHER_NO voucherNo,&#xD;&#x9;&#x9;&#x9;A.VOUCHER_DATE voucherDate,&#xD;&#x9;&#x9;&#x9;A.UNIT_CONDITION unitCondition,&#xD;&#x9;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;O.APPV_STS appvStsSolving,&#xD;&#x9;&#x9;&#x9;P.PARAM_DESCRIP appvStsNameSolving,&#xD;&#x9;&#x9;&#x9;H.YEAR_MFG yearMfg,&#xD;&#x9;&#x9;&#x9;C.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;&#x9;C.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;P&#39; THEN CEO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea1,&#xD;&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;P&#39; THEN CEO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_AREA2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_NO2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;P&#39; THEN CEO.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;&#x9;A.COLL_FEE collFee,&#xD;&#x9;&#x9;&#x9;A.PENALTY_VALUE penaltyValue,&#xD;&#x9;&#x9;&#x9;R.BRANCH_NAME locCodeName,&#xD;&#x9;&#x9;&#x9;S.BRANCH_NAME locReceiptCodeName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(sum(coll_fee-coll_fee_paid), 0) getCollfee&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) where company_code = A.COMPANY_CODE and aggr_no = C.AGGR_NO&#xD;&#x9;&#x9;&#x9;) as getCollfee,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(sum(pinalty_value-pinalty_paid), 0) penalty&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) where company_code = A.COMPANY_CODE and aggr_no = C.AGGR_NO&#xD;&#x9;&#x9;&#x9;) as penalty,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;VALUES_PARAM&#xD;&#x9;&#x9;&#x9;&#x9;FROM MST_GLOBAL_PARAM (NOLOCK) WHERE CONDITION = &#39;TU_CONFIRM_DATE&#39; AND CHAR_VALUES = C.PRODUCT_CODE AND FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;&#x9;) as confirmDay,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;VALUES_PARAM&#xD;&#x9;&#x9;&#x9;&#x9;FROM MST_GLOBAL_PARAM (NOLOCK) WHERE CONDITION = &#39;TU_INVENTORY&#39; AND CHAR_VALUES = C.PRODUCT_CODE AND FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;&#x9;) as titipanDay,&#xD;&#x9;&#x9;&#x9;PT.VALUES_PARAM simulasiPrice,&#xD;&#x9;&#x9;&#x9;B.EXPIRED_DATE expiredDateSttu,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SUM(PRINCIPAL_VALUE-PRINCIPAL_PAID) balPrincipalValue&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;) as getBalPrincipalValue,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SUM(INT_VALUE-INT_PAID) balIntValue&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;) as getBalIntValue,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;&#x9;ISNULL(ST.instNo, 0) sisaTenor,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;&#x9;A.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;&#x9;A.EXT_COLL_CODE extCollCode,&#xD;&#x9;&#x9;&#x9;T.EXT_COLL_NAME extCollName,&#xD;            A.DAYS_OVERDUE daysOverdue,&#xD;&#x9;&#x9;&#x9;A.LOC_NOTES locNotes,&#xD;&#x9;&#x9;&#x9;C.BRANCH_CODE branchAggr,&#xD;&#x9;&#x9;&#x9;BA.BRANCH_NAME branchNameAggr,&#xD;&#x9;&#x9;&#x9;A.KM_DEPOSIT kmDeposit,&#xD;&#x9;&#x9;&#x9;GP.PARAM_DESCRIP depositStsName,&#xD;&#x9;&#x9;&#x9;INV.TU_DEPOSIT_NO tuDepositInventory,&#xD;&#x9;&#x9;&#x9;INV.INVENTORY_STS inventorySts,&#xD;&#x9;&#x9;&#x9;C.WO_DATE woDate&#xD;&#x9;FROM TRX_TU_DEPOSIT A (NOLOCK)&#xD;&#x9;LEFT JOIN TRX_STTU B (NOLOCK)&#xD;&#x9;ON A.STTU_NO = B.STTU_NO&#xD;&#x9;LEFT JOIN TRX_AGGR C (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM D (NOLOCK)&#xD;&#x9;ON B.STTU_STS = D.PARAM_CODE AND D.CONDITION = &#39;STTU_STS&#39; AND D.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_AR_STS E (NOLOCK)&#xD;&#x9;ON C.AR_STS = E.AR_STS  AND ISNULL(E.SUB_STS, &#39;&#39;) = ISNULL (C.SUB_STS, &#39;&#39;)&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM F (NOLOCK)&#xD;&#x9;ON B.AGENT_TYPE = F.PARAM_CODE AND F.CONDITION = &#39;AGENT_TYPE&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM G (NOLOCK)&#xD;&#x9;ON A.SOLVING_TYPE = G.PARAM_CODE AND G.CONDITION = &#39;SOLVING_TYPE&#39; AND G.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET H (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = H.AGGR_NO&#xD;&#x9;LEFT JOIN MST_BRAND I (NOLOCK)&#xD;&#x9;ON H.BRAND_CODE = I.BRAND_CODE&#xD;&#x9;LEFT JOIN MST_TYPE J (NOLOCK)&#xD;&#x9;ON H.TYPE_CODE = J.TYPE_CODE&#xD;&#x9;LEFT JOIN MST_MODEL K (NOLOCK)&#xD;&#x9;ON H.MODEL_CODE = K.MODEL_CODE&#xD;&#x9;LEFT JOIN MST_KIND L (NOLOCK)&#xD;&#x9;ON J.KIND_CODE = L.KIND_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM M (NOLOCK)&#xD;&#x9;ON I.CATEGORY_CODE = M.PARAM_CODE AND M.CONDITION = &#39;CATEGORY_CODE&#39; AND M.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_COLOR N (NOLOCK)&#xD;&#x9;ON H.COLOR_CODE = N.COLOR_CODE&#xD;&#x9;LEFT JOIN TRX_TU_SOLVING O (NOLOCK)&#xD;&#x9;ON A.TU_DEPOSIT_NO = O.TU_DEPOSIT_NO AND A.SOLVING_TYPE = O.TU_SOLVING_TYPE AND A.SOLVING_DATE = O.TU_SOLVING_DATE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM P (NOLOCK)&#xD;&#x9;ON O.APPV_STS = P.PARAM_CODE AND P.CONDITION = &#39;STATUS_APPROVAL&#39; AND P.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_APPL Q (NOLOCK)&#xD;&#x9;ON C.APPL_NO = Q.APPL_NO&#xD;&#x9;LEFT JOIN MST_BRANCH R (NOLOCK)&#xD;&#x9;ON A.LOC_CODE = R.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH S (NOLOCK)&#xD;&#x9;ON A.LOC_RECEIPT_CODE = S.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_EXT_COLL T (NOLOCK)&#xD;&#x9;ON B.EXT_COLL_CODE = T.EXT_COLL_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE PIC1 (NOLOCK)&#xD;&#x9;ON B.PIC1_CODE = PIC1.NIK&#xD;&#x9;LEFT JOIN MST_EMPLOYEE PIC2 (NOLOCK)&#xD;&#x9;ON B.PIC2_CODE = PIC2.NIK&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM GP (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = GP.COMPANY_CODE AND A.DEPOSIT_STS = GP.PARAM_CODE AND GP.CONDITION = &#39;DEPOSIT_STS&#39; AND GP.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER CUS (NOLOCK)&#xD;&#x9;ON C.CUSTOMER_CODE = CUS.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CEO (NOLOCK)&#xD;&#x9;ON C.CUSTOMER_CODE = CEO.CUSTOMER_CODE AND CEO.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CEB (NOLOCK)&#xD;&#x9;ON C.CUSTOMER_CODE = CEB.CUSTOMER_CODE AND CEB.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND C.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM PT (NOLOCK)&#xD;&#x9;ON PT.CONDITION = &#39;PENALTY_TYPE&#39; AND PT.CHAR_VALUES = C.PRODUCT_CODE AND PT.PARAM_CODE = &#39;FLAT02&#39; AND PT.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN&#xD;&#x9;(&#xD;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;COUNT(INST_NO) as instNo,&#xD;&#x9;&#x9;&#x9;AGGR_NO aggrNo&#xD;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK)&#xD;&#x9;&#x9;WHERE PAYMENT_STS = &#39;UNPAID&#39;&#xD;&#x9;&#x9;GROUP BY COMPANY_CODE, AGGR_NO&#xD;&#x9;) ST ON A.COMPANY_CODE = ST.companyCode AND C.AGGR_NO = ST.aggrNo&#xD;&#x9;LEFT JOIN TRX_INVENTORY_TU INV (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = INV.COMPANY_CODE AND A.TU_DEPOSIT_NO = INV.TU_DEPOSIT_NO&#xD;&#x9;LEFT JOIN MST_BRANCH BR (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = BR.COMPANY_CODE AND A.BRANCH_CODE = BR.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH BA (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = BA.COMPANY_CODE AND C.BRANCH_CODE = BA.BRANCH_CODE&#xD;&#x9;WHERE C.AR_STS != &#39;LN&#39;&#xD;) a WHERE :search :filter</sql>
         <result element="count" rowName="" useColumnNumbers="false">
            <element column="total" name="total" xsdType="string"></element>
         </result>
         <param name="search" sqlType="QUERY_STRING"></param>
         <param name="filter" sqlType="QUERY_STRING"></param>
      </query>
      <query id="getDataTrxTuDeposit" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT companyCode, branchCode, branchName, tuDepositNo, tuDepositDate, executionDate, sttuNo, sttuDate, sttuSts, sttuStsName, pic1Code, pic2Code, agentType, agentTypeName, pic1Name, pic2Name, receiptFrom, &#xD;aggrNo, customerFullname, arSts, arStsName, categoryCode, categoryName, brandCode, brandName, typeCode, typeName, modelCode, modelName, kindCode, kindName, colorCode, colorName, nopol, chassisNo, machineNo, stnkName, balPrincipalValue, balIntValue,&#xD;instNo, balTenor, expiredDate, stnkExpiredDate, km, reqCostNo, tuCost, penaltyTuValue, notesPicReceive1, notesPicReceive2, locCode, locReceiptCode, printedBy, printedDate, appvSts, appvDate, solvingType,&#xD;solvingTypeName, solvingDate, solvingNotes, extCount, vehcOutDate, vehcOutBy, otrValue, taksasiValue, totMinusValue, totAddValue, depositSts, voucherNo, voucherDate, unitCondition, createdDate, appvStsSolving, appvStsNameSolving, yearMfg,&#xD;phoneArea1, phoneNo1, phoneArea2, phoneNo2, hpNo1, collFee, penaltyValue, dendaTarikUnit, locCodeName, locReceiptCodeName, getCollfee, (penalty + ((confirmDay + titipanDay) * simulasiPrice)) as getPenaltyValue, expiredDateSttu, getBalPrincipalValue, getBalIntValue, instTo, sisaTenor, productCode, bucketType, bucketCode, extCollCode, daysOverdue, locNotes, branchAggr, extCollName, kmDeposit, depositStsName, tuDepositInventory, inventorySts, woDate, branchNameAggr FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;BR.BRANCH_NAME branchName,&#xD;&#x9;&#x9;&#x9;A.TU_DEPOSIT_NO tuDepositNo,&#xD;&#x9;&#x9;&#x9;A.TU_DEPOSIT_DATE tuDepositDate,&#xD;&#x9;&#x9;&#x9;A.EXECUTION_DATE executionDate,&#xD;&#x9;&#x9;&#x9;A.STTU_NO sttuNo,&#xD;&#x9;&#x9;&#x9;B.STTU_DATE sttuDate,&#xD;&#x9;&#x9;&#x9;B.STTU_STS sttuSts,&#xD;&#x9;&#x9;&#x9;D.PARAM_DESCRIP sttuStsName,&#xD;&#x9;&#x9;&#x9;B.PIC1_CODE pic1Code,&#xD;&#x9;&#x9;&#x9;B.PIC2_CODE pic2Code,&#xD;&#x9;&#x9;&#x9;B.AGENT_TYPE agentType,&#xD;&#x9;&#x9;&#x9;F.PARAM_DESCRIP agentTypeName,&#xD;&#x9;&#x9;&#x9;PIC1.FULLNAME pic1Name,&#xD;&#x9;&#x9;&#x9;PIC2.FULLNAME pic2Name,&#xD;&#x9;&#x9;&#x9;A.RECEIPT_FROM receiptFrom,&#xD;&#x9;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;&#x9;C.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;&#x9;C.AR_STS arSts,&#xD;&#x9;&#x9;&#x9;E.TRX_NAME arStsName,&#xD;&#x9;&#x9;&#x9;I.CATEGORY_CODE categoryCode,&#xD;&#x9;&#x9;&#x9;M.PARAM_DESCRIP categoryName,&#xD;&#x9;&#x9;&#x9;H.BRAND_CODE brandCode,&#xD;&#x9;&#x9;&#x9;I.BRAND_NAME brandName,&#xD;&#x9;&#x9;&#x9;H.TYPE_CODE typeCode,&#xD;&#x9;&#x9;&#x9;J.TYPE_NAME typeName,&#xD;&#x9;&#x9;&#x9;H.MODEL_CODE modelCode,&#xD;&#x9;&#x9;&#x9;K.MODEL_NAME modelName,&#xD;&#x9;&#x9;&#x9;J.KIND_CODE kindCode,&#xD;&#x9;&#x9;&#x9;L.KIND_NAME kindName,&#xD;&#x9;&#x9;&#x9;H.COLOR_CODE colorCode,&#xD;&#x9;&#x9;&#x9;N.COLOR_NAME colorName,&#xD;&#x9;&#x9;&#x9;H.NOPOL nopol,&#xD;&#x9;&#x9;&#x9;H.CHASSIS_NO chassisNo,&#xD;&#x9;&#x9;&#x9;H.MACHINE_NO machineNo,&#xD;&#x9;&#x9;&#x9;H.BPKB_NAME stnkName,&#xD;&#x9;&#x9;&#x9;A.BAL_PRINCIPAL_VALUE balPrincipalValue,&#xD;&#x9;&#x9;&#x9;A.BAL_INT_VALUE balIntValue,&#xD;&#x9;&#x9;&#x9;A.INST_NO instNo,&#xD;&#x9;&#x9;&#x9;A.BAL_TENOR balTenor,&#xD;&#x9;&#x9;&#x9;A.EXPIRED_DATE expiredDate,&#xD;&#x9;&#x9;&#x9;A.STNK_EXPIRED_DATE stnkExpiredDate,&#xD;&#x9;&#x9;&#x9;A.KM km,&#xD;&#x9;&#x9;&#x9;A.REQ_COST_NO reqCostNo,&#xD;&#x9;&#x9;&#x9;A.TU_COST tuCost,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;select collFee from (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;select top 1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;coll_fee collFee, &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;EFFECTIVE_DATE effectiveDate&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;from mst_coll_fee (NOLOCK)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;where company_code = A.COMPANY_CODE AND product_code = C.PRODUCT_CODE AND CATEGORY_CODE = C.CATEGORY_CODE AND coll_kind_fee = &#39;ADMREP&#39; AND EFFECTIVE_DATE &lt;= GETDATE()&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;&#x9;&#x9;) a&#xD;&#x9;&#x9;&#x9;) as dendaTarikUnit,&#xD;&#x9;&#x9;&#x9;A.PENALTY_TU_VALUE penaltyTuValue,&#xD;&#x9;&#x9;&#x9;A.NOTES_PIC_RECEIVE1 notesPicReceive1,&#xD;&#x9;&#x9;&#x9;A.NOTES_PIC_RECEIVE2 notesPicReceive2,&#xD;&#x9;&#x9;&#x9;A.LOC_CODE locCode,&#xD;&#x9;&#x9;&#x9;A.LOC_RECEIPT_CODE locReceiptCode,&#xD;&#x9;&#x9;&#x9;A.PRINTED_BY printedBy,&#xD;&#x9;&#x9;&#x9;A.PRINTED_DATE printedDate,&#xD;&#x9;&#x9;&#x9;A.APPV_STS appvSts,&#xD;&#x9;&#x9;&#x9;A.APPV_DATE appvDate,&#xD;&#x9;&#x9;&#x9;A.SOLVING_TYPE solvingType,&#xD;&#x9;&#x9;&#x9;G.PARAM_DESCRIP solvingTypeName,&#xD;&#x9;&#x9;&#x9;A.SOLVING_DATE solvingDate,&#xD;&#x9;&#x9;&#x9;A.SOLVING_NOTES solvingNotes,&#xD;&#x9;&#x9;&#x9;A.EXT_COUNT extCount,&#xD;&#x9;&#x9;&#x9;A.VEHC_OUT_DATE vehcOutDate,&#xD;&#x9;&#x9;&#x9;A.VEHC_OUT_BY vehcOutBy,&#xD;&#x9;&#x9;&#x9;A.OTR_VALUE otrValue,&#xD;&#x9;&#x9;&#x9;A.TAKSASI_VALUE taksasiValue,&#xD;&#x9;&#x9;&#x9;A.TOT_MINUS_VALUE totMinusValue,&#xD;&#x9;&#x9;&#x9;A.TOT_ADD_VALUE totAddValue,&#xD;&#x9;&#x9;&#x9;A.DEPOSIT_STS depositSts,&#xD;&#x9;&#x9;&#x9;A.VOUCHER_NO voucherNo,&#xD;&#x9;&#x9;&#x9;A.VOUCHER_DATE voucherDate,&#xD;&#x9;&#x9;&#x9;A.UNIT_CONDITION unitCondition,&#xD;&#x9;&#x9;&#x9;A.CREATED_DATE createdDate,&#xD;&#x9;&#x9;&#x9;O.APPV_STS appvStsSolving,&#xD;&#x9;&#x9;&#x9;P.PARAM_DESCRIP appvStsNameSolving,&#xD;&#x9;&#x9;&#x9;H.YEAR_MFG yearMfg,&#xD;&#x9;&#x9;&#x9;C.CUSTOMER_CODE customerCode,&#xD;&#x9;&#x9;&#x9;C.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;P&#39; THEN CEO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea1,&#xD;&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;P&#39; THEN CEO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo1,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_AREA2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneArea2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.PHONE_NO2&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as phoneNo2,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;I&#39; OR C.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CUS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;P&#39; THEN CEO.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;K&#39; AND C.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN C.COLL_ADDRESS_TYPE = &#39;B&#39; THEN CEB.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;&#x9;A.COLL_FEE collFee,&#xD;&#x9;&#x9;&#x9;A.PENALTY_VALUE penaltyValue,&#xD;&#x9;&#x9;&#x9;R.BRANCH_NAME locCodeName,&#xD;&#x9;&#x9;&#x9;S.BRANCH_NAME locReceiptCodeName,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(sum(coll_fee-coll_fee_paid), 0) getCollfee&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) where company_code = A.COMPANY_CODE and aggr_no = C.AGGR_NO&#xD;&#x9;&#x9;&#x9;) as getCollfee,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;isnull(sum(pinalty_value-pinalty_paid), 0) penalty&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) where company_code = A.COMPANY_CODE and aggr_no = C.AGGR_NO&#xD;&#x9;&#x9;&#x9;) as penalty,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;VALUES_PARAM&#xD;&#x9;&#x9;&#x9;&#x9;FROM MST_GLOBAL_PARAM (NOLOCK) WHERE CONDITION = &#39;TU_CONFIRM_DATE&#39; AND CHAR_VALUES = C.PRODUCT_CODE AND FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;&#x9;) as confirmDay,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;VALUES_PARAM&#xD;&#x9;&#x9;&#x9;&#x9;FROM MST_GLOBAL_PARAM (NOLOCK) WHERE CONDITION = &#39;TU_INVENTORY&#39; AND CHAR_VALUES = C.PRODUCT_CODE AND FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;&#x9;) as titipanDay,&#xD;&#x9;&#x9;&#x9;PT.VALUES_PARAM simulasiPrice,&#xD;&#x9;&#x9;&#x9;B.EXPIRED_DATE expiredDateSttu,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SUM(PRINCIPAL_VALUE-PRINCIPAL_PAID) balPrincipalValue&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;) as getBalPrincipalValue,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;SUM(INT_VALUE-INT_PAID) balIntValue&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;) as getBalIntValue,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;convert(varchar(10),MAX(INST_NO)) &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) IS NULL THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MIN(INST_NO) instTo&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COMPANY_CODE = A.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MAX(INST_NO) instNo &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK) WHERE AGGR_NO = A.AGGR_NO AND PAYMENT_STS = &#39;UNPAID&#39; AND COLL_DUE_DATE &lt;= GETDATE() AND COMPANY_CODE = A.COMPANY_CODE)&#xD;&#x9;&#x9;&#x9;&#x9;END RESULT&#xD;&#x9;&#x9;&#x9;) as instTo,&#xD;&#x9;&#x9;&#x9;ISNULL(ST.instNo, 0) sisaTenor,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.BUCKET_TYPE bucketType,&#xD;&#x9;&#x9;&#x9;A.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;&#x9;A.EXT_COLL_CODE extCollCode,&#xD;&#x9;&#x9;&#x9;T.EXT_COLL_NAME extCollName,&#xD;            A.DAYS_OVERDUE daysOverdue,&#xD;&#x9;&#x9;&#x9;A.LOC_NOTES locNotes,&#xD;&#x9;&#x9;&#x9;C.BRANCH_CODE branchAggr,&#xD;&#x9;&#x9;&#x9;BA.BRANCH_NAME branchNameAggr,&#xD;&#x9;&#x9;&#x9;A.KM_DEPOSIT kmDeposit,&#xD;&#x9;&#x9;&#x9;GP.PARAM_DESCRIP depositStsName,&#xD;&#x9;&#x9;&#x9;INV.TU_DEPOSIT_NO tuDepositInventory,&#xD;&#x9;&#x9;&#x9;INV.INVENTORY_STS inventorySts,&#xD;&#x9;&#x9;&#x9;C.WO_DATE woDate&#xD;&#x9;FROM TRX_TU_DEPOSIT A (NOLOCK)&#xD;&#x9;LEFT JOIN TRX_STTU B (NOLOCK)&#xD;&#x9;ON A.STTU_NO = B.STTU_NO&#xD;&#x9;LEFT JOIN TRX_AGGR C (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM D (NOLOCK)&#xD;&#x9;ON B.STTU_STS = D.PARAM_CODE AND D.CONDITION = &#39;STTU_STS&#39; AND D.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_AR_STS E (NOLOCK)&#xD;&#x9;ON C.AR_STS = E.AR_STS  AND ISNULL(E.SUB_STS, &#39;&#39;) = ISNULL (C.SUB_STS, &#39;&#39;)&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM F (NOLOCK)&#xD;&#x9;ON B.AGENT_TYPE = F.PARAM_CODE AND F.CONDITION = &#39;AGENT_TYPE&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM G (NOLOCK)&#xD;&#x9;ON A.SOLVING_TYPE = G.PARAM_CODE AND G.CONDITION = &#39;SOLVING_TYPE&#39; AND G.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET H (NOLOCK)&#xD;&#x9;ON A.AGGR_NO = H.AGGR_NO&#xD;&#x9;LEFT JOIN MST_BRAND I (NOLOCK)&#xD;&#x9;ON H.BRAND_CODE = I.BRAND_CODE&#xD;&#x9;LEFT JOIN MST_TYPE J (NOLOCK)&#xD;&#x9;ON H.TYPE_CODE = J.TYPE_CODE&#xD;&#x9;LEFT JOIN MST_MODEL K (NOLOCK)&#xD;&#x9;ON H.MODEL_CODE = K.MODEL_CODE&#xD;&#x9;LEFT JOIN MST_KIND L (NOLOCK)&#xD;&#x9;ON J.KIND_CODE = L.KIND_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM M (NOLOCK)&#xD;&#x9;ON I.CATEGORY_CODE = M.PARAM_CODE AND M.CONDITION = &#39;CATEGORY_CODE&#39; AND M.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_COLOR N (NOLOCK)&#xD;&#x9;ON H.COLOR_CODE = N.COLOR_CODE&#xD;&#x9;LEFT JOIN TRX_TU_SOLVING O (NOLOCK)&#xD;&#x9;ON A.TU_DEPOSIT_NO = O.TU_DEPOSIT_NO AND A.SOLVING_TYPE = O.TU_SOLVING_TYPE AND A.SOLVING_DATE = O.TU_SOLVING_DATE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM P (NOLOCK)&#xD;&#x9;ON O.APPV_STS = P.PARAM_CODE AND P.CONDITION = &#39;STATUS_APPROVAL&#39; AND P.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_APPL Q (NOLOCK)&#xD;&#x9;ON C.APPL_NO = Q.APPL_NO&#xD;&#x9;LEFT JOIN MST_BRANCH R (NOLOCK)&#xD;&#x9;ON A.LOC_CODE = R.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH S (NOLOCK)&#xD;&#x9;ON A.LOC_RECEIPT_CODE = S.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_EXT_COLL T (NOLOCK)&#xD;&#x9;ON B.EXT_COLL_CODE = T.EXT_COLL_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE PIC1 (NOLOCK)&#xD;&#x9;ON B.PIC1_CODE = PIC1.NIK&#xD;&#x9;LEFT JOIN MST_EMPLOYEE PIC2 (NOLOCK)&#xD;&#x9;ON B.PIC2_CODE = PIC2.NIK&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM GP (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = GP.COMPANY_CODE AND A.DEPOSIT_STS = GP.PARAM_CODE AND GP.CONDITION = &#39;DEPOSIT_STS&#39; AND GP.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER CUS (NOLOCK)&#xD;&#x9;ON C.CUSTOMER_CODE = CUS.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CEO (NOLOCK)&#xD;&#x9;ON C.CUSTOMER_CODE = CEO.CUSTOMER_CODE AND CEO.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE CEB (NOLOCK)&#xD;&#x9;ON C.CUSTOMER_CODE = CEB.CUSTOMER_CODE AND CEB.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND C.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM PT (NOLOCK)&#xD;&#x9;ON PT.CONDITION = &#39;PENALTY_TYPE&#39; AND PT.CHAR_VALUES = C.PRODUCT_CODE AND PT.PARAM_CODE = &#39;FLAT02&#39; AND PT.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN&#xD;&#x9;(&#xD;&#x9;&#x9;SELECT&#xD;&#x9;&#x9;&#x9;COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;COUNT(INST_NO) as instNo,&#xD;&#x9;&#x9;&#x9;AGGR_NO aggrNo&#xD;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (NOLOCK)&#xD;&#x9;&#x9;WHERE PAYMENT_STS = &#39;UNPAID&#39;&#xD;&#x9;&#x9;GROUP BY COMPANY_CODE, AGGR_NO&#xD;&#x9;) ST ON A.COMPANY_CODE = ST.companyCode AND C.AGGR_NO = ST.aggrNo&#xD;&#x9;LEFT JOIN TRX_INVENTORY_TU INV (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = INV.COMPANY_CODE AND A.TU_DEPOSIT_NO = INV.TU_DEPOSIT_NO&#xD;&#x9;LEFT JOIN MST_BRANCH BR (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = BR.COMPANY_CODE AND A.BRANCH_CODE = BR.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH BA (NOLOCK)&#xD;&#x9;ON A.COMPANY_CODE = BA.COMPANY_CODE AND C.BRANCH_CODE = BA.BRANCH_CODE&#xD;&#x9;WHERE C.AR_STS != &#39;LN&#39;&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
