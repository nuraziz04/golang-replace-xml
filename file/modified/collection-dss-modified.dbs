      <query id="getCountMonitorLkh" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(*) as total FROM (&#xD;&#x9;SELECT distinct companyCode, branchCode, productCode, dkhNo, collCode, collHeadCode, dkhDate, hari, tanggal, jumlahPenugasanKelompok, jumlahRealisasiKelompok, concat(&#39;Kelompok &#39;, pencapaianKelompok, &#39; %, &#39;, &#39; Debitur &#39;, pencapaianDebitur, &#39; %&#39;) as pencapaian&#xD;&#x9;FROM (&#xD;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;A.PRODUCT_CODE productCode,&#xD;&#x9;&#x9;&#x9;A.DKH_NO dkhNo,&#xD;&#x9;&#x9;&#x9;A.COLL_CODE collCode,&#xD;&#x9;&#x9;&#x9;B.COLL_HEAD_CODE collHeadCode,&#xD;&#x9;&#x9;&#x9;B.BUCKET_CODE bucketCode,&#xD;&#x9;&#x9;&#x9;FORMAT(A.DKH_DATE, &#39;yyyy-MM&#39;) as dkhDate,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;case&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Sunday&#39; then &#39;Minggu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Monday&#39; then &#39;Senin&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Tuesday&#39; then &#39;Selasa&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Wednesday&#39; then &#39;Rabu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Thursday&#39; then &#39;Kamis&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Friday&#39; then &#39;Jumat&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;when datename(dw, A.DKH_DATE) = &#39;Saturday&#39; then &#39;Sabtu&#39;&#xD;&#x9;&#x9;&#x9;&#x9;end&#xD;&#x9;&#x9;&#x9;) as hari,&#xD;&#x9;&#x9;&#x9;FORMAT(A.DKH_DATE, &#39;dd&#39;) as tanggal,&#xD;&#x9;&#x9;&#x9;concat(A.QTY_GROUP, &#39; Kelompok, &#39;, A.QTY_CUSTOMER, &#39; Debitur&#39;) as jumlahPenugasanKelompok,&#xD;&#x9;&#x9;&#x9;concat(A.QTY_GROUP_CASH, &#39; Kelompok, &#39;, A.QTY_CUSTOMER_CASH, &#39; Debitur&#39;) as jumlahRealisasiKelompok,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.QTY_GROUP_CASH = &#39;0&#39; AND A.QTY_GROUP = &#39;0&#39; THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE (convert(float,A.QTY_GROUP_CASH) / convert(float,A.QTY_GROUP)) * 100&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as pencapaianKelompok,&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN A.QTY_CUSTOMER_CASH = &#39;0&#39; AND A.QTY_CUSTOMER = &#39;0&#39; THEN &#39;0&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE (convert(float,A.QTY_CUSTOMER_CASH) / convert(float,A.QTY_CUSTOMER)) * 100&#xD;&#x9;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;&#x9;) as pencapaianDebitur&#xD;&#x9;&#x9;FROM TRX_DKH_HDR A&#xD;&#x9;&#x9;JOIN MST_COLLECTOR B&#xD;&#x9;&#x9;ON A.COMPANY_CODE = B.COMPANY_CODE AND A.COLL_CODE = B.COLL_CODE AND BUCKET_TYPE = &#39;FCOL&#39;&#xD;&#x9;) a WHERE :search :filter&#xD;) t</sql>
         <result element="count" rowName="" useColumnNumbers="false">
