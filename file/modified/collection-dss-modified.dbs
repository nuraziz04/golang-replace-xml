      <query id="listMonitoringdebitur" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @BRANCH_CODE varchar(10) = :branchCode&#xD;declare @VALUESFROM numeric(5,0) = :valuesFrom&#xD;declare @VALUESTO numeric(5,0) = :valuesTo&#xD;&#xD;SELECT companyCode, branchCode, aggrNo, customerFullname, dueDate, tenor, dayOverdue, instFrom, instTo, installBill, penaltyValue, collFee, totBill, villageName FROM (&#xD;&#x9;SELECT&#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;A.AGGR_NO aggrNo,&#xD;&#x9;&#x9;C.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;A.DUE_DATE dueDate,&#xD;&#x9;&#x9;C.TENOR tenor,&#xD;&#x9;&#x9;A.DAY_OVERDUE dayOverdue,&#xD;&#x9;&#x9;A.INST_FROM instFrom,&#xD;&#x9;&#x9;A.INST_TO instTo,&#xD;&#x9;&#x9;A.INSTALL_BILL installBill,&#xD;&#x9;&#x9;A.PENALTY_VALUE penaltyValue,&#xD;&#x9;&#x9;A.COLL_FEE collFee,&#xD;&#x9;&#x9;A.TOT_BILL totBill,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN D.COLL_ADDRESS_TYPE = &#39;I&#39; THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Q.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM MST_CUSTOMER X &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN MST_VILLAGE Q&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON X.ID_VILLAGE_CODE = Q.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE X.CUSTOMER_CODE = C.CUSTOMER_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN D.COLL_ADDRESS_TYPE = &#39;D&#39; THEN (&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Q.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM MST_CUSTOMER X &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN MST_VILLAGE Q&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON X.DOMICILE_VILLAGE_CODE = Q.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE X.CUSTOMER_CODE = C.CUSTOMER_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN D.COLL_ADDRESS_TYPE = &#39;K&#39; THEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Q.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM MST_CUSTOMER_EMPLOYEE X &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN MST_VILLAGE Q&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON X.OFFICE_VILLAGE_CODE = Q.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE X.CUSTOMER_CODE = C.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;&#x9;WHEN D.COLL_ADDRESS_TYPE = &#39;B&#39; THEN &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Q.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM MST_CUSTOMER_EMPLOYEE X &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN MST_VILLAGE Q&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON X.OFFICE_VILLAGE_CODE = Q.VILLAGE_CODE&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE X.CUSTOMER_CODE = C.CUSTOMER_CODE AND X.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as villageName&#xD;&#x9;FROM TRX_DKH_DTL(nolock) A&#xD;&#x9;LEFT JOIN TRX_DKH_HDR(nolock) B&#xD;&#x9;ON A.DKH_NO = B.DKH_NO AND A.COMPANY_CODE = B.COMPANY_CODE AND A.BRANCH_CODE = B.BRANCH_CODE AND B.PRODUCT_CODE = &#39;PC03&#39; AND FORMAT(B.DKH_DATE, &#39;MM-yyyy&#39;) = FORMAT(GETDATE(), &#39;MM-yyyy&#39;)&#xD;&#x9;LEFT JOIN TRX_AGGR(nolock) C&#xD;&#x9;ON A.COMPANY_CODE = C.COMPANY_CODE AND A.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN TRX_APPL(nolock) D&#xD;&#x9;ON C.COMPANY_CODE = D.COMPANY_CODE AND C.APPL_NO = D.APPL_NO&#xD;&#x9;WHERE A.COMPANY_CODE = @COMPANY_CODE AND B.BRANCH_CODE = @BRANCH_CODE AND A.DAY_OVERDUE &gt;= @VALUESFROM AND A.DAY_OVERDUE &lt;= @VALUESTO&#xD;) a</sql>
         <result element="Entries" rowName="contents" useColumnNumbers="false">
            <element column="companyCode" name="companyCode" xsdType="string"></element>
            <element column="branchCode" name="branchCode" xsdType="string"></element>
            <element column="aggrNo" name="aggrNo" xsdType="string"></element>
            <element column="customerFullname" name="customerFullname" xsdType="string"></element>
            <element column="dueDate" name="dueDate" xsdType="string"></element>
            <element column="tenor" name="tenor" xsdType="string"></element>
            <element column="dayOverdue" name="dayOverdue" xsdType="string"></element>
            <element column="instFrom" name="instFrom" xsdType="string"></element>
            <element column="instTo" name="instTo" xsdType="string"></element>
            <element column="installBill" name="installBill" xsdType="string"></element>
            <element column="penaltyValue" name="penaltyValue" xsdType="string"></element>
            <element column="collFee" name="collFee" xsdType="string"></element>
            <element column="totBill" name="totBill" xsdType="string"></element>
            <element column="villageName" name="villageName" xsdType="string"></element>
         </result>
