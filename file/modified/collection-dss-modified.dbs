      <query id="insertTrxRequestCm" returnUpdatedRowCount="true" useConfig="UI_SUBMIT">
         <sql>declare @TYPE_COST varchar(50) = :typeCost&#xD;declare @STTU_NO varchar(50) = :sttuNo&#xD;declare @TRX_TYPE_CODE varchar(50) = &#39;RCAHO&#39;&#xD;declare @REQUEST_NOTE varchar(50) =  case&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when @TYPE_COST = &#39;001&#39; then &#39;Pengajuan Biaya Tarik Unit&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE &#39;Klaim Biaya Tarik Unit&#39;&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; end&#xD;declare @COMPANY_CODE varchar(50) = :companyCode&#xD;declare @BRANCH_CODE varchar(50) = :branchCode&#xD;declare @REQUEST_NO varchar(50) = :requestNo&#xD;declare @REQUEST_VALUE varchar(50) = :requestValue&#xD;declare @REFNO varchar(100) = :refNo&#xD;declare @isDuplikasi int&#xD;&#xD;set @isDuplikasi = (select count(*) from trx_request_cm_hdr where REF_NO=@REFNO and COMPANY_CODE = @company_code)&#xD;if @isDuplikasi = 0&#xD;begin&#xD;&#xD;insert into trx_request_cm_hdr&#xD;(&#xD;&#x9;company_code,&#xD;&#x9;branch_code,&#xD;&#x9;request_no,&#xD;&#x9;trx_type_code,&#xD;&#x9;request_date,&#xD;&#x9;expense_group_code,&#xD;&#x9;ref_no,&#xD;&#x9;payment_type,&#xD;&#x9;request_value,&#xD;&#x9;pic_code,&#xD;&#x9;product_code,&#xD;&#x9;request_position,&#xD;&#x9;request_nik,&#xD;&#x9;request_name,&#xD;&#x9;request_sts,&#xD;&#x9;request_note,&#xD;&#x9;appv_sts,&#xD;&#x9;br_to,&#xD;&#x9;created_date,&#xD;&#x9;created_by&#xD;)&#xD;values&#xD;(&#xD;&#x9;@COMPANY_CODE,&#xD;&#x9;@BRANCH_CODE,&#xD;&#x9;@REQUEST_NO,&#xD;&#x9;@TRX_TYPE_CODE,&#xD;&#x9;getdate(),&#xD;&#x9;&#39;RTN&#39;,&#xD;&#x9;@REFNO,&#xD;&#x9;&#39;CASH&#39;,&#xD;&#x9;@REQUEST_VALUE,&#xD;&#x9;:picCode,&#xD;&#x9;:productCode,&#xD;&#x9;:requestPosition,&#xD;&#x9;:requestNik,&#xD;&#x9;:requestName,&#xD;&#x9;&#39;BVLD&#39;,&#xD;&#x9;concat(@REQUEST_NOTE, &#39; &#39;, @STTU_NO),&#xD;&#x9;&#39;APPD&#39;,&#xD;&#x9;:brTo,&#xD;&#x9;getdate(),&#xD;&#x9;&#39;SYSTEM&#39;&#xD;)&#xD;&#xD;insert into trx_request_cm_dtl&#xD;(&#xD;&#x9;company_code,&#xD;&#x9;branch