A.COMPANY_CODE = F.COMPANY_CODE AND A.COLL_TIME = F.PARAM_CODE AND F.CONDITION = &#39;VISIT_TIME&#39; AND F.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;&#x9;LEFT JOIN MST_BRANCH BR (nolock) ON A.BRANCH_CODE = BR.BRANCH_CODE AND A.COMPANY_CODE = BR.COMPANY_CODE&#xD;&#x9;&#x9;left join mst_coll_day_hdr cd (nolock)&#xD;&#x9;&#x9;on A.coll_day_code = cd.coll_day_code and cd.flag_active = &#39;1&#39; and cd.product_code = BR.product_code and a.flag_revitalization=&#39;1&#39;&#xD;&#x9;&#x9;left join mst_coll_day_dtl cdtl (nolock)&#xD;&#x9;&#x9;on cd.coll_day_code = cdtl.coll_day_code and cdtl.flag_active = &#39;1&#39; and cdtl.product_code = cd.product_code&#xD;&#x9;&#x9;WHERE A.GROUP_STS = &#39;1&#39;&#xD;&#x9;) A &#xD;) a WHERE :search :filter</sql>
