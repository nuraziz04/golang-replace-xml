      <query id="updateBucketTrxAggr" returnUpdatedRowCount="true" useConfig="UI_SUBMIT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @BRANCH_CODE varchar(10) = :branchCode&#xD;declare @AGGR_NO varchar(50) = :aggrNo&#xD;&#xD;UPDATE TRX_AGGR SET&#xD;&#x9;BUCKET_CODE = :bucketCode,&#xD;&#x9;UPDATE_DATE = GETDATE(),&#xD;&#x9;UPDATE_BY = :updateBy&#xD;WHERE &#xD;&#x9;COMPANY_CODE = @COMPANY_CODE AND&#xD;&#x9;BRANCH_CODE = @BRANCH_CODE AND&#xD;&#x9;AGGR_NO = @AGGR_NO</sql>
         <result element="UpdatedRowCount" rowName="" useColumnNumbers="true">
            <element column="1" name="Value" xsdType="integer"></element>
         </result>
         <param name="bucketCode" sqlType="STRING"></param>
         <param name="updateBy" sqlType="STRING"></param>
         <param name="companyCode" sqlType="STRING"></param>
         <param name="branchCode" sqlType="STRING"></param>
         <param name="aggrNo" sqlType="STRING"></param>
      </query>
