 A.PRODUCT_CODE = H.PRODUCT_CODE&#xD;&#x9;LEFT JOIN MST_TOTAL_AGGR I&#xD;&#x9;ON A.COMPANY_CODE = I.COMPANY_CODE AND A.BRANCH_CODE = I.BRANCH_CODE AND A.VILLAGE_CODE = I.VILLAGE_CODE&#xD;&#x9;WHERE A.COMPANY_CODE = @COMPANY_CODE AND A.BRANCH_CODE = @BRANCH_CODE AND A.PRODUCT_CODE = @PRODUCT_CODE AND A.BUCKET_CODE = @BUCKET_CODE AND A.EFFECTIVE_DATE = @EFFECTIVE_DATE&#xD;&#xD;&#x9;UNION ALL&#xD;&#xD;&#x9;select DISTINCT&#xD;&#x9;&#x9;A.company_code companyCode, &#xD;&#x9;&#x9;(select &#39;&#39;) as branchCode,&#xD;&#x9;&#x9;(select &#39;&#39;) as branchName,&#xD;&#x9;&#x9;A.product_code productCode,&#xD;&#x9;&#x9;C.param_descrip productName,&#xD;&#x9;&#x9;(select &#39;&#39;) as bucketCode,&#xD;&#x9;&#x9;(select &#39;&#39;) as bucketName,&#xD;&#x9;&#x9;(select &#39;&#39;) as effectiveDate,&#xD;&#x9;&#x9;A.village_code villageCode, &#xD;&#x9;&#x9;B.village_name villageName,&#xD;&#x9;&#x9;D.subdistrict_name subdistrictName,&#xD;&#x9;&#x9;ISNULL(E.tot_aggr, 0) totalAggr,&#xD;&#x9;&#x9;(select 0) as qtyAggrNo,&#xD;&#x9;&#x9;(select &#39;&#39;) as collCode,&#xD;&#x9;&#x9;(select &#39;&#39;) as fullnameColl,&#xD;&#x9;&#x9;(select &#39;&#39;) as collCodeDay,&#xD;&#x9;&#x9;(select &#39;&#39;) as collCodeDay&#xD;&#x9;from MST_COVER_AREA_DTL A&#xD;&#x9;LEFT JOIN MST_VILLAGE B &#xD;&#x9;ON A.VILLAGE_CODE=B.VILLAGE_CODE &#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM C &#xD;&#x9;ON A.PRODUCT_CODE=C.PARAM_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT D&#xD;&#x9;ON B.SUBDISTRICT_CODE = D.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_TOTAL_AGGR E&#xD;&#x9;ON A.COMPANY_CODE = E.COMPANY_CODE AND A.BRANCH_CODE = E.BRANCH_CODE AND A.VILLAGE_CODE = E.VILLAGE_CODE&#xD;&#x9;JOIN MST_COVER_AREA_HDR HDR&#xD;&#x9;ON A.COMPANY_CODE = HDR.COMPANY_CODE AND A.BRANCH_CODE = HDR.BRANCH_CODE AND HDR.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;WHERE A.VILLAGE_CODE NOT IN (SELECT VILLAGE_CODE FROM MST_COLL_AREA_DTL CA WHERE CA.COMPANY_CODE = @COMPANY_CODE AND CA.BRANCH_CODE = @BRANCH_CODE AND CA.PRODUCT_CODE = @PRODUCT_CODE AND CA.BUCKET_CODE = @BUCKET_CODE AND CA.EFFECTIVE_DATE = @EFFECTIVE_DATE) AND A.PRODUCT_CODE = @PRODUCT_CODE AND A.BRANCH_CODE IN (select BRANCH_CODE from getViewBranch() where BRANCH_CODE = A.BRANCH_CODE AND DSO_CODE = @BRANCH_CODE)&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
