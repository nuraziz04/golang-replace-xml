9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY EFFECTIVE_DATE DESC &#xD;&#x9;&#x9;) as marketModelCode,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.NEW_USED&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketNewUsed,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.YEAR_MFG&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketYearMfg,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT TOP 1&#xD;&#x9;&#x9;&#x9;&#x9;PC.EFFECTIVE_DATE&#xD;&#x9;&#x9;&#x9;FROM MST_PRICE PC (nolock)&#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_HDR PCH (nolock)&#xD;&#x9;&#x9;&#x9;ON PC.PRICE_AREA_CODE = PCH.PRICE_AREA_CODE AND PC.COMPANY_CODE = PCH.COMPANY_CODE &#xD;&#x9;&#x9;&#x9;LEFT JOIN MST_PRICE_AREA_DTL PCD (nolock)&#xD;&#x9;&#x9;&#x9;ON PCH.PRICE_AREA_CODE = PCD.PRICE_AREA_CODE AND PCH.COMPANY_CODE = PCD.COMPANY_CODE&#xD;&#x9;&#x9;&#x9;WHERE PC.COMPANY_CODE = C.COMPANY_CODE AND PC.MODEL_CODE = C.MODEL_CODE AND PCD.BRANCH_CODE = C.BRANCH_CODE AND&#xD;&#x9;&#x9;&#x9;PC.YEAR_MFG = C.YEAR_MFG AND PC.NEW_USED = &#39;U&#39; AND PC.EFFECTIVE_DATE &lt;= GETDATE() ORDER BY EFFECTIVE_DATE DESC&#xD;&#x9;&#x9;) as marketEffectiveDate,&#xD;&#x9;&#x9;N.DSO_CODE dsoCode,&#xD;&#x9;&#x9;(&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN P.AGGR_NO IS NULL THEN &#39;Tidak&#39;&#xD;&#x9;&#x9;&#x9;&#x9;ELSE &#39;Iya&#39;&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as stsOutUnit&#xD;&#x9;FROM TRX_INVENTORY_TU A (nolock)&#xD;&#x9;LEFT JOIN TRX_AGGR B (nolock)&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET C (nolock)&#xD;&#x9;ON B.AGGR_NO = C.AGGR_NO&#xD;&#x9;LEFT JOIN MST_BRAND D (nolock)&#xD;&#x9;ON C.BRAND_CODE = D.BRAND_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM E (nolock)&#xD;&#x9;ON D.CATEGORY_CODE = E.PARAM_CODE AND E.CONDITION = &#39;CATEGORY_CODE&#39; AND E.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_TYPE F (nolock)&#xD;&#x9;ON C.TYPE_CODE = F.TYPE_CODE&#xD;&#x9;LEFT JOIN MST_KIND G (nolock)&#xD;&#x9;ON F.KIND_CODE = G.KIND_CODE&#xD;&#x9;LEFT JOIN MST_MODEL H (nolock)&#xD;&#x9;ON C.MODEL_CODE = H.MODEL_CODE&#xD;&#x9;LEFT JOIN MST_COLOR I (nolock)&#xD;&#x9;ON C.COLOR_CODE = I.COLOR_CODE&#xD;&#x9;JOIN TRX_TU_DEPOSIT J (nolock)&#xD;&#x9;ON A.TU_DEPOSIT_NO = J.TU_DEPOSIT_NO&#xD;&#x9;LEFT JOIN MST_BRANCH LRC (nolock)&#xD;&#x9;ON J.LOC_RECEIPT_CODE = LRC.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_BRANCH LC (nolock)&#xD;&#x9;ON J.LOC_CODE = LC.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_AR_STS K (nolock)&#xD;&#x9;ON B.AR_STS = K.AR_STS AND ISNULL(B.SUB_STS, &#39;&#39;) = ISNULL(K.SUB_STS, &#39;&#39;)&#xD;&#x9;LEFT JOIN MST_BPKB L (nolock)&#xD;&#x9;ON A.COMPANY_CODE = L.COMPANY_CODE AND A.BRANCH_CODE = L.BRANCH_CODE AND A.AGGR_NO = L.AGGR_NO AND C.BPKB_NO = L.BPKB_NO &#xD;&#x9;LEFT JOIN MST_BRANCH N (nolock)&#xD;&#x9;ON A.COMPANY_CODE = N.COMPANY_CODE AND A.BRANCH_CODE = N.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM O (nolock)&#xD;&#x9;ON L.COMPANY_CODE = O.COMPANY_CODE AND L.BPKB_STS = O.PARAM_CODE AND O.CONDITION = &#39;BPKB_STS&#39; AND O.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN TRX_TU_OUT_DTL P (nolock)&#xD;&#x9;ON A.COMPANY_CODE = P.COMPANY_CODE AND A.AGGR_NO = P.AGGR_NO&#xD;&#x9;LEFT JOIN TRX_AGGR_SCHEDULE SC (nolock)&#xD;&#x9;ON B.COMPANY_CODE = SC.COMPANY_CODE AND B.AGGR_NO = SC.AGGR_NO&#xD;&#x9;WHERE B.AR_STS != &#39;LN&#39; AND ISNULL(SC.AGGR_NO, &#39;&#39;) != &#39;&#39;&#xD;) a WHERE :search :filter ORDER BY :orderBy :direction OFFSET :page ROWS FETCH NEXT :limit ROWS ONLY</sql>
