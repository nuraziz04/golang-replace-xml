      <query id="allMstUtj" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>declare @COMPANY_CODE varchar(10) = :companyCode&#xD;declare @BRANCH_CODE varchar(10) = :branchCode&#xD;declare @DKH_NO varchar(50) = :dkhNo&#xD;declare @AGGR_NO varchar(50) = :aggrNo&#xD;declare @GROUP_CODE varchar(50) = :groupCode&#xD;&#xD;select companyCode,branchCode,dkhNo,groupCode,aggr_No aggrNoRecipient,aggrNoDonor aggrNo,&#xD;customerFullname,utjBalanceDonor,utjUsageValueRecipient bookValue,utjBalanceDonor - isnull(b.utjUsageValueTotalDonor,0) sisaUtj&#xD;from &#xD;(&#xD;&#x9;select distinct &#xD;&#x9;a.companyCode&#xD;&#x9;, a.branchCode&#xD;&#x9;, a.dkhNo&#xD;&#x9;, a.groupCode&#xD;&#x9;, a.aggr_No&#xD;&#x9;, a.customerFullname&#xD;&#x9;, a.aggrNoDonor&#xD;&#x9;,c.utjBalanceDonor&#xD;&#x9;, a.utjUsageValueRecipient&#xD;&#x9; &#xD;&#x9;from &#xD;&#x9;(&#x9;&#xD;&#x9;&#x9;select distinct&#xD;&#x9;&#x9;&#x9;a.companyCode&#xD;&#x9;&#x9;&#x9;, a.branchCode&#xD;&#x9;&#x9;&#x9;, a.dkhNo&#xD;&#x9;&#x9;&#x9;, a.groupCode&#xD;&#x9;&#x9;&#x9;,a.aggr_no&#xD;&#x9;&#x9;&#x9;,a.customerFullname&#xD;&#x9;&#x9;&#x9;,a.aggrNoDonor&#xD;&#x9;&#x9;&#x9;,isnull(b.utjUsageValueRecipient,0) utjUsageValueRecipient&#xD;&#x9;&#x9;from&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT distinct &#xD;&#x9;&#x9;&#x9;&#x9;c.company_Code companyCode&#xD;&#x9;&#x9;&#x9;&#x9;, c.branch_Code branchCode&#xD;&#x9;&#x9;&#x9;&#x9;, c.dkh_No dkhNo&#xD;&#x9;&#x9;&#x9;&#x9;, c.group_Code groupCode&#xD;&#x9;&#x9;&#x9;&#x9;,c.aggr_no&#xD;&#x9;&#x9;&#x9;&#x9;,b.aggr_no aggrNoDonor&#xD;&#x9;&#x9;&#x9;&#x9;,d.customer_Fullname customerFullname&#xD;&#x9;&#x9;&#x9;FROM trx_dkh_dtl c (nolock)&#xD;&#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;left join trx_dkh_dtl b (nolock)&#xD;&#x9;&#x9;&#x9;&#x9;on b.company_code = c.company_code&#xD;&#x9;&#x9;&#x9;&#x9;and b.dkh_no=c.dkh_no&#xD;&#x9;&#x9;&#x9;&#x9;and b.group_code = c.group_code&#xD;&#x9;&#x9;&#x9;LEFT JOIN TRX_AGGR d (nolock)&#xD;&#x9;&#x9;&#x9;&#x9;ON d.company_code = b.company_code&#xD;&#x9;&#x9;&#x9;&#x9;and d.AGGR_NO = b.AGGR_NO&#xD;&#x9;&#x9;&#x9;) as a&#xD;&#x9;&#x9;left join&#xD;&#x9;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;&#x9;SELECT distinct &#xD;&#x9;&#x9;&#x9;&#x9;&#x9;a.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;a.BRANCH_CODE branchCode,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;a.DKH_NO dkhNo,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;A.AGGR_NO_RECIPIENT aggrNoRecipient,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;a.utj_usage_value utjUsageValueRecipient,&#xD;&#x9;&#x9;&#x9;&#x9;&#x9;a.aggr_no_donor aggrNoDonor&#xD;&#x9;&#x9;&#x9;&#x9;FROM TRX_DKH_DTL_UTJ A (nolock)&#x9; &#x9;&#x9;&#x9;&#xD;&#x9;&#x9;&#x9;) b&#xD;&#x9;&#x9;&#x9;&#x9;on b.companyCode = a.companyCode&#xD;&#x9;&#x9;&#x9;&#x9;and b.dkhNo = a.dkhNo&#xD;&#x9;&#x9;&#x9;&#x9;and a.aggr_no = b.aggrNoRecipient&#xD;&#x9;&#x9;&#x9;&#x9;and b.aggrNoDonor = a.aggrNodonor&#xD;&#xD;&#x9;) as a&#xD;&#x9;left join &#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;select company_code,dkh_no,group_code,aggr_no,utj_balance utjBalanceDonor&#xD;&#x9;&#x9;&#x9;from &#xD;&#x9;&#x9;&#x9;&#x9;trx_dkh_dtl b (nolock)&#xD;&#x9;) c&#xD;&#x9;&#x9;on c.company_code = a.companyCode&#xD;&#x9;&#x9;and c.dkh_no = a.dkhNo&#xD;&#x9;&#x9;and c.aggr_no = a.aggrNoDonor&#xD;&#xD;) as a&#xD;left join &#xD;&#x9;(&#xD;&#x9;&#x9;select &#xD;&#x9;&#x9;&#x9;b.company_code, b.dkh_no, b.aggr_no_donor, sum(b.utj_usage_value) utjUsageValueTotalDonor&#xD;&#x9;&#x9;from trx_dkh_dtl_utj b (nolock)&#xD;&#x9;&#x9;group by b.company_code, b.dkh_no, b.aggr_no_donor&#xD;&#x9;) b&#xD;&#x9;on a.companyCode = b.company_code&#xD;&#x9;and a.dkhNo = b.Dkh_No&#xD;&#x9;and a.aggrNoDonor = b.aggr_No_Donor&#xD;WHERE &#xD;&#x9;companyCode = @COMPANY_CODE and&#xD;&#x9;branchCode = @BRANCH_CODE and&#xD;&#x9;dkhNo = @DKH_NO and&#xD;&#x9;aggr_No = @AGGR_NO and&#xD;&#x9;groupCode = @GROUP_CODE</sql>
