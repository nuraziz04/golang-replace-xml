      <query id="getCountLovCabangAsalMutasi" returnUpdatedRowCount="false" useConfig="UI_SELECT">
         <sql>SELECT COUNT(0) total FROM (&#xD;SELECT DISTINCT companyCode, branchCode, branchName FROM (&#xD;&#x9;SELECT &#xD;&#x9;&#x9;A.COMPANY_CODE companyCode,&#xD;&#x9;&#x9;A.LOC_CODE branchCode,&#xD;&#x9;&#x9;BR.BRANCH_NAME branchName,&#xD;&#x9;&#x9;--B.BRANCH_CODE branchCodeAggr,&#xD;&#x9;&#x9;(select B.BRANCH_CODE from trx_aggr B (nolock) where A.AGGR_NO = B.AGGR_NO AND A.COMPANY_CODE=B.COMPANY_CODE) branchCodeAggr,&#xD;&#x9;&#x9;A.TU_DEPOSIT_NO tuDepositNo,&#xD;&#x9;&#x9;A.DEPOSIT_STS depositSts,&#xD;&#x9;&#x9;ISNULL(J.INVENTORY_STS, &#39;&#39;) inventorySts&#xD;&#x9;FROM TRX_TU_DEPOSIT A (nolock)&#xD;&#x9;--LEFT JOIN TRX_AGGR B&#xD;&#x9;--ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN TRX_INVENTORY_TU J (nolock)&#xD;&#x9;ON A.TU_DEPOSIT_NO = J.TU_DEPOSIT_NO&#xD;&#x9;LEFT JOIN MST_BRANCH BR (nolock)&#xD;&#x9;ON A.COMPANY_CODE = BR.COMPANY_CODE AND A.LOC_CODE = BR.BRANCH_CODE&#xD;&#x9;WHERE NOT EXISTS (SELECT 1 FROM TRX_TU_MUTASI_DTL DTL JOIN TRX_TU_MUTASI_HDR HDR ON DTL.TRX_NO = HDR.TRX_NO WHERE DTL.TU_DEPOSIT_NO=A.TU_DEPOSIT_NO AND HDR.TRX_STS IN (&#39;001&#39;,&#39;002&#39;)) AND A.DEPOSIT_STS IN (&#39;1&#39;,&#39;2&#39;,&#39;3&#39;,&#39;4&#39;) AND ISNULL(J.INVENTORY_STS, &#39;&#39;) NOT IN (&#39;3&#39;,&#39;4&#39;) AND ISNULL(A.LOC_CODE, &#39;&#39;) != &#39;&#39;&#xD;) a WHERE :search :filter&#xD;) b</sql>
