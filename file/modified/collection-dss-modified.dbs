      </query>
      <query id="updateAppvStsSttuCost" returnUpdatedRowCount="true" useConfig="UI_SUBMIT">
         <sql>DECLARE @REQ_COST_NO varchar(50) = :reqCostNo&#xD;&#xD;UPDATE TRX_STTU_COST_HDR SET&#xD;&#x9;REQ_COST_STS = :appvsts,&#xD;&#x9;REQ_COST_STS_DATE = GETDATE(),&#xD;&#x9;APPV_DATE = GETDATE()&#xD;WHERE&#xD;&#x9;COMPANY_CODE = &#39;EDV&#39; AND&#xD;&#x9;REQ_COST_NO = @REQ_COST_NO</sql>
