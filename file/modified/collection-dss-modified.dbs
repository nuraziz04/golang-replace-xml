NO spNo,&#xD;&#x9;&#x9;A.SP_TYPE spType,&#xD;&#x9;&#x9;A.SP_RECEIPT_TYPE spReceiptType,&#xD;&#x9;&#x9;A.SP_RECEIPT_NAME spReceiptName,&#xD;&#x9;&#x9;A.SP_NOTES spNotes,&#xD;&#x9;&#x9;A.STTU_NO sttuNo,&#xD;&#x9;&#x9;A.COLL_NOTES collNotes,&#xD;&#x9;&#x9;A.FLAG_SEND flagSend,&#xD;&#x9;&#x9;A.FLAG_SEND_DATE flagSendDate,&#xD;&#x9;&#x9;A.LONGTITUDE longtitude,&#xD;&#x9;&#x9;A.LATITUDE latitude,&#xD;&#x9;&#x9;A.CASHIER_CODE cashierCode,&#xD;&#x9;&#x9;A.BILL_STS billSts,&#xD;&#x9;&#x9;B.CUSTOMER_FULLNAME customerFullname,&#xD;&#x9;&#x9;M.COLL_ADDRESS_TYPE collAddressType,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_ADDRESS&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as address,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.HP_NO&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.HP_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.HP_NO&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as hpNo1,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.PHONE_AREA1&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneArea1,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.PHONE_NO1&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.PHONE_NO1&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneNo1,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_AREA2&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneArea2,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; OR B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.PHONE_NO2&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as phoneNo2,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RT &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RT&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RT&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rt,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;CASE &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN CS.ID_RW &#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN CS.DOMICILE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN F.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN CO.OFFICE_RW&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN G.OFFICE_RW&#xD;&#x9;&#x9;&#x9;END AS RESULT&#xD;&#x9;&#x9;) as rw,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.VILLAGE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as villageName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN ISUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BSUB.SUBDISTRICT_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as subdistrictName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COVC.CITY_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BC.CITY_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as cityName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BP.PROVINCE_NAME&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as provinceName,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;I&#39; THEN IV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;D&#39; THEN DV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;P&#39; THEN KV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;K&#39; AND B.CUSTOMER_TYPE = &#39;C&#39; THEN COV.ZIPCODE&#xD;&#x9;&#x9;&#x9;&#x9;WHEN B.COLL_ADDRESS_TYPE = &#39;B&#39; THEN BV.ZIPCODE&#xD;&#x9;&#x9;&#x9;END&#xD;&#x9;&#x9;) as zipCode,&#xD;&#x9;&#x9;H.COLL_CODE collCode,&#xD;&#x9;&#x9;I.FULLNAME collName,&#xD;&#x9;&#x9;--K.PARAM_DESCRIP spTypeName,&#xD;&#x9;&#x9;(SELECT K.PARAM_DESCRIP FROM MST_GLOBAL_PARAM K (nolock) WHERE A.SP_TYPE = K.PARAM_CODE AND K.CONDITION = &#39;SP_TYPE&#39; AND K.FLAG_ACTIVE = &#39;1&#39; ) spTypeName,&#xD;&#x9;&#x9;N.NOPOL nopol,&#xD;&#x9;&#x9;A.CUSTOMER_PAYMENT_DATE customerPaymentDate,&#xD;&#x9;&#x9;O.BRANCH_NAME branchName,&#xD;&#x9;&#x9;B.TENOR&#x9;tenor,&#xD;&#x9;&#x9;(&#xD;&#x9;&#x9;&#x9;SELECT &#xD;&#x9;&#x9;&#x9;&#x9;SUM( isnull((COLL_FEE), 0) - isnull((COLL_FEE_PAID), 0) )&#xD;&#x9;&#x9;&#x9;&#x9;total&#xD;&#x9;&#x9;&#x9;FROM TRX_AGGR_SCHEDULE (nolock) WHERE AGGR_NO = A.AGGR_NO AND COMPANY_CODE = A.COMPANY_CODE&#xD;&#x9;&#x9;) as collFeeSchedule,&#xD;&#x9;&#x9;A.VISIT_TIME_VALUE visitTimeValue,&#xD;&#x9;&#x9;A.TU_TIME_VALUE tuTimeValue,&#xD;&#x9;&#x9;ISNULL(A.VOUCHER_NO, &#39;&#39;) voucherNo,&#xD;&#x9;&#x9;A.RATING_CODE ratingCode,&#xD;&#x9;&#x9;A.TEAM_NO teamNo,&#xD;&#x9;&#x9;P.PARAM_DESCRIP billStsName,&#xD;&#x9;&#x9;GH.GROUP_CODE positionGroupCode,&#xD;&#x9;&#x9;CASE&#xD;&#x9;&#x9;&#x9;WHEN ISNULL(GH.GROUP_CODE, &#39;&#39;) != &#39;&#39; THEN (SELECT P1.PARAM_DESCRIP FROM MST_GLOBAL_PARAM P1 (nolock) WHERE A.COMPANY_CODE = P1.COMPANY_CODE AND P1.PARAM_CODE = &#39;P1&#39; AND P1.CONDITION = &#39;POSITION_GROUP_CODE&#39;)&#xD;&#x9;&#x9;&#x9;WHEN ISNULL(GH.GROUP_CODE, &#39;&#39;) = &#39;&#39; THEN (SELECT P2.PARAM_DESCRIP FROM MST_GLOBAL_PARAM P2 (nolock) WHERE A.COMPANY_CODE = P2.COMPANY_CODE AND P2.PARAM_CODE = &#39;P2&#39; AND P2.CONDITION = &#39;POSITION_GROUP_CODE&#39;)&#xD;&#x9;&#x9;END positionGroupName,&#xD;&#x9;&#x9;H.DKH_DATE dkhDate,&#xD;&#x9;&#x9;ATT.ATT_NAME attDesc,&#xD;&#x9;&#x9;DGR.COLL_TIME waktuDebitur&#xD;&#x9;FROM TRX_DKH_DTL A (nolock)&#xD;&#x9;LEFT JOIN TRX_AGGR B (nolock)&#xD;&#x9;ON A.AGGR_NO = B.AGGR_NO&#xD;&#x9;LEFT JOIN MST_GROUP_HDR GH (nolock)&#xD;&#x9;ON A.GROUP_CODE = GH.GROUP_CODE AND B.APPL_NO = GH.GROUP_LEADER&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM P1 (nolock)&#xD;&#x9;--ON A.COMPANY_CODE = P1.COMPANY_CODE AND P1.PARAM_CODE = &#39;P1&#39; AND P1.CONDITION = &#39;POSITION_GROUP_CODE&#39;&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM P2 (nolock)&#xD;&#x9;--ON A.COMPANY_CODE = P2.COMPANY_CODE AND P2.PARAM_CODE = &#39;P2&#39; AND P2.CONDITION = &#39;POSITION_GROUP_CODE&#39;&#xD;&#x9;LEFT JOIN MST_CUSTOMER C (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = C.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN TRX_DKH_HDR H (nolock)&#xD;&#x9;ON A.DKH_NO = H.DKH_NO&#xD;&#x9;LEFT JOIN TRX_DKH_GROUP DGR (nolock)&#xD;&#x9;ON A.COMPANY_CODE = DGR.COMPANY_CODE AND A.DKH_NO = DGR.DKH_NO AND A.GROUP_CODE = DGR.GROUP_CODE&#xD;&#x9;LEFT JOIN MST_EMPLOYEE I (nolock)&#xD;&#x9;ON H.COLL_CODE = I.NIK&#xD;&#x9;--LEFT JOIN MST_GLOBAL_PARAM (nolock) K&#xD;&#x9;--ON A.SP_TYPE = K.PARAM_CODE AND K.CONDITION = &#39;SP_TYPE&#39; AND K.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_GROUP_HDR L (nolock)&#xD;&#x9;ON A.GROUP_CODE = L.GROUP_CODE&#xD;&#x9;LEFT JOIN TRX_APPL M (nolock)&#xD;&#x9;ON B.APPL_NO = M.APPL_NO&#xD;&#x9;LEFT JOIN TRX_AGGR_ASSET N (nolock)&#xD;&#x9;ON B.AGGR_NO = N.AGGR_NO&#xD;&#x9;LEFT JOIN MST_BRANCH O (nolock)&#xD;&#x9;ON A.BRANCH_CODE = O.BRANCH_CODE&#xD;&#x9;LEFT JOIN MST_GLOBAL_PARAM P (nolock)&#xD;&#x9;ON A.COMPANY_CODE = P.COMPANY_CODE AND A.BILL_STS = P.PARAM_CODE AND P.CONDITION = &#39;BILL_STS&#39; AND P.FLAG_ACTIVE = &#39;1&#39;&#xD;&#x9;LEFT JOIN MST_COLL_ATTENDANCE ATT (nolock)&#xD;&#x9;ON A.COMPANY_CODE = ATT.COMPANY_CODE AND A.ATT_CODE = ATT.ATT_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER CS (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = CS.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE IV (nolock)&#xD;&#x9;ON CS.ID_VILLAGE_CODE = IV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT ISUB (nolock)&#xD;&#x9;ON ISUB.SUBDISTRICT_CODE = IV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY IC (nolock)&#xD;&#x9;ON IV.CITY_CODE = IC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE IP (nolock)&#xD;&#x9;ON IV.PROVINCE_CODE = IP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE DV (nolock)&#xD;&#x9;ON CS.DOMICILE_VILLAGE_CODE = DV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT DSUB (nolock)&#xD;&#x9;ON DSUB.SUBDISTRICT_CODE = DV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY DC (nolock)&#xD;&#x9;ON DV.CITY_CODE = DC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE DP (nolock)&#xD;&#x9;ON DV.PROVINCE_CODE = DP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE F (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = F.CUSTOMER_CODE AND F.EMPLOYEE_TYPE = &#39;O&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE KV (nolock)&#xD;&#x9;ON F.OFFICE_VILLAGE_CODE = KV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT KSUB (nolock)&#xD;&#x9;ON KSUB.SUBDISTRICT_CODE = KV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY KC (nolock)&#xD;&#x9;ON KV.CITY_CODE = KC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE KP (nolock)&#xD;&#x9;ON KV.PROVINCE_CODE = KP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_EMPLOYEE G (nolock)&#xD;&#x9;ON B.CUSTOMER_CODE = G.CUSTOMER_CODE AND G.EMPLOYEE_TYPE = &#39;B&#39;&#xD;&#x9;LEFT JOIN MST_VILLAGE BV (nolock)&#xD;&#x9;ON G.OFFICE_VILLAGE_CODE = BV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT BSUB (nolock)&#xD;&#x9;ON BSUB.SUBDISTRICT_CODE = BV.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY BC (nolock)&#xD;&#x9;ON BV.CITY_CODE = BC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE BP (nolock)&#xD;&#x9;ON BV.PROVINCE_CODE = BP.PROVINCE_CODE&#xD;&#x9;LEFT JOIN MST_CUSTOMER_CO CO (nolock)&#xD;&#x9;ON A.COMPANY_CODE = CO.COMPANY_CODE AND A.CUSTOMER_CODE = CO.CUSTOMER_CODE&#xD;&#x9;LEFT JOIN MST_VILLAGE COV (nolock)&#xD;&#x9;ON CO.OFFICE_VILLAGE_CODE = COV.VILLAGE_CODE&#xD;&#x9;LEFT JOIN MST_SUBDISTRICT COSUB (nolock)&#xD;&#x9;ON COV.SUBDISTRICT_CODE = COSUB.SUBDISTRICT_CODE&#xD;&#x9;LEFT JOIN MST_CITY COVC (nolock)&#xD;&#x9;ON COV.CITY_CODE = COVC.CITY_CODE&#xD;&#x9;LEFT JOIN MST_PROVINCE COP (nolock)&#xD;&#x9;ON COV.PROVINCE_CODE = COP.PROVINCE_CODE&#xD;&#x9;WHERE A.PAID_STS != &#39;999&#39;&#xD;) a WHERE :search :filter</sql>
         <result element="count" rowName="" useColumnNumbers="false">
            <element column="total" name="total" xsdType="string"></element>
